<script>
  function initSlider13() {
    document.querySelector('.hero-slideshow-{{ section.id }}').classList.remove('preview');

    const slider = new Swiper(`.hero-slideshow-{{ section.id }}`, {
      speed: 300,
      loop: true,
      autoplay: {
        delay: 3000,
      },
      breakpoints: {
        320: {
          spaceBetween: parseInt(document.querySelector(`.hero-slideshow-{{ section.id }}`).dataset.gapMobile || 0),
          slidesPerView: parseFloat(document.querySelector(`.hero-slideshow-{{ section.id }}`).dataset.viewMobile || 1),
        },
        768: {
          spaceBetween: parseInt(document.querySelector(`.hero-slideshow-{{ section.id }}`).dataset.gap || 0),
          slidesPerView: parseFloat(document.querySelector(`.hero-slideshow-{{ section.id }}`).dataset.view || 3),
        },
        1024: {
          spaceBetween: parseInt(document.querySelector(`.hero-slideshow-{{ section.id }}`).dataset.gap || 0),
          slidesPerView: parseFloat(document.querySelector(`.hero-slideshow-{{ section.id }}`).dataset.view || 3),
        }
      }
    });
  }

  document.addEventListener('DOMContentLoaded', initSlider13);

  if (Shopify.designMode) {
    document.addEventListener('shopify:section:unload', initSlider13);
    document.addEventListener('shopify:section:load', initSlider13);
  }
</script>

