<link
  rel="stylesheet"
  href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css"
/>

{%- liquid 
  assign padding_horizontal = section.settings.padding_horizontal
  assign padding_horizontal_mobile = section.settings.padding_horizontal_mobile
  assign padding_top = section.settings.padding_top
  assign padding_bottom = section.settings.padding_bottom
  assign border_color = section.settings.border_color
  assign border_thickness = section.settings.border_thickness
  assign margin_top = section.settings.margin_top
  assign margin_bottom = section.settings.margin_bottom
  assign margin_horizontal_mobile = section.settings.margin_horizontal_mobile
  assign margin_horizontal = section.settings.margin_horizontal
  assign background_color = section.settings.background_color
  assign background_gradient = section.settings.background_gradient
  assign full_width = section.settings.full_width
  assign content_width = section.settings.content_width
  assign lazy = section.settings.lazy
  assign section_radius = section.settings.section_radius  
  assign background_style = section.settings.background_style

  assign gradient_start = section.settings.gradient_start
  assign gradient_end = section.settings.gradient_end

  assign layout = section.settings.layout
  assign layout_mobile = section.settings.layout_mobile
  assign layout_height = section.settings.layout_height
  assign layout_fixed_height = section.settings.layout_fixed_height
  assign layout_gap = section.settings.layout_gap
  assign layout_gap_mobile = section.settings.layout_gap_mobile

  assign images_gap = section.settings.images_gap
  assign images_gap_mobile = section.settings.images_gap_mobile
  assign images_height = section.settings.images_height
  assign images_height_mobile = section.settings.images_height_mobile
  assign images_speed = section.settings.images_speed
  assign images_animation = section.settings.images_animation

  assign image_border_color = section.settings.image_border_color
  assign image_border_thickness = section.settings.image_border_thickness
  assign image_radius = section.settings.image_radius
  assign image_ratio = section.settings.image_ratio
  assign image_ratio_mobile = section.settings.image_ratio_mobile
  assign image_shadow = section.settings.image_shadow

  assign content_align = section.settings.content_align
  assign content_align_mobile = section.settings.content_align_mobile
  assign content_vertical_align = section.settings.content_vertical_align

  assign content_horizontal_align = ""
  if content_align == "center"
    assign content_horizontal_align = "center"
  elsif content_align == "right"
    assign content_horizontal_align = "end"
  else
    assign content_horizontal_align = "start"
  endif

  assign content_horizontal_align_mobile = ""
  if content_align_mobile == "center"
    assign content_horizontal_align_mobile = "center"
  elsif content_align_mobile == "right"
    assign content_horizontal_align_mobile = "end"
  else
    assign content_horizontal_align_mobile = "start"
  endif

  assign features_mt = section.settings.features_mt
  assign features_mt_mobile = section.settings.features_mt_mobile
  assign features_align = section.settings.features_align
  assign features_align_mobile = section.settings.features_align_mobile

  assign features_horizontal_align = ""
  if features_align == "center"
    assign features_horizontal_align = "center"
  elsif features_align == "right"
    assign features_horizontal_align = "end"
  else
    assign features_horizontal_align = "start"
  endif

  assign features_horizontal_align_mobile = ""
  if features_align_mobile == "center"
    assign features_horizontal_align_mobile = "center"
  elsif features_align_mobile == "right"
    assign features_horizontal_align_mobile = "end"
  else
    assign features_horizontal_align_mobile = "start"
  endif

  assign feature_gap = section.settings.feature_gap
  assign feature_gap_mobile = section.settings.feature_gap_mobile
  assign feature_mt = section.settings.feature_mt
  assign feature_mt_mobile = section.settings.feature_mt_mobile

  assign feature_icon_size = section.settings.feature_icon_size
  assign feature_icon_color = section.settings.feature_icon_color
  assign feature_icon_size_mobile = section.settings.feature_icon_size_mobile

  assign feature_text_custom = section.settings.feature_text_custom
  assign feature_text_font = section.settings.feature_text_font
  assign feature_text_size = section.settings.feature_text_size
  assign feature_text_size_mobile = section.settings.feature_text_size_mobile
  assign feature_text_color = section.settings.feature_text_color
  assign feature_text_height = section.settings.feature_text_height

  assign loading_attribute = 'eager'
  if lazy
    assign loading_attribute = 'lazy'
  endif
-%}

{%- style -%}
  {{ feature_text_font | font_face: font_display: 'swap' }}

  .section-{{ section.id }} {
    border-top: solid {{ border_color }} {{ border_thickness }}px;
    border-bottom: solid {{ border_color }} {{ border_thickness }}px;
    margin-top: {{ margin_top | times: 0.75 | round: 0 }}px;
    margin-bottom: {{ margin_bottom | times: 0.75 | round: 0 }}px;
    margin-left: {{ margin_horizontal_mobile }}rem;
    margin-right: {{ margin_horizontal_mobile }}rem;
    border-radius: {{ section_radius | times: 0.6 | round: 0 }}px;
    overflow: hidden;
  } 
  
  .section-{{ section.id }}-settings {
    margin: 0 auto;
    padding-top: {{ padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ padding_bottom | times: 0.75 | round: 0 }}px;
    padding-left: {{ padding_horizontal_mobile }}rem;
    padding-right: {{ padding_horizontal_mobile }}rem;
    height: 100%;
  }

  .hero-body-{{ section.id }} {
    position: relative;
    display: grid;
    grid-template-columns: 1fr;
    gap: {{ layout_gap_mobile }}px;
    height: 100%;
    align-items: center;
  }

  .hero-heading-slider-{{ section.id }} {
    min-height: 0 !important;
    height: 100% !important;
    width: 100% !important;
  }

  .hero-content-{{ section.id }} {
    grid-area: content;
    display: flex;
    flex-direction: column;
    position: relative;
    justify-content: {{ content_vertical_align }};
    align-items: {{ content_horizontal_align_mobile }};   
    width: 100%;
    height: 100%;
  }

  .hero-features-{{ section.id }} {
    width: 100%;
    margin-top: {{ features_mt_mobile }}px;
  }
  
   .hero-feature-{{ section.id }} {
    display: flex;
    align-items: center;
    justify-content: {{ features_horizontal_align_mobile }};
    gap: {{ feature_gap_mobile }}px;
    text-decoration: none;
    position: relative;
    z-index: 1;
    width: 100%;
  }

  .hero-feature-{{ section.id }}:not(:first-child) {
    margin-top: {{ feature_mt_mobile }}px;
  }

  .hero-feature-text-{{ section.id }} {
    margin: 0;
    text-align: {{ features_align_mobile }};
    font-size: {{ feature_text_size_mobile }}px;
    color: {{ feature_text_color }};
    line-height: {{ feature_text_height }}%;
    text-transform: unset;
    font-weight: 700;
  }
  
  .hero-feature-icon-{{ section.id }} {
    flex-shrink: 0;
    height: {{ feature_icon_size_mobile }}px;
    width: {{ feature_icon_size_mobile }}px;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .hero-feature-icon-{{ section.id }} img,
  .hero-feature-icon-{{ section.id }} svg {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .hero-feature-icon-{{ section.id }} svg path {
    fill: {{ feature_icon_color }};
  }

  .hero-feature-icon-{{ section.id }} svg circle {
    fill: {{ feature_icon_color }};
  }

  .hero-feature-icon-{{ section.id }} .refresh-{{ section.id }} path,
  .hero-feature-icon-{{ section.id }} .refresh-{{ section.id }} polyline{
    fill: none;
    stroke: {{ feature_icon_color }};
  }
  
  .hero-images-wrapper-{{ section.id }} {
    display: grid;
    grid-area: image;
    grid-template-columns: repeat(3, 1fr);
    gap: {{ images_gap_mobile }}px;
    position: relative;
    width: 100%;
    overflow: hidden;
    mask-image: linear-gradient(transparent, black 20%, black 80%, transparent 100%);
  }

  .hero-images-left-{{ section.id }},
  .hero-images-right-{{ section.id }} {
    display: flex;
    align-items: flex-start;
    justify-content: center;
    flex-direction: column;
    gap: {{ images_gap_mobile }}px;
  }

  .hero-images-inner-{{ section.id }} {
    display: flex;
    flex-direction: column;
    gap: {{ images_gap_mobile }}px;;
    width: 100%;
  }

  .hero-image-{{ section.id }} {
    border-radius: {{ image_radius }}px;
    border: {{ image_border_thickness }}px solid {{ image_border_color }};
    overflow: hidden;
  }

  .hero-image-{{ section.id }} img,
  .hero-image-{{ section.id }} svg {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .hero-image-{{ section.id }} svg {
    background-color: #AFAFAF;
  }
  
  .hero-sticker-{{ section.id }} {
    position: absolute;   
    z-index: 2;  
  }

  .hero-sticker-{{ section.id }} img {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .hero-sticker-icon-{{ section.id }} {
    width: 100%;
    position: relative;
  }

  @media(min-width: 1024px) {

    .section-{{ section.id }} {
      margin-top: {{ margin_top }}px;
      margin-bottom: {{ margin_bottom }}px;
      margin-left: {{ margin_horizontal }}rem;
      margin-right: {{ margin_horizontal }}rem;
      border-radius: {{ section_radius }}px;
    }
    
    .section-{{ section.id }}-settings {
      padding: 0 5rem;
      padding-top: {{ padding_top }}px;
      padding-bottom: {{ padding_bottom }}px;
      padding-left: {{ padding_horizontal }}rem;
      padding-right: {{ padding_horizontal }}rem;
    }
    
    .hero-content-{{ section.id }} {
      align-items: {{ content_horizontal_align }};
    }

    .hero-body-{{ section.id }} {      
      gap: {{ layout_gap }}px;
    }

    .hero-features-{{ section.id }} {
      margin-top: {{ features_mt }}px;
    }

    .hero-feature-{{ section.id }}:not(:first-child) {
      margin-top: {{ feature_mt }}px;
    }

    .hero-feature-{{ section.id }} {      
      justify-content: {{ features_horizontal_align }};
      gap: {{ feature_gap }}px;     
    }

    .hero-images-left-{{ section.id }},
    .hero-images-right-{{ section.id }} {     
      gap: {{ images_gap }}px;;
    }

    .hero-images-inner-{{ section.id }} {  
      gap: {{ images_gap }}px;;
    }
    
    .hero-feature-text-{{ section.id }} {
      text-align: {{ features_align }};
      font-size: {{ feature_text_size }}px;
    }
    
    .hero-feature-icon-{{ section.id }} {
      height: {{ feature_icon_size }}px;
      width: {{ feature_icon_size }}px;
    }

    .hero-images-{{ section.id }} {
      margin-top: -{{ padding_top }}px;
      margin-bottom: -{{ padding_bottom }}px;
    }

    .hero-images-wrapper-{{ section.id }} {
      gap: {{ images_gap }}px;
    }
  }
  
{%- endstyle -%}

{% unless full_width %}
  <style>
    .section-{{ section.id }}-settings {
      max-width: {{ content_width }}px;
    }
  </style>
{% endunless %}

{% if margin_horizontal_mobile > 0 %}
  <style>
    .section-{{ section.id }} {
      border-left: solid {{ border_color }} {{ border_thickness }}px;
      border-right: solid {{ border_color }} {{ border_thickness }}px;
    }
    
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: 0px;
        border-right: 0px;
      }
    }
  </style>
{% endif %}

{% if margin_horizontal > 0 %}
  <style>
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: solid {{ border_color }} {{ border_thickness }}px;
        border-right: solid {{ border_color }} {{ border_thickness }}px;
      }
    }
  </style>
{% endif %}

{% if feature_text_custom %}
  <style>
    .hero-feature-text-{{ section.id }} {
      font-family: {{ feature_text_font.family }}, {{ feature_text_font.fallback_families }};
      font-weight: {{ feature_text_font.weight }};
      font-style: {{ feature_text_font.style }};
    }
  </style>
{% endif %}

{% if layout_height == "full_screen" %}
  <style>

    .hero-{{ section.id }} {
      height: 100%;
    }

    @media(min-width: 1024px) {      

      .hero-{{ section.id }} {
        height: 95vh;
      }
    }
  </style>
{% elsif layout_height == 'fixed_height' %}  
  <style>      
    .hero-{{ section.id }} {
      height: 100%;
    }

    @media(min-width: 1024px) {
      .hero-{{ section.id }} {
        height: {{ layout_fixed_height }}px;
      }
    }
  </style>
{% endif %}

{% if layout_mobile == 'content_image'%}
  <style>
    .hero-body-{{ section.id }} {
      grid-template-areas: 
        "content"
        "image";     
    }
  </style>
{% elsif layout_mobile == 'image_content' %}
  <style>
    .hero-body-{{ section.id }} {
      grid-template-areas: 
        "image"
        "content";     
    }
  </style>
{% endif %}

{% if layout == 'content_image'%}
  <style>
    @media(min-width: 1024px) {
      .hero-body-{{ section.id }} {
        grid-template-columns: 1fr 1fr;
        grid-template-areas: "content image";    
      }
    }    
  </style>
{% elsif layout == 'image_content' %}
  <style>
    @media(min-width: 1024px) {
      .hero-body-{{ section.id }} {
        grid-template-columns: 1fr 1fr;
        grid-template-areas: "image content";   
      }
    }   
  </style>
{% endif %}


{% if image_ratio_mobile == "portrait" %}
  <style>
    .hero-image-{{ section.id }} {
     aspect-ratio: 9.6/14; 
    }
  </style>
{% elsif image_ratio_mobile == "landscape" %}
  <style>
    .hero-image-{{ section.id }}{
     aspect-ratio: 12/9; 
    }
  </style>
{% elsif image_ratio_mobile == "square" %}
  <style>
    .hero-image-{{ section.id }} {
     aspect-ratio: 12/12; 
    }
  </style>
{% else %}
  <style>
    .hero-image-{{ section.id }} {
     aspect-ratio: auto; 
    }
  </style>
{% endif %}

{% if image_ratio == "portrait" %}
  <style>
    @media(min-width: 1024px) {
     .hero-image-{{ section.id }} {
       aspect-ratio: 9.6/14; 
      } 
    }
  </style>
{% elsif image_ratio == "landscape" %}
  <style>
    @media(min-width: 1024px) {
      .hero-image-{{ section.id }}{
       aspect-ratio: 12/9; 
      }
    }
  </style>
{% elsif image_ratio == "square" %}
  <style>
    @media(min-width: 1024px) {
      .hero-image-{{ section.id }} {
       aspect-ratio: 12/12; 
      }
    }
  </style>
{% else %}
  <style>
    @media(min-width: 1024px) {
      .hero-image-{{ section.id }} {
       aspect-ratio: auto; 
      }
    }
  </style>
{% endif %}

{% if image_shadow %}
  <style>
    .hero-images-inner-{{ section.id }} {
      padding: 0px 8px;
    }
    .hero-image-{{ section.id }} {
      box-shadow: rgba(0, 0, 0, 0.08) 0px 4px 8px;
    }
  </style>
{% endif %}

{% if images_animation == "back_forth" %}
  <style>

    .hero-images-left-{{ section.id }} {
      animation: leftImagesAnimation{{ section.id }} {{ images_speed }}s linear infinite alternate;
    }
  
    .hero-images-right-{{ section.id }} {
      animation: rightImagesAnimation{{ section.id }} {{ images_speed }}s linear infinite alternate;
    }
    
    @keyframes leftImagesAnimation{{ section.id }} {
      0% {
        transform: translateY(0%);
      }
      50% {
        transform: translateY(-50%);
      }
      100% {
        transform: translateY(0%);
      }
    }
  
    @keyframes rightImagesAnimation{{ section.id }} {
      0% {
        transform: translateY(-50%);
      }
      50% {
        transform: translateY(0%);
      }
      100% {
        transform: translateY(-50%);
      }
    }
  </style>
{% else %}
  <style>

    .hero-images-left-{{ section.id }} {
      animation: leftImagesAnimation{{ section.id }} {{ images_speed }}s linear infinite;
    }
  
    .hero-images-right-{{ section.id }} {
      animation: rightImagesAnimation{{ section.id }} {{ images_speed }}s linear infinite;
    }
    
    @keyframes leftImagesAnimation{{ section.id }} {
      0% {
        transform: translateY(0%);
      }
      100% {
        transform: translateY(-50%);
      }
    }

    @keyframes rightImagesAnimation{{ section.id }} {
      0% {
        transform: translateY(-50%);
      }
      100% {
        transform: translateY(0%);
      }
    }
  </style>
{% endif %}

{% if layout_height == 'auto' %}
  <style>
    .hero-images-wrapper-{{ section.id }} {
      height: {{ images_height_mobile }}px;
    }

    @media(min-width: 1024px) {      
      .hero-images-wrapper-{{ section.id }} {
        height: {{ images_height }}px;
      }
    }
  </style>
{% elsif layout_height == 'fixed_height' %}
  <style>
    .hero-images-wrapper-{{ section.id }} {
      height: {{ images_height_mobile }}px;
    }

    @media(min-width: 1024px) {     
      .hero-images-wrapper-{{ section.id }} {
        height: {{ layout_fixed_height | minus: 50 }}px;
      } 
    }
  </style>
{% else %}
  <style>
    .hero-images-wrapper-{{ section.id }} {
      height: {{ images_height_mobile }}px;
    }
    
    @media(min-width: 1024px) {     
      .hero-images-wrapper-{{ section.id }} {
        height: 95vh;
      } 
    }
  </style>
{% endif %}

{% for block in section.blocks %}
  {% case block.type %}
      {% when 'sticker' %}
      <style>        
        .hero-sticker-{{ block.id }} {              
          transform: translate(-50%, -50%);
          width: 100%;
          max-width: {{ block.settings.sticker_width_mobile }}px;
          display: {% if block.settings.show_mobile %} block {% else %} none {% endif %};
        }

        .hero-sticker-wrap-{{ block.id }} {
          width: 100%;
          height: 100%;
          transform: rotate({{ block.settings.rotate }}deg);
        }

        .hero-sticker-icon-{{ block.id }} svg path {
          fill: {{ block.settings.sticker_bg_color }};
        }

        .hero-sticker-text-{{ block.id }} {
          padding: 4px 4px 12px 4px;
          color: {{ block.settings.sticker_text_color }};
          margin: 0px;
          position: absolute;
          top: 50%;
          left: 50%;
          transform: translate(-50%, -50%);
          max-width: 90%;
          width: 100%;
          font-size: {{ block.settings.sticker_text_size_mobile }}px;
          line-height: {{ block.settings.sticker_text_height }}%;
          text-align: {{ block.settings.sticker_text_align_mobile }};
          text-transform: unset;
          text-decoration: none;
          font-weight: 700;
        }

        @media(min-width: 1024px) {
          .hero-sticker-{{ block.id }} {            
            max-width: {{ block.settings.sticker_width }}px;
            display: block;
          }

          .hero-sticker-text-{{ block.id }} {
            text-align: {{ block.settings.sticker_text_align }};
            font-size: {{ block.settings.sticker_text_size }}px;
          }
        }
      </style>

      {% if block.settings.sticker_text_custom %}
        <style>
          {{ block.settings.sticker_text_font | font_face: font_display: 'swap' }}
          
          .hero-sticker-text-{{ block.id }} {
            font-family: {{ block.settings.sticker_text_font.family }}, {{ block.settings.sticker_text_font.fallback_families }};
            font-weight: {{ block.settings.sticker_text_font.weight }};
            font-style: {{ block.settings.sticker_text_font.style }};
          }
        </style>
      {% endif %}

      {% if content_align_mobile == 'right' %}
        <style>
          .hero-sticker-{{ block.id }} {            
            left: 25px;
            top: 60px;                  
          }
        </style>
      {% elsif content_align_mobile == 'center' %}
         <style>
          .hero-sticker-{{ block.id }} {
            left: 275px;
            top: 60px;       
          }
        </style>
      {% else %}
        <style>
          .hero-sticker-{{ block.id }} {
            left: 270px;
            top: 60px;       
          }
        </style>
      {% endif %}

      {% if content_align == 'right' %}
        <style>
          @media(min-width: 1024px) {
            .hero-sticker-{{ block.id }} {            
              left: 25px;
              top: 60px;                 
            }
          }
        </style>
      {% elsif content_align == 'center' %}
        <style>
          @media(min-width: 1024px) {
            .hero-sticker-{{ block.id }} {
              left: 400px;
              top: 60px;       
            }
          }
        </style>
      {% else %}
        <style>
          @media(min-width: 1024px) {
            .hero-sticker-{{ block.id }} {            
              left: 400px;
              top: 60px;            
            }
          }
        </style>
      {% endif %}

      {% if block.settings.custom_sticker == "custom" %}
        <style>
          .hero-sticker-wrap-{{ block.id }} svg {
            background-color: #AFAFAF;
          }
        </style>
      {% endif %}
   {% when 'progress_bar' %}  
      <style>
        {{  block.settings.heading_font | font_face: font_display: 'swap' }}

        .hero-heading-{{ block.id }} {
          margin-top: {{ block.settings.heading_mt_mobile }}px;
          text-align: {{ content_align_mobile }};
        }
      
        .hero-heading-{{ block.id }} * {
          margin: 0;
          font-size: {{ block.settings.heading_size_mobile }}px;
          color: {{ block.settings.heading_color }};
          line-height: {{ block.settings.heading_height }}%;
          text-transform: unset;
          word-break: break-word;
        }

        .hero-heading-{{ block.id }} * strong {
          text-decoration: underline {{ block.settings.heading_border_color }};
        }

        .hero-bar-{{ block.id }} {
          display: block !important;
          width: 270px;
          margin-top: 16px;
          position: relative;
          border-radius: {{ block.settings.progress_radius }}px;
          overflow: hidden;
          height: {{ block.settings.progress_height }}px;
          transition: all 0.25s ease 0s;          
          background-color: {{ block.settings.progress_color }};
        }

        .hero-bar-{{ block.id }}:after {
          content: "";
          position: absolute;
          top: 0;
          left: 0;
          height: 100%;
          background-color: {{ block.settings.progress_active_color }};
          width: 0;         
          animation: progress-animation-{{ block.id }} 1.5s ease forwards;
          transition: all 0.25s ease 0s;
        }

        @keyframes progress-animation-{{ block.id }} {
          0% {
            width: 0;
          }
          100% {
            width: {{ block.settings.progress_percent }}%;
          }
        }

        @media(min-width: 1024px) {
          
          .hero-heading-{{ block.id }} {
            margin-top: {{ block.settings.heading_mt }}px;
            text-align: {{ content_align }};
          }
        
          .hero-heading-{{ block.id }} * {
            font-size: {{ block.settings.heading_size }}px;
          }

          .hero-bar-{{ block.id }} {          
            width: 400px;           
          }
        }          
      </style>
        
      {% if block.settings.heading_custom %}
        <style>
          .hero-heading-{{ block.id }} * {
            font-family: {{ block.settings.heading_font.family }}, {{ block.settings.heading_font.fallback_families }};
