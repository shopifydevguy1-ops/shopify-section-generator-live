<style>
  .section-{{ section.id }}-padding {
    max-width: var(--max-width);
    margin: var(--mt, 0) auto var(--mb, 0);
    padding-top: var(--pt);
    padding-bottom: var(--pb);
    padding-left: var(--px, 1.5rem);
    padding-right: var(--px, 1.5rem);
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-left: var(--px-desktop, 5rem);
      padding-right: var(--px-desktop, 5rem);
      padding-top: var(--pt-desktop);
      padding-bottom: var(--pb-desktop);
    }
  }
</style>

<div style="background-color:{{ section.settings.background-color }}; position: relative;">
<div
  class="section-{{ section.id }}-padding"
  style="
    --max-width: {{ section.settings.section-width }}rem;
    --pt: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    --pt-desktop: {{ section.settings.padding_top }}px;
    --pb: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
    --pb-desktop: {{ section.settings.padding_bottom }}px;
    --px: {{ section.settings.padding_x }}px;
    --px-desktop: {{ section.settings.padding_x_desktop }}px;
    color: {{ section.settings.text_color }};
  "
>
<div class="placeholder-{{ section.id }}"
    style=" padding: 0 30px; width: 100%; display: flex; justify-content: center; align-items: center; 
    font-size: 1.5rem; color: black; text-align: center;"
  >
    Click SAVE to activate the section
      <script>
    (
      function() {
        function hidePlaceholder() {
          document.querySelector('.placeholder-{{ section.id }}').style.display = 'none';
        }
        hidePlaceholder();
        if(window.Shopify && Shopify.designMode) {
          document.addEventListener('shopify:section:load', hidePlaceholder);
        }
      }
    )()
  </script>
  </div>
    <ss-testimonials-carousel
      heading="{{ section.settings.heading }}"
      {% assign autoplay_delay_in_ms = section.settings.autoplay_delay | times: 1000 | round: 0 %}
      autoplayDelay="{{ autoplay_delay_in_ms }}"
      autoplayDisableOnInteraction="{{ section.settings.autoplay_disable_on_interaction }}"
      data='{
        "data":[
          {% for block in section.blocks %}
            {% assign image_src = block.settings.image | img_url: "300x" %}
            {
              "icon":"{{ block.settings.icon }}",
              "title":"{{ block.settings.title | url_encode | replace: "+" , "%20" }}",
              "author":"{{ block.settings.author | url_encode | replace: "+" , "%20"  }}",
              "info":"{{ block.settings.info | url_encode | replace: "+" , "%20"  }}"
              {% if block.settings.image %}
              , "image": "{{ image_src }}"
              {% endif %}
            }
            {% if forloop.last == false %},{% endif %}
          {% endfor %}
        ]
      }'
      style="
        --testimonials-heading-font-size: {{ section.settings.heading_font_size }}px;
        --testimonials-p-font-size: {{ section.settings.p_font_size }}px;
        --testimonials-author-font-size: {{ section.settings.author_font_size }}px;
        --testimonials-info-font-size: {{ section.settings.info_font_size }}px;
        --testimonials-image-size: {{ section.settings.image_size }}px;
        --testimonials-image-border-radius: {{ section.settings.image_border_radius }}%;
        --title-align: {{ section.settings.title_align }};
        text-align: {{ section.settings.text_align }};
        {% if section.settings.text_align == 'center' %}
          --image-margin: 0 auto;
        {% elsif section.settings.text_align == 'left' %}
          --image-margin: 0 auto 0 0;
        {% elsif section.settings.text_align == 'right' %}
          --image-margin: 0 0 0 auto;
        {% endif %}
        --swiper-pagination-color: {{ section.settings.text_color }};
      "
    >
    </ss-testimonials-carousel>
</div>
</div>

