                        aria-describedby="Newsletter-succesg--{{ block.id }}"
                      {% endif %}
                      placeholder="{{ block.settings.placeholder_name }}"
                      required
                    >
                    <input
                      type="email"
                      name="contact[email]"
                      class="footer-newsletter-input-{{ block.id }}"
                      value="{{ form.email }}"
