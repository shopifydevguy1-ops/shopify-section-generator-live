{%- liquid 
  assign padding_horizontal = section.settings.padding_horizontal
  assign padding_horizontal_mobile = section.settings.padding_horizontal_mobile
  assign padding_top = section.settings.padding_top
  assign padding_bottom = section.settings.padding_bottom
  assign border_color = section.settings.border_color
  assign border_thickness = section.settings.border_thickness
  assign margin_top = section.settings.margin_top
  assign margin_bottom = section.settings.margin_bottom
  assign margin_horizontal_mobile = section.settings.margin_horizontal_mobile
  assign margin_horizontal = section.settings.margin_horizontal
  assign background_color = section.settings.background_color
  assign background_gradient = section.settings.background_gradient
  assign full_width = section.settings.full_width
  assign content_width = section.settings.content_width
  assign lazy = section.settings.lazy
  assign section_radius = section.settings.section_radius
  assign background_style = section.settings.background_style

  assign content_align = section.settings.content_align
  assign content_align_mobile = section.settings.content_align_mobile

  assign content_horizontal_align = ""
  if content_align == "center"
    assign content_horizontal_align = "center"
  elsif content_align == "right"
    assign content_horizontal_align = "end"
  else
    assign content_horizontal_align = "start"
  endif

  assign content_horizontal_align_mobile = ""
  if content_align_mobile == "center"
    assign content_horizontal_align_mobile = "center"
  elsif content_align_mobile == "right"
    assign content_horizontal_align_mobile = "end"
  else
    assign content_horizontal_align_mobile = "start"
  endif

  assign form_gap = section.settings.form_gap
  assign form_gap_mobile = section.settings.form_gap_mobile
  assign form_mt = section.settings.form_mt
  assign form_mt_mobile = section.settings.form_mt_mobile
  assign form_bg_color = section.settings.form_bg_color

  assign success_text = section.settings.success_text
  assign success_text_size = section.settings.success_text_size
  assign success_text_size_mobile = section.settings.success_text_size_mobile
  assign success_text_color = section.settings.success_text_color
  assign success_text_custom = section.settings.success_text_custom
  assign success_text_font = section.settings.success_text_font
  assign success_text_height = section.settings.success_text_height
  assign success_text_align = section.settings.success_text_align
  assign success_text_align_mobile = section.settings.success_text_align_mobile

  assign title_size = section.settings.title_size
  assign title_size_mobile = section.settings.title_size_mobile
  assign title_color = section.settings.title_color
  assign title_custom = section.settings.title_custom
  assign title_font = section.settings.title_font
  assign title_height = section.settings.title_height

  assign label_size = section.settings.label_size
  assign label_size_mobile = section.settings.label_size_mobile
  assign label_color = section.settings.label_color
  assign label_custom = section.settings.label_custom
  assign label_font = section.settings.label_font
  assign label_height = section.settings.label_height
  assign label_mt = section.settings.label_mt
  assign label_mt_mobile = section.settings.label_mt_mobile

  assign required_text  = section.settings.required_text
  assign required_size = section.settings.required_size
  assign required_size_mobile = section.settings.required_size_mobile
  assign required_color = section.settings.required_color
  assign required_custom = section.settings.required_custom
  assign required_font = section.settings.required_font
  assign required_height = section.settings.required_height

  assign input_size = section.settings.input_size
  assign input_size_mobile = section.settings.input_size_mobile
  assign input_color = section.settings.input_color
  assign input_custom = section.settings.input_custom
  assign input_font = section.settings.input_font
  assign input_height = section.settings.input_height
  assign input_mt = section.settings.input_mt
  assign input_mt_mobile = section.settings.input_mt_mobile
  assign input_padding_vertical = section.settings.input_padding_vertical
  assign input_padding_vertical_mobile = section.settings.input_padding_vertical_mobile
  assign input_padding_horizontal = section.settings.input_padding_horizontal
  assign input_padding_horizontal_mobile = section.settings.input_padding_horizontal_mobile
  assign input_radius = section.settings.input_radius
  assign input_border_thickness = section.settings.input_border_thickness
  assign input_bg_color = section.settings.input_bg_color
  assign input_border_color = section.settings.input_border_color
  assign input_border_hover_color = section.settings.input_border_hover_color
  assign input_active_color = section.settings.input_active_color
  assign select_icon_color = section.settings.select_icon_color

  assign placeholder_color = section.settings.placeholder_color

  assign checkbox_bg_color = section.settings.checkbox_bg_color
  assign checkbox_bg_active_color = section.settings.checkbox_bg_active_color
  assign checkbox_border_color = section.settings.checkbox_border_color
  assign checkbox_border_active_color = section.settings.checkbox_border_active_color
  assign checkbox_checkmar_color = section.settings.checkbox_checkmar_color

  assign checkbox_size = section.settings.checkbox_size
  assign checkbox_size_mobile = section.settings.checkbox_size_mobile
  assign checkbox_color = section.settings.checkbox_color
  assign checkbox_custom = section.settings.checkbox_custom
  assign checkbox_font = section.settings.checkbox_font
  assign checkbox_height = section.settings.checkbox_height
  assign checkbox_radius = section.settings.checkbox_radius

  assign error_text_size = section.settings.error_text_size
  assign error_text_size_mobile = section.settings.error_text_size_mobile
  assign error_text_color = section.settings.error_text_color
  assign error_text_custom = section.settings.error_text_custom
  assign error_text_font = section.settings.error_text_font
  assign error_text_height = section.settings.error_text_height
  assign error_text_align = section.settings.error_text_align
  assign error_text_align_mobile = section.settings.error_text_align_mobile

  assign submit = section.settings.submit
  assign submit_size = section.settings.submit_size
  assign submit_size_mobile = section.settings.submit_size_mobile
  assign submit_color = section.settings.submit_color
  assign submit_hover_color = section.settings.submit_hover_color
  assign submit_custom = section.settings.submit_custom
  assign submit_font = section.settings.submit_font
  assign submit_height = section.settings.submit_height
  assign submit_mt = section.settings.submit_mt
  assign submit_mt_mobile = section.settings.submit_mt_mobile
  assign submit_padding_vertical = section.settings.submit_padding_vertical
  assign submit_padding_vertical_mobile = section.settings.submit_padding_vertical_mobile
  assign submit_padding_horizontal = section.settings.submit_padding_horizontal
  assign submit_padding_horizontal_mobile = section.settings.submit_padding_horizontal_mobile
  assign submit_radius = section.settings.submit_radius
  assign submit_border_thickness = section.settings.submit_border_thickness
  assign submit_border_color = section.settings.submit_border_color
  assign submit_border_hover_color = section.settings.submit_border_hover_color
  assign submit_bg_color = section.settings.submit_bg_color
  assign submit_bg_hover_color = section.settings.submit_bg_hover_color
  assign submit_bg_gradient = section.settings.submit_bg_gradient
  assign submit_bg_hover_gradient = section.settings.submit_bg_hover_gradient

  assign loading_attribute = 'eager'
  if lazy
    assign loading_attribute = 'lazy'
  endif
-%}

{%- style -%}
  {{  title_font | font_face: font_display: 'swap' }}
  {{  label_font | font_face: font_display: 'swap' }}
  {{  input_font | font_face: font_display: 'swap' }}
  {{  checkbox_font | font_face: font_display: 'swap' }}
  {{  required_font | font_face: font_display: 'swap' }}
  {{  error_text_font | font_face: font_display: 'swap' }}
  {{  success_text_font | font_face: font_display: 'swap' }}
  {{  submit_font | font_face: font_display: 'swap' }}

  .section-{{ section.id }} {
    border-top: solid {{ border_color }} {{ border_thickness }}px;
    border-bottom: solid {{ border_color }} {{ border_thickness }}px;
    margin-top: {{ margin_top | times: 0.75 | round: 0 }}px;
    margin-bottom: {{ margin_bottom | times: 0.75 | round: 0 }}px;
    margin-left: {{ margin_horizontal_mobile }}rem;
    margin-right: {{ margin_horizontal_mobile }}rem;
    border-radius: {{ section_radius | times: 0.6 | round: 0 }}px;
    overflow: hidden;
  }
  
  .section-{{ section.id }}-settings {
    margin: 0 auto;
    padding-top: {{ padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ padding_bottom | times: 0.75 | round: 0 }}px;
    padding-left: {{ padding_horizontal_mobile }}rem;
    padding-right: {{ padding_horizontal_mobile }}rem;
  }

  .contact-form-{{ section.id }} {
    grid-area: form;
    padding: 0;
    background-color: {{ form_bg_color }};
  }

  .contact-fields-{{ section.id }} {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    align-items: stretch;
    gap: {{ form_gap_mobile }}px;
    margin-top: {{ form_mt_mobile }}px;
  }

  .contact-succesg-{{ section.id }} {
    display: block;
    margin: 16px 0;
    font-size: {{ success_text_size_mobile }}px;
    color: {{ success_text_color }};
    line-height: {{ success_text_height }}%;
    text-align: {{ success_text_align_mobile }};
    text-transform: unset;
  }

  .contact-field-label-{{ section.id }} {
    display: block;
    margin-top: {{ label_mt_mobile }}px;
    font-size: {{ label_size_mobile }}px;
    color: {{ label_color }};
    line-height: {{ label_height }}%;
    text-transform: unset;
    font-weight: 400;
  }

  .contact-field-title-{{ section.id }} + .contact-field-checkbox-{{ section.id }},
  .contact-field-label-{{ section.id }} + .contact-field-checkbox-{{ section.id }} {
    margin-top: {{ label_mt_mobile }}px;
  }
  
  .contact-field-title-{{ section.id }} {
    display: flex;
    align-items: flex-start;
    margin: 0;
    font-size: {{ title_size_mobile }}px;
    color: {{ title_color }};
    line-height: {{ title_height }}%;
    text-transform: unset;
    font-weight: 700;
  }

  .contact-field-required-{{ section.id }} {
    margin: 0;
    font-size: {{ required_size_mobile }}px;
    color: {{ required_color }};
    line-height: {{ required_height }}%;
    text-transform: unset;
  }

  .contact-{{ section.id }} .errors ul {
    padding: 0;
    list-style: none;
  }

  .contact-{{ section.id }} .errors * {
    margin: 16px 0;
    font-size: {{ error_text_size_mobile }}px;
    color: {{ error_text_color }};
    line-height: {{ error_text_height }}%;
    text-align: {{ error_text_align_mobile }};
    text-transform: unset;
  }

  .contact-field-form-tab-{{ section.id }} {
    width: 100%;
    padding: 3rem;
    border: {{ input_border_thickness | plus: 2 }}px solid {{ input_border_color }};
    border-radius: {{ input_radius }}px;
    margin-top: 36px !important;
  }

  .contact-field-form-tab-{{ section.id }} {
    display: none !important;
  }

  .contact-field-form-tab-{{ section.id }}.active {
    display: flex !important;
  }

  .contact-field-input-{{ section.id }} {
    display: block;
    width: 100%;
    margin: 0;
    margin-top: {{ input_mt_mobile }}px;
    font-size: {{ input_size_mobile }}px;
    color: {{ input_color }};
    line-height: {{ input_height }}%;
    text-transform: unset;
    border: 0;
    padding: {{ input_padding_vertical_mobile }}px {{ input_padding_horizontal_mobile }}px;
    border-bottom: {{ input_border_thickness }}px solid {{ input_border_color }}; 
    background-color: {{ input_bg_color }};
    transition: all 0.25s ease 0s;
    border-radius: 0;
  }

  .contact-field-input-{{ section.id }}:hover {
    transition: all 0.25s ease 0s;
    border-color: {{ input_border_hover_color }};
  }

  .contact-field-input-{{ section.id }}:after,
  .contact-field-input-{{ section.id }}:before {
    display: none;
  }

  .contact-field-input-{{ section.id }}:focus,
  .contact-field-input-{{ section.id }}:focus-visible {
    outline-offset: unset;
    box-shadow: none;
    outline: 0px;
    border-color: {{ input_active_color }};
  }

  .contact-field-input-{{ section.id }}::placeholder {
    color: {{ placeholder_color }};
  }

  .contact-field-select-{{ section.id }} {
    position: relative;
  }

  .contact-field-select-svg-{{ section.id }} {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    right: 15px;
    width: 12px;  
  }

  .contact-field-select-svg-{{ section.id }} path {
    fill: {{ select_icon_color }};
  }

  .contact-field-input-select-{{ section.id }} {
    appearance: none;
    -webkit-appearance: none;
    -moz-appearance: none;
    color: {{ input_color }};
    border: {{ input_border_thickness }}px solid {{ input_border_color }};
    border-radius: {{ input_radius }}px;
  }

  .contact-field-title-{{ section.id }} + .contact-field-checkbox-{{ section.id }},
  .contact-field-label-{{ section.id }} + .contact-field-checkbox-{{ section.id }} {
    margin-top: {{ label_mt_mobile }}px;
  }

  .contact-field-checkbox-{{ section.id }} input[type="checkbox"] {
    position: absolute;
    opacity: 0;
    cursor: pointer;
    height: 0;
    width: 0;
  }

  .contact-field-checkbox-{{ section.id }} label {
    position: relative;
    padding-left: {{ checkbox_size_mobile | plus: 5 }}px;
    cursor: pointer;
    margin: 0;
    font-size: {{ checkbox_size_mobile }}px;
    color: {{ checkbox_color }};
    line-height: {{ checkbox_height }}%;
    text-transform: unset;
  }

  .contact-field-checkbox-{{ section.id }} label:before {
    margin: 0;
    content: '';
    position: absolute;
    {% if checkbox_size_mobile < 20 %}
    left: {{ checkbox_size_mobile | times: 0.13 | round: 0 }}px;
    {% else %}
    left: {{ checkbox_size_mobile | times: 0.18 | round: 0 }}px;
    {% endif %}
    {% if checkbox_size_mobile < 10 %}
    top: {{ checkbox_size_mobile | times: 0.4 | round: 0 }}px;
    {% elsif checkbox_size_mobile < 20 %}
    top: {{ checkbox_size_mobile | times: 0.3 | round: 0 }}px;
    {% elsif checkbox_size_mobile < 30 %}
    top: {{ checkbox_size_mobile | times: 0.25 | round: 0 }}px;
    {% else %}
    top: {{ checkbox_size_mobile | times: 0.18 | round: 0 }}px;
    {% endif %}
    width: {{ checkbox_size_mobile | times: 0.7 | round: 0 }}px;
    height: {{ checkbox_size_mobile | times: 0.7 | round: 0 }}px;
    transform: scale(0);
    z-index: 2;
    background-color: {{ checkbox_checkmar_color }};
    clip-path: polygon(13.29% 49.19%,3.89% 59.8%,40.98% 90%,96.95% 21.23%,86.94% 13.08%,85.97% 12.29%,38.95% 70.07%);
  }

  .contact-field-checkbox-{{ section.id }} label:after {
    transform: translateY(-2px);
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    width: {{ checkbox_size_mobile }}px;
    height: {{ checkbox_size_mobile }}px;
    margin-top: 3px;
    z-index: 1;
    background-color: {{ checkbox_bg_color }};
    border: 1px solid {{ checkbox_border_color }};
    border-radius: {{ checkbox_radius }}px;
  }

  .contact-field-checkbox-{{ section.id }} input:checked + label:before {
    transform: scale(1) translateY(-2px);
  }

  .contact-field-checkbox-{{ section.id }} input:checked + label:after {
    background-color: {{ checkbox_bg_active_color }};
    border: 1px solid {{ checkbox_border_active_color }};
  }

  .contact-field-inner-{{ section.id }} textarea {
    min-height: 250px;
    resize: vertical;
    border: {{ input_border_thickness }}px solid {{ input_border_color }};
    border-radius: {{ input_radius }}px;
  }

  .contact-submit-button-{{ section.id }} {
    display: flex;
    width: 100%;
    margin-top: {{ submit_mt_mobile }}px;
    justify-content: {{ content_horizontal_align_mobile }};
  }

  .contact-submit-{{ section.id }} {
    display: block;
    width: 100%;
    word-break: break-word;
    max-width: fit-content;
    font-size: {{ submit_size_mobile }}px;
    color: {{ submit_color }};
    line-height: {{ submit_height }}%;
    text-transform: unset;    
    padding: {{ submit_padding_vertical_mobile }}px {{ submit_padding_horizontal_mobile }}px;
    border: {{ submit_border_thickness }}px solid {{ submit_border_color }};
    border-radius: {{ submit_radius }}px;
    background-color: {{ submit_bg_color }};
    background-image: {{ submit_bg_gradient }};
    cursor: pointer;
    font-weight: 700;
    transition: all 0.25s ease 0s;
  }

  .contact-submit-{{ section.id }}:hover {
    transition: all 0.25s ease 0s;
    color: {{ submit_hover_color }};
    background-color: {{ submit_bg_hover_color }};
    background-image: {{ submit_bg_hover_gradient }};
    border: {{ submit_border_thickness }}px solid {{ submit_border_hover_color }};
  }

  input[type="date"]::-webkit-calendar-picker-indicator {
    opacity: 0;
    cursor: pointer;
    position: relative;
    z-index: 2;
  }

  input[type="date"] {
    position: relative;
  }

  .custom-date-icon-{{ section.id }} {
    position: absolute;
    right: 16px;
    top: 50%;
    transform: translateY(-50%);
    pointer-events: none;
    width: 16px;
    height: 16px;
    fill: {{ input_color }};
  }
  
  @media(min-width: 1024px) {

    .section-{{ section.id }} {
      margin-top: {{ margin_top }}px;
      margin-bottom: {{ margin_bottom }}px;
      margin-left: {{ margin_horizontal }}rem;
      margin-right: {{ margin_horizontal }}rem;
      border-radius: {{ section_radius }}px;
    }
    
    .section-{{ section.id }}-settings {
      padding: 0 5rem;
      padding-top: {{ padding_top }}px;
      padding-bottom: {{ padding_bottom }}px;
      padding-left: {{ padding_horizontal }}rem;
      padding-right: {{ padding_horizontal }}rem;
    }

    .contact-form-{{ section.id }} {
      padding: 0;
    }

    .contact-fields-{{ section.id }} {
      gap: {{ form_gap }}px;
      margin-top: {{ form_mt }}px;
    }

    .contact-field-label-{{ section.id }} {
      margin-top: {{ label_mt }}px;
      font-size: {{ label_size }}px;
    }

    .contact-field-title-{{ section.id }} {
      font-size: {{ title_size }}px;
    }

    .contact-field-title-{{ section.id }} + .contact-field-checkbox-{{ section.id }},
    .contact-field-label-{{ section.id }} + .contact-field-checkbox-{{ section.id }} {
      margin-top: {{ label_mt }}px;
    }

    .contact-field-label-file-{{ section.id }} {
      margin-top: {{ label_mt }}px !important;
      padding: {{ input_padding_vertical }}px {{ input_padding_horizontal }}px {{ input_padding_vertical | plus: 50 }}px;
    }

    .contact-field-required-{{ section.id }} {
      font-size: {{ required_size }}px;
    }

    .contact-succesg-{{ section.id }} {
      font-size: {{ success_text_size }}px;
      text-align: {{ success_text_align }};
    }

    .contact-{{ section.id }} .errors * {
      text-align: {{ error_text_align }};
      font-size: {{ error_text_size }}px;
    }

    .contact-field-input-{{ section.id }} {
      font-size: {{ input_size }}px;
      padding: {{ input_padding_vertical }}px {{ input_padding_horizontal }}px;
      margin-top: {{ input_mt }}px;
    }

    .contact-field-title-{{ section.id }} + .contact-field-checkbox-{{ section.id }},
    .contact-field-label-{{ section.id }} + .contact-field-checkbox-{{ section.id }} {
      margin-top: {{ label_mt }}px;
    }

    .contact-field-checkbox-{{ section.id }} label {
      padding-left: {{ checkbox_size | plus: 5 }}px;
      font-size: {{ checkbox_size }}px;
    }

    .contact-field-checkbox-{{ section.id }} label:before {
      {% if checkbox_size < 10 %}
      left: {{ checkbox_size | times: 0.15 | round: 0 }}px;
      {% elsif checkbox_size < 20 %}
      left: {{ checkbox_size | times: 0.15 | round: 0 }}px;
      {% else %}
      left: {{ checkbox_size | times: 0.18 | round: 0 }}px;
      {% endif %}    
      {% if checkbox_size_mobile < 10 %}
      top: {{ checkbox_size_mobile | times: 0.4 | round: 0 }}px;  
      {% elsif checkbox_size < 20 %}
      top: {{ checkbox_size | times: 0.3 | round: 0 }}px;
      {% elsif checkbox_size < 30 %}
      top: {{ checkbox_size | times: 0.25 | round: 0 }}px;
      {% else %}
      top: {{ checkbox_size | times: 0.18 | round: 0 }}px;
      {% endif %};
      width: {{ checkbox_size | times: 0.7 | round: 0 }}px;
      height: {{ checkbox_size | times: 0.7 | round: 0 }}px;
    }

    .contact-image-{{ section.id }} {
      aspect-ratio: auto !important;
    }

    .contact-field-checkbox-{{ section.id }} label:after {
      width: {{ checkbox_size }}px;
      height: {{ checkbox_size }}px;
    }

    .contact-submit-button-{{ section.id }} {
      margin-top: {{ submit_mt }}px;    
      justify-content: {{ content_horizontal_align }};
    }

    .contact-submit-{{ section.id }} {      
      font-size: {{ submit_size }}px;
      padding: {{ submit_padding_vertical }}px {{ submit_padding_horizontal }}px;
    }
  }
  
{%- endstyle -%}

{% unless full_width %}
  <style>
    .section-{{ section.id }}-settings {
      max-width: {{ content_width }}px;
    }
  </style>
{% endunless %}

{% if margin_horizontal_mobile > 0 %}
  <style>
    .section-{{ section.id }} {
      border-left: solid {{ border_color }} {{ border_thickness }}px;
      border-right: solid {{ border_color }} {{ border_thickness }}px;
    }
    
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: 0px;
        border-right: 0px;
      }
    }
  </style>
{% endif %}

{% if margin_horizontal > 0 %}
  <style>
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: solid {{ border_color }} {{ border_thickness }}px;
        border-right: solid {{ border_color }} {{ border_thickness }}px;
      }
    }
  </style>
{% endif %}


{% if success_text_custom %}
  <style>
    .contact-succesg-{{ section.id }} {
      font-family: {{ success_text_font.family }}, {{ success_text_font.fallback_families }};
      font-weight: {{ success_text_font.weight }};
      font-style: {{ success_text_font.style }};
    }
  </style>
{% endif %}

{% if title_custom %}
  <style>
    .contact-field-title-{{ section.id }} {
      font-family: {{ title_font.family }}, {{ title_font.fallback_families }};
      font-weight: {{ title_font.weight }};
      font-style: {{ title_font.style }};
    }
  </style>
{% endif %}

{% if label_custom %}
  <style>
    .contact-field-label-{{ section.id }} {
      font-family: {{ label_font.family }}, {{ label_font.fallback_families }};
      font-weight: {{ label_font.weight }};
      font-style: {{ label_font.style }};
    }
  </style>
{% endif %}

{% if required_custom %}
  <style>
    .contact-field-required-{{ section.id }} {
      font-family: {{ required_font.family }}, {{ required_font.fallback_families }};
      font-weight: {{ required_font.weight }};
      font-style: {{ required_font.style }};
    }
  </style>
{% endif %}

{% if input_custom %}
  <style>
    .contact-field-input-{{ section.id }},
    .contact-field-input-{{ section.id }}::placeholder,
    .contact-field-inner--{{ section.id }} textarea {
      font-family: {{ input_font.family }}, {{ input_font.fallback_families }} !important;
      font-weight: {{ input_font.weight }} !important;
      font-style: {{ input_font.style }} !important;
    }
  </style>
{% endif %}

{% if checkbox_custom %}
  <style>
    .contact-field-checkbox-{{ section.id }} label {
      font-family: {{ checkbox_font.family }}, {{ checkbox_font.fallback_families }};
      font-weight: {{ checkbox_font.weight }};
      font-style: {{ checkbox_font.style }};
    }
  </style>
{% endif %}

{% if error_text_custom %}
  <style>
    .contact-{{ section.id }} .errors * {
      font-family: {{ error_text_font.family }}, {{ error_text_font.fallback_families }};
