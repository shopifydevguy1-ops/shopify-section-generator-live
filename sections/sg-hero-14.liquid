{%- liquid 
  assign padding_horizontal = section.settings.padding_horizontal
  assign padding_horizontal_mobile = section.settings.padding_horizontal_mobile
  assign padding_top = section.settings.padding_top
  assign padding_bottom = section.settings.padding_bottom
  assign border_color = section.settings.border_color
  assign border_thickness = section.settings.border_thickness
  assign margin_top = section.settings.margin_top
  assign margin_bottom = section.settings.margin_bottom
  assign margin_horizontal_mobile = section.settings.margin_horizontal_mobile
  assign margin_horizontal = section.settings.margin_horizontal
  assign background_color = section.settings.background_color
  assign background_gradient = section.settings.background_gradient
  assign full_width = section.settings.full_width
  assign content_width = section.settings.content_width
  assign section_radius = section.settings.section_radius

  assign hero_height = section.settings.hero_height
  assign hero_fixed_height = section.settings.hero_fixed_height

  assign body_gap_mobile = section.settings.body_gap_mobile
  assign body_gap = section.settings.body_gap

  assign content_horizontal_align_mobile = section.settings.content_horizontal_align_mobile
  assign content_horizontal_align = section.settings.content_horizontal_align
  assign content_vertical_align = section.settings.content_vertical_align
  assign content_vertical_align_mobile = section.settings.content_vertical_align_mobile
  assign content_padding_vertical = section.settings.content_padding_vertical
  assign content_padding_vertical_mobile = section.settings.content_padding_vertical_mobile
  assign content_padding_horizontal = section.settings.content_padding_horizontal
  assign content_padding_horizontal_mobile = section.settings.content_padding_horizontal_mobile
  assign content_overlay = section.settings.content_overlay
  assign content_use_fixed_height = section.settings.content_use_fixed_height
  assign content_fixed_height = section.settings.content_fixed_height

  assign text_align = ""
  if content_horizontal_align == "start"
    assign text_align = "left"
  elsif content_horizontal_align == "center"
    assign text_align = "center"
  else
    assign text_align = "right"
  endif

  assign text_align_mobile = ""
  if content_horizontal_align_mobile == "start"
    assign text_align_mobile = "left"
  elsif content_horizontal_align_mobile == "center"
    assign text_align_mobile = "center"
  else
    assign text_align_mobile = "right"
  endif

  assign content_image = section.settings.content_image

  assign video = section.settings.video
  assign video_url = section.settings.video_url
  assign video_ratio_mobile = section.settings.video_ratio_mobile
  assign video_bottom_mobile = section.settings.video_bottom_mobile
  assign video_hide_mobile = section.settings.video_hide_mobile
  
-%}

{%- style -%}
  
  .section-{{ section.id }} {
    border-top: solid {{ border_color }} {{ border_thickness }}px;
    border-bottom: solid {{ border_color }} {{ border_thickness }}px;
    margin-top: {{ margin_top | times: 0.75 | round: 0 }}px;
    margin-bottom: {{ margin_bottom | times: 0.75 | round: 0 }}px;
    margin-left: {{ margin_horizontal_mobile }}rem;
    margin-right: {{ margin_horizontal_mobile }}rem;
    border-radius: {{ section_radius | times: 0.6 | round: 0 }}px;
    overflow: hidden;
  }
  
  .section-{{ section.id }}-settings {
    margin: 0 auto;
    padding-top: {{ padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ padding_bottom | times: 0.75 | round: 0 }}px;
    padding-left: {{ padding_horizontal_mobile }}rem;
    padding-right: {{ padding_horizontal_mobile }}rem;
    height: 100%;
  }

  .hero-body-{{ section.id }} {
    display: grid;
    grid-template-columns: 1fr;
    gap: {{ body_gap_mobile }}px;
    height: 100%;
  }

  .hero-content-{{ section.id }} {
    position: relative;
    display: flex;
    flex-direction: column;
    align-items: {{ content_horizontal_align_mobile }};
    justify-content: {{ content_vertical_align_mobile }};
    padding: {{ content_padding_vertical_mobile }}px {{ content_padding_horizontal_mobile }}px;
  }

  .hero-content-{{ section.id }}:after {
    content: '';
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    background-color: rgba(0,0,0,{{ content_overlay }});
    z-index: 1;
  }

  .hero-image-{{ section.id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
  }

  .hero-image-{{ section.id }} img,
  .hero-image-{{ section.id }} svg {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .hero-image-{{ section.id }} svg {
    background-color: #AFAFAF;
  }

  .hero-video-{{ section.id }} {
    position: relative;
  }

  .hero-video-{{ section.id }} video,
  .hero-video-{{ section.id }} iframe {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
    box-shadow: none;
    border: 0px;
  }

  .hero-video-{{ section.id }}::after {
    content: "";
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 1;
  }
  
  @media(min-width: 1024px) {

    .section-{{ section.id }} {
      margin-top: {{ margin_top }}px;
      margin-bottom: {{ margin_bottom }}px;
      margin-left: {{ margin_horizontal }}rem;
      margin-right: {{ margin_horizontal }}rem;
      border-radius: {{ section_radius }}px;
    }
    
    .section-{{ section.id }}-settings {
      padding: 0 5rem;
      padding-top: {{ padding_top }}px;
      padding-bottom: {{ padding_bottom }}px;
      padding-left: {{ padding_horizontal }}rem;
      padding-right: {{ padding_horizontal }}rem;
    }

    .hero-body-{{ section.id }} {
      gap: {{ body_gap }}px;
    }

    .hero-content-{{ section.id }} {
      justify-content: {{ content_vertical_align }};
      align-items: {{ content_horizontal_align }};
      padding: {{ content_padding_vertical }}px {{ content_padding_horizontal }}px;
      order: 1 !important;
    }

    .hero-video-{{ section.id }} {
      aspect-ratio: unset !important;
    }

    .hero-video-{{ section.id }} {
      display: block !important;
      order: 2 !important;
    }
  }
  
{%- endstyle -%}

{% unless full_width %}
  <style>
    .section-{{ section.id }}-settings {
      max-width: {{ content_width }}rem;
    }
  </style>
{% endunless %}

{% if margin_horizontal_mobile > 0 %}
  <style>
    .section-{{ section.id }} {
      border-left: solid {{ border_color }} {{ border_thickness }}px;
      border-right: solid {{ border_color }} {{ border_thickness }}px;
    }
    
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: 0px;
        border-right: 0px;
      }
    }
  </style>
{% endif %}

{% if margin_horizontal > 0 %}
  <style>
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: solid {{ border_color }} {{ border_thickness }}px;
        border-right: solid {{ border_color }} {{ border_thickness }}px;
      }
    }
  </style>
{% endif %}

{% if hero_height == "adapt_first" %}
  <style>

    @media(min-width: 1024px) {
      .hero-{{ section.id }} {
        height: 95vh;
      }

      .hero-video-{{ section.id }} video,
      .hero-video-{{ section.id }} iframe {
        max-height: calc(95vh - {{ padding_top }}px - {{ padding_bottom }}px);
      }
    }
  </style>
{% else %}
  <style>
  
    @media(min-width: 1024px) {
      .hero-{{ section.id }} {
        height: {{ hero_fixed_height }}px;
      }

      .hero-video-{{ section.id }} video,
      .hero-video-{{ section.id }} iframe {
        max-height: calc({{ hero_fixed_height }}px - {{ padding_top }}px - {{ padding_bottom }}px);
      }
    }
  </style>
{% endif %}

{% if content_use_fixed_height %}
  <style>
    .hero-content-{{ section.id }} {
      height: {{ content_fixed_height }}px;
    }

    @media(min-width: 1024px) {
      .hero-content-{{ section.id }} {
        height: auto;
      }
    }
  </style>
{% endif %}

{% if video != null or video_url != null %}
  <style>
    @media(min-width: 1024px) {
      .hero-body-{{ section.id }} {
        grid-template-columns: 1fr 1fr;
      }
    }
  </style>
{% endif %}

{% if video_ratio_mobile == "portrait" %}
    <style>
      .hero-video-{{ section.id }} {
          aspect-ratio: 9.6/12; 
      }
    </style>
{% elsif video_ratio_mobile == "landscape" %}
    <style>
      .hero-video-{{ section.id }}{
          aspect-ratio: 12/10; 
      }
    </style>
{% elsif video_ratio_mobile == "square" %}
    <style>
      .hero-video-{{ section.id }} {
          aspect-ratio: 12/12; 
      }
    </style>
{% else %}
    <style>
      .hero-video-{{ section.id }} {
          aspect-ratio: auto; 
      }
    </style>
{% endif %}

{% if video_bottom_mobile %}
  <style>
     .hero-video-{{ section.id }} {
      order: 2;
     }
    
    .hero-content-{{ section.id }} {
      order: 1;
    }
  </style>
{% else %}
  <style>
    .hero-video-{{ section.id }} {
      order: 1;
     }
    
    .hero-content-{{ section.id }} {
      order: 2;
    }
  </style>
{% endif %}

{% if video_hide_mobile %}
  <style>
    .hero-video-{{ section.id }} {
      display: none;
    }
  </style>
{% endif %}

{% for block in section.blocks %}

  {% case block.type %}
    
    {% when 'heading' %}

      <style>
        {{  block.settings.heading_font | font_face: font_display: 'swap' }}

        .hero-heading-{{ block.id }} {
          margin-top: {{ block.settings.heading_mt_mobile }}px;
          text-align: {{ text_align_mobile }};
          position: relative;
          z-index: 2;
        }
      
        .hero-heading-{{ block.id }} * {
          margin: 0;
          font-size: {{ block.settings.heading_size_mobile }}px;
          color: {{ block.settings.heading_color }};
          line-height: {{ block.settings.heading_height }}%;
          text-transform: unset;
        }

        @media(min-width: 1024px) {
          
          .hero-heading-{{ block.id }} {
            margin-top: {{ block.settings.heading_mt }}px;
            text-align: {{ text_align }};
          }
        
          .hero-heading-{{ block.id }} * {
            font-size: {{ block.settings.heading_size }}px;
          }
        }
      </style>

      {% if block.settings.heading_custom %}
        <style>
          .hero-heading-{{ block.id }} * {
            font-family: {{ block.settings.heading_font.family }}, {{ block.settings.heading_font.fallback_families }};
            font-weight: {{ block.settings.heading_font.weight }};
            font-style: {{ block.settings.heading_font.style }};
          }
        </style>
      {% endif %}

    {% when 'text' %}

      <style>
        {{  block.settings.text_font | font_face: font_display: 'swap' }}
        
        .hero-text-{{ block.id }} {
          margin-top: {{ block.settings.text_mt_mobile }}px;
          text-align: {{ text_align_mobile }};
          position: relative;
          z-index: 2;
        }
      
        .hero-text-{{ block.id }} * {
          margin: 0;
          font-size: {{ block.settings.text_size_mobile }}px;
          color: {{ block.settings.text_color }};
          line-height: {{ block.settings.text_height }}%;
          text-transform: unset;
        }

        @media(min-width: 1024px) {
          
          .hero-text-{{ block.id }} {
            margin-top: {{ block.settings.text_mt }}px;
            text-align: {{ text_align }};
          }
      
          .hero-text-{{ block.id }} * {
            font-size: {{ block.settings.text_size }}px;
          }
          
        }
      </style>

      {% if block.settings.text_custom %}
        <style>
          .hero-text-{{ block.id }} * {
            font-family: {{ block.settings.text_font.family }}, {{ block.settings.text_font.fallback_families }};
            font-weight: {{ block.settings.text_font.weight }};
            font-style: {{ block.settings.text_font.style }};
          }
        </style>
      {% endif %}

    {% when 'button' %}

      <style>
        {{  block.settings.button_font | font_face: font_display: 'swap' }}
        
        .hero-button-{{ block.id }} {
          display: flex;
          align-items: center;
          justify-content: center;
          gap: 10px;
          width: 100%;
          max-width: fit-content;
          margin: 0;
          margin-top: {{ block.settings.button_mt_mobile }}px;
          font-size: {{ block.settings.button_size_mobile }}px;
          color: {{ block.settings.button_color }};
          line-height: {{ block.settings.button_height }}%;
          text-align: center;
          text-transform: unset;
          text-decoration: none;
          padding: {{ block.settings.button_padding_vertical_mobile }}px {{ block.settings.button_padding_horizontal_mobile }}px;
          border-radius: {{ block.settings.button_radius }}px;
          transition: all 0.25s ease 0s;
          position: relative;
          font-weight: 700;
          z-index: 2;
        }
      
        .hero-button-{{ block.id }}:hover {
          color: {{ block.settings.button_hover_color }};
          transition: all 0.25s ease 0s;
        }
      
        .hero-button-{{ block.id }} svg {
          width: 14px;
          height: 14px;
        }
      
        .hero-button-{{ block.id }} svg path {
          fill: {{ block.settings.button_color }};
          transition: all 0.25s ease 0s;
        }
      
        .hero-button-{{ block.id }}:hover svg path {
          fill: {{ block.settings.button_hover_color }};
          transition: all 0.25s ease 0s;
        }

        @media(min-width: 1024px) {
          
          .hero-button-{{ block.id }} {
            margin-top: {{ block.settings.button_mt }}px;
            padding: {{ block.settings.button_padding_vertical }}px {{ block.settings.button_padding_horizontal }}px;
            font-size: {{ block.settings.button_size }}px;
          }
          
        }
      </style>

      {% if block.settings.button_custom %}
        <style>
          .hero-button-{{ block.id }} {
            font-family: {{ block.settings.button_font.family }}, {{ block.settings.button_font.fallback_families }};
            font-weight: {{ block.settings.button_font.weight }};
            font-style: {{ block.settings.button_font.style }};
          }
        </style>
      {% endif %}

      {% if block.settings.button_style == "non_outline" or block.settings.button_style == "non_outline_arrow" %}
        <style>
          .hero-button-{{ block.id }} {
            background-color: {{ block.settings.button_bg_color }};
          }
      
          .hero-button-{{ block.id }}:hover {
            background-color: {{ block.settings.button_bg_hover_color }};
          }
        </style>
      {% elsif block.settings.button_style == "outline" or block.settings.button_style == "outline_arrow" %}
        <style>
          .hero-button-{{ block.id }} {
            background-color: {{ block.settings.button_bg_color }};
            border: {{ block.settings.button_border_thickness }}px solid {{ block.settings.button_border_color }};
          }
      
          .hero-button-{{ block.id }}:hover {
            background-color: {{ block.settings.button_bg_hover_color }};
            border: {{ block.settings.button_border_thickness }}px solid {{ block.settings.button_border_hover_color }};
          }
        </style>
      {% elsif block.settings.button_style == "link" %}
        <style>
          .hero-button-{{ block.id }} {
            padding: 0px !important;
          }
        </style>
      {% endif %}

    {% when 'product' %}

      <style>
        
        .hero-product-{{ block.id }} {
          text-decoration: none;
          position: absolute;
          width: 100%;
          max-width: {{ block.settings.product_width_mobile }}px;
          z-index: 2;
        }
      
        .hero-product-image-{{ block.id }} {
          border-radius: {{ block.settings.product_image_radius }}px;
          overflow: hidden;
        }
      
        .hero-product-image-{{ block.id }} img,
        .hero-product-image-{{ block.id }} svg {
          display: block;
          width: 100%;
          height: 100%;
          object-fit: cover;
        }
      
        .hero-product-image-{{ block.id }} svg {
          background-color: #AFAFAF;
        }
      
        .hero-product-title-{{ block.id }} {
          margin: 0;
          margin-top: {{ block.settings.title_mt_mobile }}px;
          text-align: {{ block.settings.title_align_mobile }};
          font-size: {{ block.settings.title_size_mobile }}px;
          color: {{ block.settings.title_color }};
          line-height: {{ block.settings.title_height }}%;
          text-transform: unset;
          font-weight: 700;
        }

        @media(min-width: 1024px) {

          .hero-product-{{ block.id }} {
            max-width: {{ block.settings.product_width }}px;
          }
      
          .hero-product-title-{{ block.id }} {
            margin-top: {{ block.settings.title_mt }}px;
            text-align: {{ block.settings.title_align }};
            font-size: {{ block.settings.title_size }}px;
          }
          
        }
      </style>

      {% if block.settings.title_custom %}
        <style>
          .hero-product-title-{{ block.id }} {
            font-family: {{ block.settings.title_font.family }}, {{ block.settings.title_font.fallback_families }};
            font-weight: {{ block.settings.title_font.weight }};
            font-style: {{ block.settings.title_font.style }};
          }
        </style>
      {% endif %}

      {% if  block.settings.product_image_ratio_mobile == "portrait" %}
        <style>
          .hero-product-image-{{ block.id }} {
              aspect-ratio: 9.6/12; 
          }
        </style>
      {% elsif  block.settings.product_image_ratio_mobile == "landscape" %}
        <style>
          .hero-product-image-{{ block.id }}{
              aspect-ratio: 12/10; 
          }
        </style>
      {% elsif  block.settings.product_image_ratio_mobile == "square" %}
        <style>
          .hero-product-image-{{ block.id }} {
              aspect-ratio: 12/12; 
          }
        </style>
      {% else %}
        <style>
          .hero-product-image-{{ block.id }} {
              aspect-ratio: auto; 
          }
        </style>
      {% endif %}
      
      {% if  block.settings.product_image_ratio == "portrait" %}
        <style>
          @media(min-width: 1024px) {
           .hero-product-image-{{ block.id }} {
                aspect-ratio: 9.6/12; 
            } 
          }
        </style>
      {% elsif  block.settings.product_image_ratio == "landscape" %}
        <style>
          @media(min-width: 1024px) {
           .hero-product-image-{{ block.id }} {
                aspect-ratio: 12/10; 
            } 
          }
        </style>
      {% elsif  block.settings.product_image_ratio == "square" %}
        <style>
          @media(min-width: 1024px) {
           .hero-product-image-{{ block.id }} {
                aspect-ratio: 10/10; 
            } 
          }
        </style>
      {% else %}
        <style>
          @media(min-width: 1024px) {
           .hero-product-image-{{ block.id }} {
                aspect-ratio: auto; 
            } 
          }
        </style>
      {% endif %}

      {% if block.settings.product_horizontal_align_mobile == "start" %}
        <style>
          .hero-product-{{ block.id }} {
            left: {{ content_padding_horizontal_mobile }}px;
          }
        </style>
      {% elsif block.settings.product_horizontal_align_mobile == "center" %}
        <style>
          .hero-product-{{ block.id }} {
            left: 50%;
            transform: translateX(-50%);
          }
        </style>
      {% else %}
        <style>
          .hero-product-{{ block.id }} {
            right: {{ content_padding_horizontal_mobile }}px;
          }
        </style>
      {% endif %}
      
      {% if block.settings.product_horizontal_align == "start" %}
        <style>
          @media(min-width: 1024px) {
            .hero-product-{{ block.id }} {
              left: {{ content_padding_horizontal }}px;
              right: unset;
              transform: translateX(0);
            }
          }
        </style>
      {% elsif block.settings.product_horizontal_align == "center" %}
        <style>
          @media(min-width: 1024px) {
            .hero-product-{{ block.id }} {
              left: 50%;
              right: unset;
              transform: translateX(-50%);
            }
          }
        </style>
      {% else %}
        <style>
          @media(min-width: 1024px) {
            .hero-product-{{ block.id }} {
              left: unset;
              right: {{ content_padding_horizontal }}px;
              transform: translateX(0);
            }
          }
        </style>
      {% endif %}
      
      {% if block.settings.product_vertical_align_mobile == "start" %}
        <style>
          .hero-product-{{ block.id }} {
            top: {{ content_padding_vertical_mobile }}px;
          }
        </style>
      {% elsif block.settings.product_vertical_align_mobile == "center" %}
        <style>
          .hero-product-{{ block.id }} {
            top: 50%;
            transform: translateY(-50%) {% if block.settings.product_horizontal_align_mobile == "center" %}translateX(-50%){% endif %};
          }
        </style>
      {% else %}
        <style>
          .hero-product-{{ block.id }} {
            bottom: {{ content_padding_vertical_mobile }}px;
          }
        </style>
      {% endif %}
      
      {% if block.settings.product_vertical_align == "start" %}
        <style>
          @media(min-width: 1024px) {
            .hero-product-{{ block.id }} {
              top: {{ content_padding_vertical }}px;
              bottom: unset;
              transform: translateY(0) {% if block.settings.product_horizontal_align == "center" %}translateX(-50%){% endif %};
            }
          }
        </style>
      {% elsif block.settings.product_vertical_align == "center" %}
        <style>
          @media(min-width: 1024px) {
            .hero-product-{{ block.id }} {
              top: 50%;
              bottom: unset;
              transform: translateY(-50%) {% if block.settings.product_horizontal_align == "center" %}translateX(-50%){% endif %};
            }
          }
        </style>
      {% else %}
        <style>
          @media(min-width: 1024px) {
            .hero-product-{{ block.id }} {
              top: unset;
              bottom: {{ content_padding_vertical }}px;
              transform: translateY(0) {% if block.settings.product_horizontal_align == "center" %}translateX(-50%){% endif %};
            }
          }
        </style>
      {% endif %}

  {% endcase %}

{% endfor %}

<div class="section-{{ section.id }} hero-{{ section.id }}" style="background-color:{{ background_color }}; background-image: {{ background_gradient }};">
    <div class="section-{{ section.id }}-settings">
      <div class="hero-body-{{ section.id }}">
        <div class="hero-content-{{ section.id }}">
          {% if video_bottom_mobile %}
            <div class="hero-image-{{ section.id }}">
              {% if content_image != blank %}
                <img src="{{ content_image | image_url }}" alt="{{ content_image.alt }}">
              {% else %}
                {{ 'image' | placeholder_svg_tag }}
              {% endif %}
            </div>
          {% endif %}

          {% for block in section.blocks %}

            {% case block.type %}

              {% when 'heading' %}
          
                {% if block.settings.heading != blank %}
                  <div class="hero-heading-{{ block.id }}">
                    {{ block.settings.heading }}
                  </div>
                {% endif %}

              {% when 'text' %}
    
              {% if block.settings.text != blank %}
                <div class="hero-text-{{ block.id }}">
                  {{ block.settings.text }}
                </div>
              {% endif %}

              {% when 'button' %}
    
                {% if block.settings.button != blank %}
                  <a href="{{ block.settings.button_url }}" class="hero-button-{{ block.id }}">
                    {{ block.settings.button }}
                    {% if block.settings.button_style == "non_outline_arrow" or block.settings.button_style == "outline_arrow" %}
                      <svg width="192" height="192" viewBox="0 0 192 192" fill="none" xmlns="http://www.w3.org/2000/svg">
