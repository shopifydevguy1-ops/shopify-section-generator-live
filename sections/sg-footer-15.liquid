    {% when 'newsletter' %}    
      <style>     
        {{  block.settings.input_font | font_face: font_display: 'swap' }}
        {{  block.settings.submit_font | font_face: font_display: 'swap' }}
        {{  block.settings.thanks_text_font | font_face: font_display: 'swap' }}       

        .footer-newsletter-row-{{ block.id }} {
          margin-top: 20px;
          position: relative;
          display: flex;
          align-items: stretch;
          gap: 16px;
          padding: {{ block.settings.input_padding_vertical_mobile }}px {{ block.settings.input_padding_horizontal_mobile }}px;
          max-width: fit-content;
          margin-top: {{ block.settings.form_mt_mobile }}px;
          background-color: {{ block.settings.input_bg_color }};        
          border-radius: {{ block.settings.input_radius }}px;
          border: {{ block.settings.input_border_thickness }}px solid {{ block.settings.input_border_color }};
        }
      
        .footer-newsletter-input-{{ block.id }} {
          width: 100%;
          margin: 0;
          box-shadow: none;
          border: 0px;     
          font-size: {{ block.settings.input_size_mobile }}px;
          color: {{ block.settings.input_color }};
          line-height: {{ block.settings.input_height }}%;
          text-transform: unset;
          border-radius: {{ block.settings.input_radius }}px;
          background-color: transparent;         
          padding: {{ block.settings.input_padding_vertical_mobile }}px {{ block.settings.input_padding_horizontal_mobile }}px;
        }
      
        .footer-newsletter-input-{{ block.id }}::placeholder {
          color: {{ block.settings.input_placeholder_color }};
        }
        
        .footer-newsletter-input-{{ block.id }}:focus {
          outline: none;
        }
      
        .footer-newsletter-submit-{{ block.id }} {
          margin-top: 0px;
          position: static !important;
          max-width: 100%;
          display: flex;
          align-items: center;
          justify-content: center;
          font-size: {{ block.settings.submit_size_mobile }}px;
          color: {{ block.settings.submit_color }};
          line-height: {{ block.settings.submit_height }}%;
          text-align: center;
          text-transform: unset;
          text-decoration: none;
          border-radius: {{ block.settings.submit_radius }}px;
          padding: {{ block.settings.submit_padding_vertical_mobile }}px {{ block.settings.submit_padding_horizontal_mobile }}px;         
          border: 0px;
          border: {{ block.settings.submit_border_thickness }}px solid {{ block.settings.submit_border_color }} ;
          background-color: transparent;
          background-color: {{ block.settings.submit_bg_color }};
          transition: all 0.25s ease 0s;
          box-shadow: none;
          cursor: pointer;
        }
      
        .footer-newsletter-submit-{{ block.id }}:hover {
          color: {{ block.settings.submit_hover_color }};
          border: {{ block.settings.submit_border_thickness }}px solid {{ block.settings.submit_border_hover_color }};
          background-color: {{ block.settings.submit_bg_hover_color }};
          transition: all 0.25s ease 0s;
        }

       .footer-newsletter-submit-{{ block.id }}[disabled="true"] {
         opacity: 0.6;
       }

       .footer-newsletter-checkbox-{{ block.id }} {
         opacity: 0;
         z-index: -100;
         position: absolute;
       }

       .footer-newsletter-label-{{ block.id }} {
         position: relative;
         display: inline-flex;
         align-items: center;
         gap: 10px;
         margin-top: {{ block.settings.privacy_mt_mobile }}px;
         cursor: pointer;
       }

       .footer-newsletter-label-{{ block.id }} .checkbox-view {
         content: '';
         display: inline-block;
         width: 18px;
         flex-shrink: 0;
         height: 18px;
         padding: 2px;
         border-radius: 4px;
         border: 1px solid {{ block.settings.privacy_checkbox_color }};
         transition: all 0.25s ease 0s;
       }

       .footer-newsletter-label-{{ block.id }} .checkbox-icon {
          width: 12px;
          opacity: 0;
          transition: .3s;
        }

       .footer-newsletter-label-{{ block.id }} input:checked + .checkbox-view {
          background-color: {{ block.settings.privacy_checkbox_active_color }};
          background-color: {{ block.settings.privacy_checkbox_active_color }};
          transition: all 0.25s ease 0s;
        }
        
        .footer-newsletter-label-{{ block.id }} input:checked + .checkbox-view .checkbox-icon {
          opacity: 1;
        }

       .footer-newsletter-label-{{ block.id }} * {
          margin: 0px;
          font-size: {{ block.settings.privacy_size_mobile }}px;
          color: {{ block.settings.privacy_color }};
          line-height: {{ block.settings.privacy_height }}%;
          text-transform: unset;
       }

       .footer-newsletter-thanks-text-{{ block.id }} {
          display: none;
          margin: 0px;
          text-align: {{ content_align_mobile }};
          margin-top: {{ block.settings.thanks_text_mt_mobile }}px;
          font-size: {{ block.settings.thanks_text_size_mobile }}px;
          color: {{ block.settings.thanks_text_color }};
          line-height: {{ block.settings.thanks_text_height }}%;
          text-transform: unset;
       }

       .footer-newsletter-thanks-text-{{ block.id }}.active {
         display: block;
       }

        @media(min-width: 1024px) {      
          .footer-newsletter-row-{{ block.id }} {         
            padding: {{ block.settings.input_padding_vertical }}px {{ block.settings.input_padding_horizontal }}px; 
            margin-top: {{ block.settings.form_mt }}px;
          }

          .footer-newsletter-input-{{ block.id }} {
            width: {{ block.settings.input_width }}px;         
            font-size: {{ block.settings.input_size }}px;
            padding: {{ block.settings.input_padding_vertical }}px {{ block.settings.input_padding_horizontal }}px;
          }
      
          .footer-newsletter-submit-{{ block.id }} {
            padding: {{ block.settings.submit_padding_vertical }}px {{ block.settings.submit_padding_horizontal }}px;
            font-size: {{ block.settings.submit_size }}px;
          }

          .footer-newsletter-label-{{ block.id }} {
             margin-top: {{ block.settings.privacy_mt }}px;
           }
    
           .footer-newsletter-label-{{ block.id }} * {
              font-size: {{ block.settings.privacy_size }}px;
           }

           .footer-newsletter-thanks-text-{{ block.id }} {
             text-align: {{ content_align }};
             margin-top: {{ block.settings.thanks_text_mt }}px;
             font-size: {{ block.settings.thanks_text_size }}px;
           }
        }
      </style>

      {% if block.settings.input_custom %}
        <style>
          .footer-newsletter-input-{{ block.id }} {
            font-family: {{ block.settings.input_font.family }}, {{ block.settings.input_font.fallback_families }};
            font-weight: {{ block.settings.input_font.weight }};
            font-style: {{ block.settings.input_font.style }};
          }
        </style>
      {% endif %}
      
      {% if block.settings.submit_custom %}
        <style>
          .footer-newsletter-submit-{{ block.id }} {
            font-family: {{ block.settings.submit_font.family }}, {{ block.settings.submit_font.fallback_families }};
            font-weight: {{ block.settings.submit_font.weight }};
            font-style: {{ block.settings.submit_font.style }};
          }
        </style>
      {% endif %}

      {% if block.settings.use_shadow %}
        <style>
          .footer-newsletter-row-{{ block.id }} {          
            box-shadow: 0 1px 2px {{ block.settings.input_shadow_color | hex_to_rgba: 0.8 }},0 1px 3px {{ block.settings.input_shadow_color | hex_to_rgba: 0.1 }};             
          }
        </style>
      {% endif %}

      {% if block.settings.thanks_text_custom %}
        <style>
          .footer-newsletter-thanks-text-{{ block.id }} {
            font-family: {{ block.settings.thanks_text_font.family }}, {{ block.settings.thanks_text_font.fallback_families }};
            font-weight: {{ block.settings.thanks_text_font.weight }};
            font-style: {{ block.settings.thanks_text_font.style }};
          }
        </style>
      {% endif %}

      {% if content_align_mobile == 'right' %}
        <style>
          .footer-newsletter-row-{{ block.id }} {
            margin-left: auto;         
          }
        </style>
      {% elsif content_align_mobile == 'center' %}
        <style>
          .footer-newsletter-row-{{ block.id }} {
            margin-left: auto;
            margin-right: auto;           
          }
        </style>  
      {% elsif content_align_mobile == 'left' %}
        <style>
          .footer-newsletter-row-{{ block.id }} {
            margin-right: auto;          
          }
        </style>  
      {% endif %}
      
      {% if content_align == 'right' %}
        <style>
          @media(min-width: 1024px) {
            .footer-newsletter-row-{{ block.id }} {
              margin-left: auto;
              margin-right: 0;
            }
          }   
        </style>
      {% elsif content_align == 'center' %}
        <style>
          @media(min-width: 1024px) {
            .footer-newsletter-row-{{ block.id }} {
              margin-left: auto;
              margin-right: auto;
            }
          } 
        </style>  
      {% elsif content_align == 'left' %}
        <style>
          @media(min-width: 1024px) {
            .footer-newsletter-row-{{ block.id }} {
              margin-right: 0;
              margin-left: 0;
            }
          } 
