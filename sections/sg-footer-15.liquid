<div class="section-{{ section.id }} footer-{{ section.id }}" style="background-color:{{ background_color }}; background-image: {{ background_gradient }};">
    <div class="section-{{ section.id }}-settings">
      {% for block in section.blocks %}
        {% case block.type %}
          {% when 'heading' %}
            {% if block.settings.heading != blank %}
              <div class="footer-heading-{{ block.id }}">
                {{ block.settings.heading }}
              </div>
            {% endif %}
          {% when 'text' %}
            {% if block.settings.text != blank %}
              <div class="footer-text-{{ block.id }}">
                {{ block.settings.text }}
              </div>
            {% endif %} 
          {% when 'newsletter' %}
            <div class="footer-newsletter-form-{{ section.id }}">
              {% form 'customer', class: 'footer-newsletter-form' %}
                <input type="hidden" name="contact[tags]" value="newsletter">
    
                <div class="footer-newsletter-row-{{ block.id }}">
                  <input
                    type="email"
                    name="contact[email]"
                    class="footer-newsletter-input-{{ block.id }}"
                    value="{{ form.email }}"
                    aria-required="true"
                    autocorrect="off"
                    autocapitalize="off"
                    autocomplete="email"
                    {% if form.errors %}
                      autofocus
                      aria-invalid="true"
                      aria-describedby="Newsletter-error--{{ block.id }}"
                    {% elsif form.posted_successfully? %}
                      aria-describedby="Newsletter-succesg--{{ block.id }}"
                    {% endif %}
                      placeholder="{{ block.settings.placeholder_mail }}"
                    required
                  >
                  <input type="submit" class="footer-newsletter-submit-{{ block.id }} footer-newsletter-submit-{{ section.id }}" name="commit" value="{{ block.settings.submit }}">
                </div>

                {% if form.posted_successfully? %}
                  {% if block.settings.thanks_text != blank %}
                    <p class="footer-newsletter-thanks-text-{{ block.id }} footer-newsletter-thanks-text-{{ section.id }}">{{ block.settings.thanks_text }}</p>
                  {% endif %}
                {% endif %}
              {% endform %}        
            </div>          
          {% else %}
            
        {% endcase %}
      {% endfor %}
    
      <div class="footer-features-{{ section.id }}">
        {% for block in section.blocks %}          
          {% if block.type == 'feature' %}
             <div class="footer-feature-{{ section.id }}">
                {% if block.settings.custom_icon != blank or block.settings.icon != "none" %}
                  <div class="footer-feature-icon-{{ section.id }} feature-feature-icon-{{ block.id }}">
                    {% if block.settings.custom_icon != blank %}
                      {%- capture sizes -%}(min-width: {{ content_width }}px) {{ content_width | minus: 100 | divided_by: 2 }}px, (min-width: 750px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2){%- endcapture -%}
                      {{ block.settings.custom_icon | image_url: width: 1500 | image_tag:
                        loading: loading_attribute,
                        width: block.settings.custom_icon.width,
                        height: block.settings.custom_icon.height,
                        sizes: sizes,
                        widths: '165, 360, 535, 750, 1070, 1500'
                      }}
                    {% else %}
                        {% case block.settings.icon %}
                          {% when 'wallet_2' %}
                            {{ wallet_2 }}
                          {% when 'target' %}
                            {{ target }}
                          {% when 'tags' %}
                            {{ tags }}
                          {% when 'sliders' %}
                            {{ sliders }}
                          {% when 'shop' %}
                            {{ shop }}
                          {% when 'shield_check' %}
                            {{ shield_check }}
                          {% when 'scissors_coupon_2' %}
                            {{ scissors_coupon_2 }}
                          {% when 'rocket' %}
                            {{ rocket }}
                          {% when 'refresh_2' %}
                            {{ refresh_2 }}
                          {% when 'star_sparkle' %}
                            {{ star_sparkle }}
                          {% when 'pin_tack' %}
                            {{ pin_tack }}
                          {% when 'money_bill_coin' %}
                            {{ money_bill_coin }}
                          {% when 'lock' %}
                            {{ lock }}
                          {% when 'life_ring' %}
                            {{ life_ring }}
                          {% when 'leaf' %}
                            {{ leaf }}
                          {% when 'image_mountain' %}
                            {{ image_mountain }}
                          {% when 'heart_hand' %}
                            {{ heart_hand }}
                          {% when 'heart' %}
                            {{ heart }}
                          {% when 'headset' %}
                            {{ headset }}
                          {% when 'hand' %}
                            {{ hand }}
                          {% when 'gift' %}
                            {{ gift }}
                          {% when 'gem_sparkle' %}
                            {{ gem_sparkle }}
                          {% when 'feather' %}
                            {{ feather }}
                          {% when 'face_smile_2' %}
                            {{ face_smile_2 }}
                          {% when 'envelope' %}
                            {{ envelope }}
                          {% when 'earth' %}
                            {{ earth }}
                          {% when 'credit_card' %}
                            {{ credit_card }}
                          {% when 'bookmark' %}
