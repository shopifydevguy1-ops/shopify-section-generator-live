{%- liquid 
  assign padding_horizontal = section.settings.padding_horizontal
  assign padding_horizontal_mobile = section.settings.padding_horizontal_mobile
  assign padding_top = section.settings.padding_top
  assign padding_bottom = section.settings.padding_bottom
  assign border_color = section.settings.border_color
  assign border_thickness = section.settings.border_thickness
  assign margin_top = section.settings.margin_top
  assign margin_bottom = section.settings.margin_bottom
  assign margin_horizontal_mobile = section.settings.margin_horizontal_mobile
  assign margin_horizontal = section.settings.margin_horizontal
  assign background_color = section.settings.background_color
  assign background_gradient = section.settings.background_gradient
  assign full_width = section.settings.full_width
  assign content_width = section.settings.content_width
  assign lazy = section.settings.lazy
  assign section_radius = section.settings.section_radius 
  

  assign content_align = section.settings.content_align
  assign content_align_mobile = section.settings.content_align_mobile

  assign content_horizontal_align = ""
  if content_align == "center"
    assign content_horizontal_align = "center"
  elsif content_align == "right"
    assign content_horizontal_align = "end"
  else
    assign content_horizontal_align = "start"
  endif

  assign content_horizontal_align_mobile = ""
  if content_align_mobile == "center"
    assign content_horizontal_align_mobile = "center"
  elsif content_align_mobile == "right"
    assign content_horizontal_align_mobile = "end"
  else
    assign content_horizontal_align_mobile = "start"
  endif

  assign menus_mt = section.settings.menus_mt
  assign menus_mt_mobile = section.settings.menus_mt_mobile
  assign menus_gap = section.settings.menus_gap
  assign menus_gap_mobile = section.settings.menus_gap_mobile

  assign menu_gap = section.settings.menu_gap
  assign menu_gap_mobile = section.settings.menu_gap_mobile
  assign menu_mt = section.settings.menu_mt
  assign menu_mt_mobile = section.settings.menu_mt_mobile
  assign menu_align = section.settings.menu_align
  assign menu_align_mobile = section.settings.menu_align_mobile

  assign menu_heading_size = section.settings.menu_heading_size
  assign menu_heading_size_mobile = section.settings.menu_heading_size_mobile
  assign menu_heading_color = section.settings.menu_heading_color
  assign menu_heading_custom = section.settings.menu_heading_custom
  assign menu_heading_font = section.settings.menu_heading_font
  assign menu_heading_height = section.settings.menu_heading_height

  assign link_size = section.settings.link_size
  assign link_size_mobile = section.settings.link_size_mobile
  assign link_color = section.settings.link_color
  assign link_hover_color = section.settings.link_hover_color
  assign link_custom = section.settings.link_custom
  assign link_font = section.settings.link_font
  assign link_height = section.settings.link_height

  assign features_mt = section.settings.features_mt
  assign features_mt_mobile = section.settings.features_mt_mobile
  assign features_gap = section.settings.features_gap
  assign features_gap_mobile = section.settings.features_gap_mobile

  assign feature_icon_size = section.settings.feature_icon_size
  assign feature_icon_size_mobile = section.settings.feature_icon_size_mobile
  assign feature_icon_padding = section.settings.feature_icon_padding
  assign feature_icon_border_thickness = section.settings.feature_icon_border_thickness
  assign feature_icon_border_color = section.settings.feature_icon_border_color
  assign feature_icon_radius = section.settings.feature_icon_radius
  assign feature_icon_color = section.settings.feature_icon_color

  assign feature_title_size = section.settings.feature_title_size
  assign feature_title_size_mobile = section.settings.feature_title_size_mobile
  assign feature_title_color = section.settings.feature_title_color
  assign feature_title_custom = section.settings.feature_title_custom
  assign feature_title_font = section.settings.feature_title_font
  assign feature_title_height = section.settings.feature_title_height
  
  assign feature_text_size = section.settings.feature_text_size
  assign feature_text_size_mobile = section.settings.feature_text_size_mobile
  assign feature_text_color = section.settings.feature_text_color
  assign feature_text_custom = section.settings.feature_text_custom
  assign feature_text_font = section.settings.feature_text_font
  assign feature_text_height = section.settings.feature_text_height
  assign feature_text_mt = section.settings.feature_text_mt
  assign feature_text_mt_mobile = section.settings.feature_text_mt_mobile

  assign bottom_mt = section.settings.bottom_mt
  assign bottom_mt_mobile = section.settings.bottom_mt_mobile
  assign bottom_border_thickness = section.settings.bottom_border_thickness
  assign bottom_border_color = section.settings.bottom_border_color

  assign loading_attribute = 'eager'
  if lazy
    assign loading_attribute = 'lazy'
  endif
-%}

{%- style -%}
  {{ menu_heading_font | font_face: font_display: 'swap' }}
  {{ link_font | font_face: font_display: 'swap' }}
  {{ feature_title_font | font_face: font_display: 'swap' }}
  {{ feature_text_font | font_face: font_display: 'swap' }}

  .section-{{ section.id }} {
    border-top: solid {{ border_color }} {{ border_thickness }}px;
    border-bottom: solid {{ border_color }} {{ border_thickness }}px;
    margin-left: {{ margin_horizontal_mobile }}rem;
    margin-right: {{ margin_horizontal_mobile }}rem;
    margin-top: {{ margin_top | times: 0.75 | round: 0 }}px;
    margin-bottom: {{ margin_bottom | times: 0.75 | round: 0 }}px;
    border-radius: {{ section_radius | times: 0.6 | round: 0 }}px;
    overflow: hidden;
  }
  
  .section-{{ section.id }}-settings {
    margin: 0 auto;
    padding-top: {{ padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ padding_bottom | times: 0.75 | round: 0 }}px;
    padding-left: {{ padding_horizontal_mobile }}rem;
    padding-right: {{ padding_horizontal_mobile }}rem;
  }

  .footer-menu-heading-{{ section.id }} {
    margin: 0;
    text-align: {{ menu_align_mobile }};
    font-size: {{ menu_heading_size_mobile }}px;
    color: {{ menu_heading_color }};
    line-height: {{ menu_heading_height }}%;
    text-transform: unset;
    text-decoration: none;
    font-weight: 700;
  }

  .footer-top-menu-body-{{ section.id }} {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: {{ menus_gap_mobile }}px;
    margin-top: {{ menus_mt_mobile }}px;
  }

  .footer-bottom-menu-list-{{ section.id }} {
    flex-direction: row;
    justify-content: start;
  }

  .footer-top-menu-{{ section.id }} {
    flex: 1 1 0;
  }

  .footer-menu-list-{{ section.id }},
  .footer-info-text-{{ section.id }} {
    padding: 0px;
    margin: 0px;
    display: flex;
    flex-direction: column;
    gap: {{ menu_gap_mobile }}px;
    margin-top: {{ menu_mt_mobile }}px !important;
    list-style: none;
    flex-wrap: wrap;
  }

  .footer-menu-item-{{ section.id }} {
    line-height: 100%;
  }

  .footer-menu-link-{{ section.id }} {
    margin: 0;
    display: block;
    text-align: {{ menu_align_mobile }};
    font-size: {{ link_size_mobile }}px;
    color: {{ link_color }};
    line-height: {{ link_height }}%;
    text-transform: unset;
    text-decoration: none;
    font-weight: 400;
  }

  .footer-bottom-{{ section.id }} {
    margin-top: {{ bottom_mt_mobile }}px;
    padding-top: {{ bottom_mt_mobile }}px;
    border-top: {{ bottom_border_thickness }}px solid {{ bottom_border_color }};
  }

  .footer-bottom-menus-wrapper-{{ section.id }} {
    display: flex;
    gap: 20px;
    align-items: center;
    justify-content: space-between;
    flex-direction: column;
    width: 100%;
  }

  .footer-bottom-payments-wrapper-{{ section.id }} {
    display: flex;
    gap: 20px;
    align-items: center;
    justify-content: space-between;
    width: 100%;
  }

  .footer-features-{{ section.id }} {
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: {{ content_horizontal_align_mobile }};
    gap: {{ features_gap_mobile }}px;
    margin-top: {{ features_mt_mobile }}px;
    position: relative;
  }

  .footer-features-{{ section.id }}::after {
    content: "";
    position: absolute;
    top: -25px;
    left: 0;
    width: calc(100% + 100px);
    transform: translateX(-50px);
    height: {{ bottom_border_thickness }}px;
    background-color: {{ bottom_border_color }};
  }

  .footer-features-{{ section.id }}::before {
    content: "";
    position: absolute;
    bottom: -25px;
    left: 0;
    width: calc(100% + 100px);
    transform: translateX(-50px);
    height: {{ bottom_border_thickness }}px;
    background-color: {{ bottom_border_color }};
  }


  .footer-feature-{{ section.id }} {
    display: flex;
    gap: 16px;
    align-items: center;
    flex: 1 1 0;
    width: 100%;
  }

  .footer-feature-icon-{{ section.id }} {
    flex-shrink: 0;
    border: {{ feature_icon_border_thickness }}px solid {{ feature_icon_border_color }};
    padding: {{ feature_icon_padding }}px;    
    border-radius: {{ feature_icon_radius }}px;
    overflow: hidden;
  }

  .footer-feature-{{ section.id }}:hover .footer-feature-icon-{{ section.id }} img,
  .footer-feature-{{ section.id }}:hover .footer-feature-icon-{{ section.id }} svg {
    transform: scale(1.1);
    transition: all 0.4s ease 0s;
  }

  .footer-feature-icon-{{ section.id }} img,
  .footer-feature-icon-{{ section.id }} svg {
    display: block;
    width: {{ feature_icon_size_mobile }}px;
    height: {{ feature_icon_size_mobile }}px;
    object-fit: cover;
    transition: all 0.4s ease 0s;
  }
  
  .footer-feature-icon-{{ section.id }} svg path {
    fill: {{ feature_icon_color }};
  }

  .footer-feature-icon-{{ section.id }} svg circle {
    fill: {{ feature_icon_color }};
  }

  .footer-feature-icon-{{ section.id }} .refresh-{{ section.id }} path,
  .footer-feature-icon-{{ section.id }} .refresh-{{ section.id }} polyline{
    fill: none;
    stroke: {{ feature_icon_color }};
  }

  .footer-feature-title-{{ section.id }} {
    margin: 0px;
    font-size: {{ feature_title_size_mobile }}px;
    color: {{ feature_title_color }};
    line-height: {{ feature_title_height }}%;
    text-transform: unset;
    font-weight: 700;
  }

  .footer-feature-text-{{ section.id }} {
    margin-top: {{ feature_text_mt_mobile }}px;
  }

  .footer-feature-text-{{ section.id }} * {
    margin: 0px;
    font-size: {{ feature_text_size_mobile }}px;
    color: {{ feature_text_color }};
    line-height: {{ feature_text_height }}%;
    text-transform: unset;
    text-wrap: wrap;
    word-break: break-word;
  }

  @media(min-width: 1024px) {

    .section-{{ section.id }} {
      margin-left: {{ margin_horizontal }}rem;
      margin-right: {{ margin_horizontal }}rem;
      border-radius: {{ section_radius }}px;
      margin-top: {{ margin_top }}px;
      margin-bottom: {{ margin_bottom }}px;
    }
    
    .section-{{ section.id }}-settings {
      padding: 0 5rem;
      padding-top: {{ padding_top }}px;
      padding-bottom: {{ padding_bottom }}px;
      padding-left: {{ padding_horizontal }}rem;
      padding-right: {{ padding_horizontal }}rem;
    }

    .footer-top-{{ section.id }} {
      grid-template-columns: 1fr 1fr;
    }

    .footer-top-menu-body-{{ section.id }} {    
      display: flex;
      flex-wrap: nowrap;
      gap: {{ menus_gap }}px;
      margin-top: {{ menus_mt }}px;
    }

    .footer-top-menu-{{ section.id }} {
      flex: 1 1 0;
      width: 100%;
    }

    .footer-bottom-{{ section.id }} {
      margin-top: {{ bottom_mt }}px;
      padding-top: {{ bottom_mt }}px;     
    }

    .footer-menu-heading-{{ section.id }} {  
      font-size: {{ menu_heading_size }}px;
      text-align: {{ menu_align }};
    } 

    .footer-menu-link-{{ section.id }} {
      font-size: {{ link_size }}px;
      text-align: {{ menu_align }};
    } 

    .footer-menu-link-{{ section.id }}:hover {
      color: {{ link_hover_color }};
      transition: all 0.25s ease 0s;
    }

    .footer-menu-heading-{{ section.id }} {
      text-align: {{ menu_align }};
      font-size: {{ menu_heading_size }}px;
    }

    .footer-menu-list-{{ section.id }},
    .footer-info-text-{{ section.id }} {
      gap: {{ menu_gap }}px;
      margin-top: {{ menu_mt }}px !important;
    }

    .footer-menu-link-{{ section.id }} {    
      text-align: {{ menu_align }};
      font-size: {{ link_size }}px;
    }

    .footer-bottom-{{ section.id }} {
      grid-template-columns: 1fr 1fr;
      margin-top: {{ bottom_mt }}px;
    }

    .footer-features-{{ section.id }} {
      flex-direction: row;
      gap: {{ features_gap }}px;
      margin-top: {{ features_mt }}px;
      justify-content: {{ content_horizontal_align }};
    }

    .footer-feature-icon-{{ section.id }} img,
    .footer-feature-icon-{{ section.id }} svg {
      width: {{ feature_icon_size }}px;
      height: {{ feature_icon_size }}px;
    }

    .footer-bottom-menus-wrapper-{{ section.id }} {    
      flex-direction: row;
    }
    
    .footer-feature-title-{{ section.id }} {    
      font-size: {{ feature_title_size }}px;
    }

    .footer-feature-text-{{ section.id }} {
      margin-top: {{ feature_text_mt }}px;
    }

    .footer-feature-text-{{ section.id }} * {
      font-size: {{ feature_text_size }}px;     
    }

    .footer-features-{{ section.id }}::after {
      display: none;
    }
  
    .footer-features-{{ section.id }}::before {
       display: none;
    }
  }
  
{%- endstyle -%}

{% unless full_width %}
  <style>
    .section-{{ section.id }}-settings {
      max-width: {{ content_width }}px;
    }
  </style>
{% endunless %}

{% if margin_horizontal_mobile > 0 %}
  <style>
    .section-{{ section.id }} {
      border-left: solid {{ border_color }} {{ border_thickness }}px;
      border-right: solid {{ border_color }} {{ border_thickness }}px;
    }
    
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: 0px;
        border-right: 0px;
      }
    }
  </style>
{% endif %}

{% if margin_horizontal > 0 %}
  <style>
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: solid {{ border_color }} {{ border_thickness }}px;
        border-right: solid {{ border_color }} {{ border_thickness }}px;
      }
    }
  </style>
{% endif %}

{% if menu_heading_custom %}
  <style>
    .footer-menu-heading-{{ section.id }} {
      font-family: {{ menu_heading_font.family }}, {{ menu_heading_font.fallback_families }};
      font-weight: {{ menu_heading_font.weight }};
      font-style: {{ menu_heading_font.style }};
    }
  </style>
{% endif %}

{% if link_custom %}
  <style>
    .footer-menu-link-{{ section.id }} {
      font-family: {{ link_font.family }}, {{ link_font.fallback_families }};
      font-weight: {{ link_font.weight }};
      font-style: {{ link_font.style }};
    }
  </style>
{% endif %}

{% if feature_title_custom %}
  <style>
    .footer-feature-title-{{ section.id }} {
      font-family: {{ feature_title_font.family }}, {{ feature_title_font.fallback_families }};
      font-weight: {{ feature_title_font.weight }};
      font-style: {{ feature_title_font.style }};
    }
  </style>
{% endif %}

{% if feature_text_custom %}
  <style>
    .footer-feature-text-{{ section.id }} {
      font-family: {{ feature_text_font.family }}, {{ feature_text_font.fallback_families }};
      font-weight: {{ feature_text_font.weight }};
      font-style: {{ feature_text_font.style }};
    }
  </style>
{% endif %}

{% if menu_align_mobile == "center" %}
  <style>
    .footer-menu-heading-{{ section.id }} {
      justify-content: center;
    }
  </style>
{% elsif menu_align_mobile == "right" %}
  <style>
    .footer-menu-heading-{{ section.id }} {
      justify-content: end;
    }
  </style>
{% endif %}

{% if menu_align == "center" %}
  <style>
    @media(min-width: 1024px) {
      .footer-menu-heading-{{ section.id }} {
        justify-content: center;
      }
    }
  </style>
{% elsif menu_align == "right" %}
  <style>
    @media(min-width: 1024px) {
      .footer-menu-heading-{{ section.id }} {
        justify-content: end;
      }
    }
  </style> 
{% else %}
  <style>
    @media(min-width: 1024px) {
      .footer-menu-heading-{{ section.id }} {
        justify-content: start;
      }
    }
  </style>
{% endif %}

{% for block in section.blocks %}
  {% case block.type %}
    {% when 'heading' %}  
      <style>
        {{  block.settings.heading_font | font_face: font_display: 'swap' }}

        .footer-heading-{{ block.id }} {
          width: 100%;
          margin-top: {{ block.settings.heading_mt_mobile }}px;
          text-align: {{ content_align_mobile }};
        }
      
        .footer-heading-{{ block.id }} * {
          margin: 0;
          font-size: {{ block.settings.heading_size_mobile }}px;
          color: {{ block.settings.heading_color }};
          line-height: {{ block.settings.heading_height }}%;
          text-transform: unset;
          word-break: break-word;
          font-weight: 700;
        }

        .footer-heading-{{ block.id }} * strong {
          text-decoration: underline {{ block.settings.heading_border_color }};
        }

        @media(min-width: 1024px) {
          
          .footer-heading-{{ block.id }} {
            width: {{ block.settings.heading_width }}%;
            margin-top: {{ block.settings.heading_mt }}px;
            text-align: {{ content_align }};
          }
        
          .footer-heading-{{ block.id }} * {
            font-size: {{ block.settings.heading_size }}px;
          }
        }          
      </style>
        
      {% if block.settings.heading_custom %}
        <style>
          .footer-heading-{{ block.id }} * {
            font-family: {{ block.settings.heading_font.family }}, {{ block.settings.heading_font.fallback_families }};
            font-weight: {{ block.settings.heading_font.weight }};
            font-style: {{ block.settings.heading_font.style }};
          }
        </style>
      {% endif %}

      {% if content_align_mobile == 'right' %}
        <style>
          .footer-heading-{{ block.id }} {
            margin-left: auto;         
          }
        </style>
      {% elsif content_align_mobile == 'center' %}
        <style>
          .footer-heading-{{ block.id }} {
            margin-left: auto;
            margin-right: auto;           
          }
        </style>  
      {% elsif content_align_mobile == 'left' %}
        <style>
          .footer-heading-{{ block.id }} {
            margin-right: auto;          
          }
        </style>  
      {% endif %}
      
      {% if content_align == 'right' %}
        <style>
          @media(min-width: 1024px) {
            .footer-heading-{{ block.id }} {
              margin-left: auto;
              margin-right: 0;
            }
          }   
        </style>
      {% elsif content_align == 'center' %}
        <style>
          @media(min-width: 1024px) {
            .footer-heading-{{ block.id }} {
              margin-left: auto;
              margin-right: auto;
            }
          } 
        </style>  
      {% elsif content_align == 'left' %}
        <style>
          @media(min-width: 1024px) {
            .footer-heading-{{ block.id }} {
              margin-right: 0;
              margin-left: 0;
            }
          } 
        </style>  
      {% endif %}

    {% when 'text' %}   
      <style>
        {{  block.settings.text_font | font_face: font_display: 'swap' }}
        
        .footer-text-{{ block.id }} {
          width: 100%;
          margin-top: {{ block.settings.text_mt_mobile }}px;
          text-align: {{ content_align_mobile }};
          position: relative;
          z-index: 2;
        }
      
        .footer-text-{{ block.id }} * {
          margin: 0;
          font-size: {{ block.settings.text_size_mobile }}px;
          color: {{ block.settings.text_color }};
          line-height: {{ block.settings.text_height }}%;
          text-transform: unset;
          word-break: break-word;
          text-wrap: wrap;
        }

        @media(min-width: 1024px) {
          
          .footer-text-{{ block.id }} {
            width: {{ block.settings.text_width }}%;
            margin-top: {{ block.settings.text_mt }}px;
            text-align: {{  content_align }};
          }
      
          .footer-text-{{ block.id }} * {
            font-size: {{ block.settings.text_size }}px;
          }
          
        }
      </style>

      {% if block.settings.text_custom %}
        <style>
          .footer-text-{{ block.id }} * {
            font-family: {{ block.settings.text_font.family }}, {{ block.settings.text_font.fallback_families }};
            font-weight: {{ block.settings.text_font.weight }};
            font-style: {{ block.settings.text_font.style }};
          }
        </style>          
      {% endif %}  

      {% if content_align_mobile == 'right' %}
        <style>
          .footer-text-{{ block.id }} {
            margin-left: auto;         
          }
        </style>
      {% elsif content_align_mobile == 'center' %}
        <style>
          .footer-text-{{ block.id }} {
            margin-left: auto;
            margin-right: auto;           
          }
        </style>  
      {% elsif content_align_mobile == 'left' %}
        <style>
          .footer-text-{{ block.id }} {
            margin-right: auto;          
          }
        </style>  
      {% endif %}
      
      {% if content_align == 'right' %}
        <style>
          @media(min-width: 1024px) {
            .footer-text-{{ block.id }} {
              margin-left: auto;
              margin-right: 0;
            }
          }   
        </style>
      {% elsif content_align == 'center' %}
        <style>
          @media(min-width: 1024px) {
            .footer-text-{{ block.id }} {
              margin-left: auto;
              margin-right: auto;
            }
          } 
        </style>  
      {% elsif content_align == 'left' %}
        <style>
          @media(min-width: 1024px) {
            .footer-text-{{ block.id }} {
              margin-right: 0;
              margin-left: 0;
            }
          } 
        </style>  
      {% endif %}

    {% when 'copyright' %}
      <style>
        {{ block.settings.copyright_font | font_face: font_display: 'swap' }}

        .footer-copyright-{{ block.id }} {
          text-align: center;
          margin-top: {{ block.settings.copyright_mt_mobile }}px;
        }

        .footer-copyright-{{ block.id }} * {
          margin: 0;
          font-size: {{ block.settings.copyright_size_mobile }}px;
          color: {{ block.settings.copyright_color }};
          line-height: {{ block.settings.copyright_height }}%;
          text-transform: unset;
          text-decoration: none;
          word-break: break-word;
        }

        @media(min-width: 1024px) {
          .footer-copyright-{{ block.id }} {
            text-align: left;
            margin-top: {{ block.settings.copyright_mt }}px;
          }
        
          .footer-copyright-{{ block.id }} * {
            font-size: {{ block.settings.copyright_size }}px;
          }
        }
      </style>
      {% if block.settings.copyright_custom %}
        <style>
          .footer-copyright-{{ block.id }} * {
            font-family: {{ block.settings.copyright_font.family }}, {{ block.settings.copyright_font.fallback_families }};
            font-weight: {{ block.settings.copyright_font.weight }};
            font-style: {{ block.settings.copyright_font.style }};
          }
        </style>
      {% endif %}

    {% when 'payment' %}
      <style>
        .footer-payments-{{ block.id }} {
          margin: 0px;
          display: flex;
          align-items: center;
          justify-content: start;
          flex-wrap: wrap;
          gap: 8px;
          padding: 0px;
          margin-top: {{ block.settings.payment_icon_mt_mobile }}px;
        }

