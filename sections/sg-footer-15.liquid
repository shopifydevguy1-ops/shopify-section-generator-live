            font-style: {{ block.settings.link_font.style }};
          }
        </style>
      {% endif %}

    {% when "socials" %}
      <style>         
        .footer-social-list-{{ section.id }} {
          padding: 0px;
          margin: 0px;
          list-style: none;
          display: flex;
          flex-wrap: wrap;
          align-items: center;
          justify-content: center;
          gap: {{ block.settings.social_list_gap_mobile }}px;
