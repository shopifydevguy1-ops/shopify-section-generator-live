{%- liquid 
  assign padding_horizontal = section.settings.padding_horizontal
  assign padding_horizontal_mobile = section.settings.padding_horizontal_mobile
  assign padding_top = section.settings.padding_top
  assign padding_bottom = section.settings.padding_bottom
  assign border_color = section.settings.border_color
  assign border_thickness = section.settings.border_thickness
  assign margin_top = section.settings.margin_top
  assign margin_bottom = section.settings.margin_bottom
  assign margin_horizontal_mobile = section.settings.margin_horizontal_mobile
  assign margin_horizontal = section.settings.margin_horizontal
  assign background_color = section.settings.background_color
  assign background_gradient = section.settings.background_gradient
  assign full_width = section.settings.full_width
  assign content_width = section.settings.content_width
  assign lazy = section.settings.lazy
  assign section_radius = section.settings.section_radius 
  assign background_style = section.settings.background_style

  assign menus_mt = section.settings.menus_mt
  assign menus_mt_mobile = section.settings.menus_mt_mobile
  assign menus_gap = section.settings.menus_gap
  assign menus_gap_mobile = section.settings.menus_gap_mobile

  assign menu_gap = section.settings.menu_gap
  assign menu_gap_mobile = section.settings.menu_gap_mobile
  assign menu_mt = section.settings.menu_mt
  assign menu_mt_mobile = section.settings.menu_mt_mobile
  assign menu_align = section.settings.menu_align
  assign menu_align_mobile = section.settings.menu_align_mobile

  assign menu_heading_size = section.settings.menu_heading_size
  assign menu_heading_size_mobile = section.settings.menu_heading_size_mobile
  assign menu_heading_color = section.settings.menu_heading_color
  assign menu_heading_custom = section.settings.menu_heading_custom
  assign menu_heading_font = section.settings.menu_heading_font
  assign menu_heading_height = section.settings.menu_heading_height

  assign image_ratio = section.settings.image_ratio
  assign image_ratio_mobile = section.settings.image_ratio_mobile
  assign image_border_thickness = section.settings.image_border_thickness
  assign image_border_color = section.settings.image_border_color
  assign image_radius = section.settings.image_radius

  assign link_size = section.settings.link_size
  assign link_size_mobile = section.settings.link_size_mobile
  assign link_color = section.settings.link_color
  assign link_hover_color = section.settings.link_hover_color
  assign link_custom = section.settings.link_custom
  assign link_font = section.settings.link_font
  assign link_height = section.settings.link_height

  assign bottom_mt = section.settings.bottom_mt
  assign bottom_mt_mobile = section.settings.bottom_mt_mobile
  assign bottom_border_thickness = section.settings.bottom_border_thickness
  assign bottom_border_color = section.settings.bottom_border_color

  assign icon_color = section.settings.icon_color
  assign icon_bg_color = section.settings.icon_bg_color
  assign icon_size_mobile = section.settings.icon_size_mobile
  assign icon_radius_mobile = section.settings.icon_radius_mobile
  
  assign lazy_load_setting = 'lazy'
  unless lazy
    assign lazy_load_setting = 'eager'
  endunless
-%}

{%- style -%}
  {{ menu_heading_font | font_face: font_display: 'swap' }}
  {{ link_font | font_face: font_display: 'swap' }}

  .section-{{ section.id }} {
    border-top: solid {{ border_color }} {{ border_thickness }}px;
    border-bottom: solid {{ border_color }} {{ border_thickness }}px;
    margin-left: {{ margin_horizontal_mobile }}rem;
    margin-right: {{ margin_horizontal_mobile }}rem;
    margin-top: {{ margin_top | times: 0.75 | round: 0 }}px;
    margin-bottom: {{ margin_bottom | times: 0.75 | round: 0 }}px;
    border-radius: {{ section_radius | times: 0.6 | round: 0 }}px;
    overflow: hidden;
  }
  
  .section-{{ section.id }}-settings {
    margin: 0 auto;
    padding-top: {{ padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ padding_bottom | times: 0.75 | round: 0 }}px;
    padding-left: {{ padding_horizontal_mobile }}rem;
    padding-right: {{ padding_horizontal_mobile }}rem;
  }

  .footer-content-{{ section.id }} {
    display: flex;
    align-items: start;
    justify-content: space-between;
    flex-direction: column;
  }

  .footer-menu-heading-{{ section.id }} {
    margin: 0;
    text-align: {{ menu_align_mobile }};
    font-size: {{ menu_heading_size_mobile }}px;
    color: {{ menu_heading_color }};
    line-height: {{ menu_heading_height }}%;
    text-transform: unset;
    text-decoration: none;
    font-weight: 700;
  }

  .footer-top-menu-wrapper-{{ section.id }} {
    display: grid;
    grid-template-columns: 1fr;
    gap: 25px;
    margin-top: {{ menus_mt_mobile }}px;
    padding-top: {{ menus_mt_mobile }}px;
    border-top: {{ bottom_border_thickness }}px solid {{ bottom_border_color }};
  }

  .footer-top-menu-body-{{ section.id }} {
    display: grid;   
    grid-template-columns: 1fr;
  }

  .footer-newsletter-wrapper-{{ section.id }} {
    width: 100%;
  }

  .footer-bottom-menu-list-{{ section.id }} {
    display: grid !important;
    grid-template-columns: 1fr 1fr;
    align-items: center;
    gap: 20px;
    width: 100%;
    max-width: 100%;
  }

  .footer-top-menu-{{ section.id }} {
    flex: 1 1 0;
  }

  .footer-top-menu-body-desktop-{{ section.id }} {
    display: none;
  }

  .footer-top-menu-body-mobile-{{ section.id }} {
    display: flex;
    flex-direction: column;
    gap: {{ menus_gap_mobile }}px;
  }

  .footer-tab-toggle-{{ section.id }} {
    display: flex;
    align-items: center;
    justify-content: space-between;
    cursor: pointer;
    gap: 12px;
  }

  .footer-icon-{{ section.id }} {
    display: flex;
    align-items: center;
    justify-content: center;
    width: {{ icon_size_mobile }}px;
    height: {{ icon_size_mobile }}px;
    transition: all 0.25s ease 0s;
    padding: 8px;
    border-radius: {{ icon_radius_mobile }}px;
    background: {{ icon_bg_color }};
    box-sizing: content-box;
  }

  .footer-icon-{{ section.id }} svg {
    width: {{ icon_size_mobile }}px;
    height: {{ icon_size_mobile }}px;
    transition: all 0.25s ease 0s;
  }

  .footer-icon-{{ section.id }} svg path {
    stroke: {{ icon_color }};
  }

  .footer-icon-{{ section.id }} svg:first-child {
    display: block;
  }

  .footer-icon-{{ section.id }} svg:last-child {
    display: none;
  }

  .footer-icon-{{ section.id }}.active svg:first-child {
    display: none;
  }

  .footer-icon-{{ section.id }}.active svg:last-child {
    display: block;
  }

  .footer-menu-list-{{ section.id }},
  .footer-info-text-{{ section.id }} {
    padding: 0px;
    margin: 0px;
    display: flex;
    flex-direction: column;
    gap: {{ menu_gap_mobile }}px;
    list-style: none;
    flex-wrap: wrap;
  }

  .footer-menu-list-{{ section.id }} {
    margin-top: 0px !important;
  }

  .footer-height-cal-{{ section.id }} {
    visibility: hidden;
    opacity: 0;
    pointer-events: none;
    position: absolute;
    top: 0;
    left: 0;
    gap: {{ menu_gap_mobile }}px;
    margin-top: 0px !important;
  }

  .footer-menu-item-{{ section.id }} {
    line-height: 100%;
  }

  .footer-menu-link-{{ section.id }} {
    margin: 0;
    display: block;
    text-align: {{ menu_align_mobile }};
    font-size: {{ link_size_mobile }}px;
    color: {{ link_color }};
    line-height: {{ link_height }}%;
    text-transform: unset;
    text-decoration: none;
    font-weight: 400;
  }

  .footer-bottom-{{ section.id }} {
    display: flex;
    gap: 20px;
    align-items: center;
    justify-content: space-between;
    flex-direction: column;
    width: 100%;
    margin-top: {{ bottom_mt_mobile }}px;
    padding-top: {{ bottom_mt_mobile }}px;
    border-top: {{ bottom_border_thickness }}px solid {{ bottom_border_color }};
  }

  .footer-image-{{ section.id }} {
    border: {{ image_border_thickness }}px solid {{ image_border_color }};
    border-radius: {{ image_radius }}px;
    overflow: hidden;
  }

  .footer-image-{{ section.id }} img,
  .footer-image-{{ section.id }} svg {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .footer-image-{{ section.id }} svg {
    background-color: #AFAFAF;
  }

  .footer-top-images-{{ section.id }} {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 10px;
  }

  .footer-answer-{{ section.id }} {
    transition: all 0.2s ease-in-out;
    overflow: hidden;
  }

  @media(min-width: 1024px) {

    .section-{{ section.id }} {
      margin-left: {{ margin_horizontal }}rem;
      margin-right: {{ margin_horizontal }}rem;
      border-radius: {{ section_radius }}px;
      margin-top: {{ margin_top }}px;
      margin-bottom: {{ margin_bottom }}px;
    }
    
    .section-{{ section.id }}-settings {
      padding: 0 5rem;
      padding-top: {{ padding_top }}px;
      padding-bottom: {{ padding_bottom }}px;
      padding-left: {{ padding_horizontal }}rem;
      padding-right: {{ padding_horizontal }}rem;
    }

    .footer-content-{{ section.id }} {
      align-items: start;
      flex-direction: row;
    }

    .footer-bottom-menu-list-{{ section.id }} {
      display: flex !important;
      align-items: center;
      gap: 20px;
    }

    .footer-newsletter-wrapper-{{ section.id }} {
      width: fit-content;
    }

    .footer-top-menu-body-desktop-{{ section.id }} {
      display: flex;
    }

    .footer-top-menu-body-mobile-{{ section.id }} {
      display: none;
    }

    .footer-top-menu-wrapper-{{ section.id }} {
      grid-template-columns: 1fr 30%; 
      margin-top: {{ menus_mt }}px;
      padding-top: {{ menus_mt }}px;
      border-top: {{ bottom_border_thickness }}px solid {{ bottom_border_color }};
    }

    .footer-top-menu-body-{{ section.id }} {
      flex-wrap: nowrap;
      gap: {{ menus_gap }}px;
    }

    .footer-top-menu-{{ section.id }} {
      flex: 1 1 0;
      width: 100%;      
    }

    .footer-bottom-{{ section.id }} {
      margin-top: {{ bottom_mt }}px;
      padding-top: {{ bottom_mt }}px;     
    }

    .footer-menu-heading-{{ section.id }} {  
      font-size: {{ menu_heading_size }}px;
      text-align: {{ menu_align }};
    } 

    .footer-menu-link-{{ section.id }} {
      font-size: {{ link_size }}px;
      text-align: {{ menu_align }};
    } 

    .footer-menu-link-{{ section.id }}:hover {
      color: {{ link_hover_color }};
      transition: all 0.25s ease 0s;
    }

    .footer-menu-heading-{{ section.id }} {
      text-align: {{ menu_align }};
      font-size: {{ menu_heading_size }}px;
    }

    .footer-menu-list-{{ section.id }},
    .footer-info-text-{{ section.id }} {
      gap: {{ menu_gap }}px;
      margin-top: {{ menu_mt }}px !important;
    }

    .footer-menu-link-{{ section.id }} {    
      text-align: {{ menu_align }};
      font-size: {{ link_size }}px;
    }

    .footer-bottom-{{ section.id }} {
      flex-direction: row;
      margin-top: {{ bottom_mt }}px;
    }
  }
  
{%- endstyle -%}

{% unless full_width %}
  <style>
    .section-{{ section.id }}-settings {
      max-width: {{ content_width }}px;
    }
  </style>
{% endunless %}

{% if margin_horizontal_mobile > 0 %}
  <style>
    .section-{{ section.id }} {
      border-left: solid {{ border_color }} {{ border_thickness }}px;
      border-right: solid {{ border_color }} {{ border_thickness }}px;
    }
    
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: 0px;
        border-right: 0px;
      }
    }
  </style>
{% endif %}

{% if margin_horizontal > 0 %}
  <style>
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: solid {{ border_color }} {{ border_thickness }}px;
        border-right: solid {{ border_color }} {{ border_thickness }}px;
      }
    }
  </style>
{% endif %}

{% if menu_heading_custom %}
  <style>
    .footer-menu-heading-{{ section.id }} {
      font-family: {{ menu_heading_font.family }}, {{ menu_heading_font.fallback_families }};
      font-weight: {{ menu_heading_font.weight }};
      font-style: {{ menu_heading_font.style }};
    }
  </style>
{% endif %}

{% if link_custom %}
  <style>
    .footer-menu-link-{{ section.id }} {
      font-family: {{ link_font.family }}, {{ link_font.fallback_families }};
      font-weight: {{ link_font.weight }};
      font-style: {{ link_font.style }};
    }
  </style>
{% endif %}

{% if menu_align_mobile == "center" %}
  <style>
    .footer-menu-heading-{{ section.id }} {
      justify-content: center;
    }
  </style>
{% elsif menu_align_mobile == "right" %}
  <style>
    .footer-menu-heading-{{ section.id }} {
      justify-content: end;
    }
  </style>
{% endif %}

{% if menu_align == "center" %}
  <style>
    @media(min-width: 1024px) {
      .footer-menu-heading-{{ section.id }} {
        justify-content: center;
      }
    }
  </style>
{% elsif menu_align == "right" %}
  <style>
    @media(min-width: 1024px) {
      .footer-menu-heading-{{ section.id }} {
        justify-content: end;
      }
    }
  </style> 
{% else %}
  <style>
    @media(min-width: 1024px) {
      .footer-menu-heading-{{ section.id }} {
        justify-content: start;
      }
    }
  </style>
{% endif %}



{% if image_ratio_mobile == "portrait" %}
  <style>
    .footer-image-{{ section.id }} {
      aspect-ratio: 9.6/13;
    }
  </style>
{% elsif image_ratio_mobile == "landscape" %}
  <style>
  .footer-image-{{ section.id }} {
    aspect-ratio: 12/8;
  }
  </style>
{% elsif image_ratio_mobile == "square" %}
  <style>
    .footer-image-{{ section.id }} {
      aspect-ratio: 12/12;
    }
  </style>
{% else %}
  <style>
    .footer-image-{{ section.id }} {
      aspect-ratio: auto;
    }
  </style>
{% endif %}

{% if image_ratio == "portrait" %}
  <style>
    @media (min-width: 1024px) {
      .footer-image-{{ section.id }} {
        aspect-ratio: 9.6/13;
      }
    }
  </style>
{% elsif image_ratio == "landscape" %}
  <style>
    @media (min-width: 1024px) {
      .footer-image-{{ section.id }} {
        aspect-ratio: 12/8;
      }
    }
  </style>
{% elsif image_ratio == "square" %}
  <style>
    @media (min-width: 1024px) {
      .footer-image-{{ section.id }} {
        aspect-ratio: 12/12;
      }
    }
  </style>
{% else %}
  <style>
    @media (min-width: 1024px) {
      .footer-image-{{ section.id }} {
        aspect-ratio: auto;
      }
    }
  </style>
{% endif %}

{% for block in section.blocks %}
  {% case block.type %}
    {% when 'heading' %}  
      <style>
        {{  block.settings.heading_font | font_face: font_display: 'swap' }}

        .footer-heading-{{ block.id }} {
          width: 100%;
          margin-top: {{ block.settings.heading_mt_mobile }}px;
           text-align: {{ block.settings.heading_align_mobile }};
        }
      
        .footer-heading-{{ block.id }} * {
          margin: 0;
          font-size: {{ block.settings.heading_size_mobile }}px;
          color: {{ block.settings.heading_color }};
          line-height: {{ block.settings.heading_height }}%;
          text-transform: unset;
          word-break: break-word;
        }

        .footer-heading-{{ block.id }} * strong {
          text-decoration: underline {{ block.settings.heading_border_color }};
        }

        @media(min-width: 1024px) {
          
          .footer-heading-{{ block.id }} {
            width: {{ block.settings.heading_width }}%;
            text-align: {{ block.settings.heading_align }};
            margin-top: {{ block.settings.heading_mt }}px;
          }
        
          .footer-heading-{{ block.id }} * {
            font-size: {{ block.settings.heading_size }}px;
          }
        }          
      </style>
        
      {% if block.settings.heading_custom %}
        <style>
          .footer-heading-{{ block.id }} * {
            font-family: {{ block.settings.heading_font.family }}, {{ block.settings.heading_font.fallback_families }};
            font-weight: {{ block.settings.heading_font.weight }};
            font-style: {{ block.settings.heading_font.style }};
          }
        </style>
      {% endif %}

    {% when 'copyright' %}
      <style>
        {{ block.settings.copyright_font | font_face: font_display: 'swap' }}

        .footer-copyright-{{ block.id }} {
          width: 100%;
          text-align: {{ block.settings.copyright_align_mobile }};
        }

        .footer-copyright-{{ block.id }} * {
          margin: 0;
          font-size: {{ block.settings.copyright_size_mobile }}px;
          color: {{ block.settings.copyright_color }};
          line-height: {{ block.settings.copyright_height }}%;
          text-transform: unset;
          text-decoration: none;
          word-break: break-word;
        }

        @media(min-width: 1024px) {
          .footer-copyright-{{ block.id }} {
            text-align: {{ block.settings.copyright_align }};
          }
        
          .footer-copyright-{{ block.id }} * {
            font-size: {{ block.settings.copyright_size }}px;
          }
        }
      </style>
      {% if block.settings.copyright_custom %}
        <style>
          .footer-copyright-{{ block.id }} * {
            font-family: {{ block.settings.copyright_font.family }}, {{ block.settings.copyright_font.fallback_families }};
            font-weight: {{ block.settings.copyright_font.weight }};
            font-style: {{ block.settings.copyright_font.style }};
          }
        </style>
      {% endif %}

    {% when 'newsletter' %}    
      <style>     
        {{  block.settings.input_font | font_face: font_display: 'swap' }}
        {{  block.settings.submit_font | font_face: font_display: 'swap' }}
        {{  block.settings.thanks_text_font | font_face: font_display: 'swap' }}       

        .footer-newsletter-row-{{ block.id }} {
          margin-top: 20px;
          position: relative;
          display: flex;
          align-items: stretch;
          gap: 16px;
          padding-left: 15px;
          max-width: 100%;
          margin-top: {{ block.settings.form_mt_mobile }}px;
          background-color: {{ block.settings.input_bg_color }};        
          border-radius: {{ block.settings.input_radius }}px;
          border: {{ block.settings.input_border_thickness }}px solid {{ block.settings.input_border_color }};
        }
      
        .footer-newsletter-input-{{ block.id }} {
          width: 100%;
          margin: 0;
          box-shadow: none;
          border: 0px;
          font-family: inherit;
          font-size: {{ block.settings.input_size_mobile }}px;
          color: {{ block.settings.input_color }};
          line-height: {{ block.settings.input_height }}%;
          text-transform: unset;
          border-radius: {{ block.settings.input_radius }}px;
          background-color: transparent;         
          padding: {{ block.settings.input_padding_vertical_mobile }}px {{ block.settings.input_padding_horizontal_mobile }}px;
        }
      
        .footer-newsletter-input-{{ block.id }}::placeholder {
          color: {{ block.settings.input_placeholder_color }};
        }

        input:-webkit-autofill,
        input:-webkit-autofill:hover,
        input:-webkit-autofill:focus,
        input:-webkit-autofill:active {
          -webkit-text-fill-color: {{ block.settings.input_color }} !important;
          caret-color: transparent !important;
          border-radius: 0;
          -webkit-box-shadow: 0 0 0px 1000px {{ block.settings.input_bg_color }} inset !important; 
        }
        
        .footer-newsletter-input-{{ block.id }}:focus {
          outline: none;
        }
      
        .footer-newsletter-submit-{{ block.id }} {
          margin-top: 0px;
          position: static !important;
          max-width: 100%;
          display: flex;
          align-items: center;
          justify-content: center;
          font-family: inherit;
          font-size: {{ block.settings.submit_size_mobile }}px;
          color: {{ block.settings.submit_color }};
          line-height: {{ block.settings.submit_height }}%;
          text-align: center;
          text-transform: unset;
          text-decoration: none;
          border-radius: {{ block.settings.submit_radius }}px;
          padding: {{ block.settings.submit_padding_vertical_mobile }}px {{ block.settings.submit_padding_horizontal_mobile }}px;         
          border: 0px;
          border: {{ block.settings.submit_border_thickness }}px solid {{ block.settings.submit_border_color }} ;
          background-color: transparent;
          background-color: {{ block.settings.submit_bg_color }};
          transition: all 0.25s ease 0s;
          box-shadow: none;
          cursor: pointer;
        }
      
        .footer-newsletter-submit-{{ block.id }}:hover {
          color: {{ block.settings.submit_hover_color }};
          border: {{ block.settings.submit_border_thickness }}px solid {{ block.settings.submit_border_hover_color }};
          background-color: {{ block.settings.submit_bg_hover_color }};
          transition: all 0.25s ease 0s;
        }

       .footer-newsletter-submit-{{ block.id }}[disabled="true"] {
         opacity: 0.6;
       }

       .footer-newsletter-checkbox-{{ block.id }} {
         opacity: 0;
         z-index: -100;
         position: absolute;
       }

       .footer-newsletter-thanks-text-{{ block.id }} {
          display: none;
          margin: 0px;
          width: 100%;
          text-align: {{ block.settings.thanks_text_align_mobile }};
          margin-top: {{ block.settings.thanks_text_mt_mobile }}px;
          font-size: {{ block.settings.thanks_text_size_mobile }}px;
          color: {{ block.settings.thanks_text_color }};
          line-height: {{ block.settings.thanks_text_height }}%;
          text-transform: unset;
       }

       .footer-newsletter-thanks-text-{{ block.id }}.active {
         display: block;
       }

        @media(min-width: 1024px) {      
          .footer-newsletter-row-{{ block.id }} {     
            max-width: fit-content;    
            margin-top: {{ block.settings.form_mt }}px;
          }

          .footer-newsletter-input-{{ block.id }} {
            width: {{ block.settings.input_width }}px;         
            font-size: {{ block.settings.input_size }}px;
          }
      
          .footer-newsletter-submit-{{ block.id }} {
            padding: {{ block.settings.submit_padding_vertical }}px {{ block.settings.submit_padding_horizontal }}px;
            font-size: {{ block.settings.submit_size }}px;
          }

           .footer-newsletter-thanks-text-{{ block.id }} {
            text-align: {{ block.settings.thanks_text_align }};
             margin-top: {{ block.settings.thanks_text_mt }}px;
             font-size: {{ block.settings.thanks_text_size }}px;
           }
        }
      </style>

      {% if block.settings.input_custom %}
        <style>
          .footer-newsletter-input-{{ block.id }} {
            font-family: {{ block.settings.input_font.family }}, {{ block.settings.input_font.fallback_families }};
            font-weight: {{ block.settings.input_font.weight }};
            font-style: {{ block.settings.input_font.style }};
          }
        </style>
      {% endif %}
      
      {% if block.settings.submit_custom %}
        <style>
          .footer-newsletter-submit-{{ block.id }} {
            font-family: {{ block.settings.submit_font.family }}, {{ block.settings.submit_font.fallback_families }};
            font-weight: {{ block.settings.submit_font.weight }};
            font-style: {{ block.settings.submit_font.style }};
          }
        </style>
      {% endif %}

      {% if block.settings.use_shadow %}
        <style>
          .footer-newsletter-row-{{ block.id }} {          
            box-shadow: 0 1px 2px {{ block.settings.input_shadow_color | hex_to_rgba: 0.8 }},0 1px 3px {{ block.settings.input_shadow_color | hex_to_rgba: 0.1 }};             
          }
        </style>
      {% endif %}

      {% if block.settings.thanks_text_custom %}
        <style>
          .footer-newsletter-thanks-text-{{ block.id }} {
            font-family: {{ block.settings.thanks_text_font.family }}, {{ block.settings.thanks_text_font.fallback_families }};
            font-weight: {{ block.settings.thanks_text_font.weight }};
            font-style: {{ block.settings.thanks_text_font.style }};
          }
        </style>
      {% endif %}
    

    {% when 'bottom_menu' %}
      {% liquid
       assign menu_horizontal_align = ""
        if block.settings.menu_align == "center"
          assign menu_horizontal_align = "center"
        elsif block.settings.menu_align == "right"
          assign menu_horizontal_align = "end"
        else
          assign menu_horizontal_align = "start"
        endif

        assign menu_horizontal_align_mobile = ""
        if block.settings.menu_align_mobile == "center"
          assign menu_horizontal_align_mobile = "center"
        elsif block.settings.menu_align_mobile == "right"
          assign menu_horizontal_align_mobile = "end"
        else
          assign menu_horizontal_align_mobile = "start"
        endif
      %}

      <style>
        {{ block.settings.link_font | font_face: font_display: 'swap' }}

        .footer-menu-list-{{ block.id }} {
          padding: 0;
          margin: 0;
          display: flex;
          align-items: center;
          gap: 20px;
          flex-wrap: wrap;
          list-style-type: none;
          justify-content: {{ menu_horizontal_align_mobile }};
        }

        .footer-bottom-menu-link-{{ block.id }} {
          margin: 0;
          display: block;    
          text-align: {{ block.settings.menu_align_mobile }};   
          font-size: {{ block.settings.link_size_mobile }}px;
          color: {{ block.settings.link_color }};
          line-height: {{ block.settings.link_height }}%;
          text-transform: unset;
          text-decoration: none;
        }

        @media(min-width: 1024px) {
          .footer-bottom-menu-link-{{ block.id }} {          
            font-size: {{ block.settings.link_size }}px;
            text-align: {{ block.settings.menu_align }};   
          }

          .footer-menu-list-{{ block.id }} {
            justify-content: {{ menu_horizontal_align }};
          }
        }
      </style>

      {% if block.settings.link_custom %}
        <style>
          .footer-bottom-menu-link-{{ block.id }} {
            font-family: {{ block.settings.link_font.family }}, {{ block.settings.link_font.fallback_families }};
            font-weight: {{ block.settings.link_font.weight }};
            font-style: {{ block.settings.link_font.style }};
          }
        </style>
      {% endif %}

    {% when "socials" %}
      {% liquid
       assign socials_horizontal_align = ""
        if block.settings.socials_align == "center"
          assign socials_horizontal_align = "center"
        elsif block.settings.socials_align == "right"
          assign socials_horizontal_align = "end"
        else
          assign socials_horizontal_align = "start"
        endif

        assign socials_horizontal_align_mobile = ""
        if block.settings.socials_align_mobile == "center"
          assign socials_horizontal_align_mobile = "center"
        elsif block.settings.socials_align_mobile == "right"
          assign socials_horizontal_align_mobile = "end"
        else
          assign socials_horizontal_align_mobile = "start"
        endif
      %}
      <style>    
        {{  block.settings.heading_font | font_face: font_display: 'swap' }}

        .footer-social-heading-{{ block.id }} {
          width: 100%;
          margin-top: {{ block.settings.heading_mt_mobile }}px;
          text-align: {{ block.settings.socials_align_mobile }};
        }
      
        .footer-social-heading-{{ block.id }} * {
          margin: 0;
          font-size: {{ block.settings.heading_size_mobile }}px;
          color: {{ block.settings.heading_color }};
          line-height: {{ block.settings.heading_height }}%;
          text-transform: unset;
          word-break: break-word;
        }

        .footer-social-list-{{ section.id }} {
          padding: 0px;
          margin: 0px;
          list-style: none;
          display: flex;
          flex-wrap: wrap;
          align-items: center;
          justify-content: {{ socials_horizontal_align_mobile }};
          margin-top: 20px;
          gap: {{ block.settings.social_list_gap_mobile }}px;
        }
      
        .footer-social-link-{{ section.id }} {
          padding: {{ block.settings.social_icon_size_mobile | times: 0.5 }}px;
          box-sizing: content-box;
          background-color: {{ block.settings.social_icon_bg_color }};
          border-radius: {{ block.settings.social_icon_radius }}px;
          transition: all 0.25s ease 0s;
          display: block;       
          width: {{ block.settings.social_icon_size_mobile }}px;
          height: {{ block.settings.social_icon_size_mobile }}px;
        }
      
        .footer-social-link-{{ section.id }} svg {
          display: block;
          width: 100%;
          height: 100%;
          object-fit: contain;
        }
      
        .footer-social-link-{{ section.id }} svg path {
          fill: {{ block.settings.social_icon_color }};
          transition: all 0.25s ease 0s;
        }
  
        @media(min-width: 1024px) {  
          .footer-social-heading-{{ block.id }} {
            width: 100%;
            margin-top: {{ block.settings.heading_mt }}px;
            text-align: {{ block.settings.socials_align }};
          }
        
          .footer-social-heading-{{ block.id }} * {
            font-size: {{ block.settings.heading_size }}px;
          }

          .footer-social-list-{{ section.id }} {
            justify-content: {{ socials_horizontal_align }};
            gap: {{ block.settings.social_list_gap }}px;
          }
      
          .footer-social-link-{{ section.id }} {
            padding: {{ block.settings.social_icon_size | times: 0.5 }}px;
            width: {{ block.settings.social_icon_size }}px;
            height: {{ block.settings.social_icon_size }}px;
          }

          .footer-social-item-{{ section.id }}:hover .footer-social-link-{{ section.id }} {
            background-color: {{ block.settings.social_icon_bg_hover_color }};
            transition: all 0.25s ease 0s;
          }
  
          .footer-social-item-{{ section.id }}:hover .footer-social-link-{{ section.id }} svg path {
            fill: {{ block.settings.social_icon_hover_color }};
            transition: all 0.25s ease 0s;
          } 
          
        }
      </style>

      {% if block.settings.heading_custom %}
        <style>
          .footer-social-heading-{{ block.id }} * {
            font-family: {{ block.settings.heading_font.family }}, {{ block.settings.heading_font.fallback_families }};
            font-weight: {{ block.settings.heading_font.weight }};
            font-style: {{ block.settings.heading_font.style }};
          }
        </style>
      {% endif %}

    {% when 'scrolling_text' %}
      <style>
        {{ block.settings.scrolling_text_font | font_face: font_display: 'swap' }}

        .footer-scrolling-wrap-{{ block.id }} {          
          display: flex;    
          flex-wrap: nowrap;
          z-index: 0;        
          width: 200vw;   
          background-attachment: scroll !important;        
          margin-top: {{ block.settings.scrolling_mt_mobile }}px;
          background-color: {{ block.settings.scrolling_bg_color }};
        }

        .footer-scrolling-list-{{ block.id }} {
          display: -webkit-box;
          display: -ms-flexbox;
          display: flex;
          align-items: center;
          white-space: nowrap;
          background-attachment: scroll !important;
          -webkit-animation: ticker{{ block.id | replace: '-', '' }} {{ block.settings.scrolling_velocity_mobile }}s infinite linear;
          animation: ticker{{ block.id | replace: '-', '' }} {{ block.settings.scrolling_velocity_mobile }}s infinite linear;
          flex-shrink: 0;
        }

        .footer-scrolling-{{ block.id }} {
          margin: 0;
          font-size: {{ block.settings.scrolling_text_size_mobile }}px;
          color: {{ block.settings.scrolling_text_color }};
          line-height: {{ block.settings.scrolling_text_height }}%;
          margin-right: {{ block.settings.scrolling_gap_mobile }}px;
          text-transform: unset;
          word-wrap: break-word;
          background-attachment: scroll !important;
          position: relative;
          flex-shrink: 0;
        }  

        .footer-scrolling-dot-{{ block.id }} {
          display: block !important;
          margin-right: {{ block.settings.scrolling_gap_mobile }}px;
          width: {{ block.settings.scrolling_text_size_mobile | times: 0.4 }}px;
          height: {{ block.settings.scrolling_text_size_mobile | times: 0.4 }}px;
          border-radius: 100%;
          background-color: {{ block.settings.scrolling_dot_color }};
        }

        @media (min-width: 1024px) {               
          .footer-scrolling-wrap-{{ block.id }} {          
            margin-top: {{ block.settings.scrolling_mt }}px;
          }

          .footer-scrolling-list-{{ block.id }} {
            -webkit-animation: ticker{{ block.id | replace: '-', '' }} {{ block.settings.scrolling_velocity }}s infinite linear;
            animation: ticker{{ block.id | replace: '-', '' }} {{ block.settings.scrolling_velocity }}s infinite linear;        
          }

          .footer-scrolling-{{ block.id }} {
            font-size: {{ block.settings.scrolling_text_size }}px;
            margin-right: {{ block.settings.scrolling_gap }}px;
          }  

          .footer-scrolling-dot-{{ block.id }} {
            margin-right: {{ block.settings.scrolling_gap }}px;
            width: {{ block.settings.scrolling_text_size | times: 0.4 }}px;
            height: {{ block.settings.scrolling_text_size | times: 0.4 }}px;
          }

        }
      </style>
    
      {% if block.settings.scrolling_text_custom %}
        <style>
          .footer-scrolling-{{ block.id }} {
            font-family: {{ block.settings.scrolling_text_font.family }}, {{ block.settings.scrolling_text_font.fallback_families }};
            font-weight: {{ block.settings.scrolling_text_font.weight }};
            font-style: {{ block.settings.scrolling_text_font.style }};
          }
        </style>
      {% endif %}

      {% if block.settings.reverse_direction %}
        <style>          
          @keyframes ticker{{ block.id | replace: '-', '' }} {
            0% {
              transform: translateX(-100%);
            }
            100% {
              transform: translateX(0%);
            }
          }
        </style>
      {% else %}
        <style>      
          @keyframes ticker{{ block.id | replace: '-', '' }} {
            0% {
              transform: translateX(0%);
            }
            100% {
              transform: translateX(-100%);
            }
          }
        </style>
      {% endif %}
      
      {% if block.settings.pause_hover %}
        <style>
          .footer-scrolling-wrap-{{ block.id }}:hover .footer-scrolling-list-{{ block.id }} {
            animation-play-state: paused;
          }
        </style>
      {% endif %}
  {% endcase %}
{% endfor %}

