<script>
  function initShapes() {
    const slider = new Swiper(".shapes-slider-{{ section.id }}", {
      speed: 300,
      spaceBetween: 20,
      loop: true,
      centeredSlides: true,
      spaceBetween: {{ slider_gap_mobile }},
      slidesPerView: {{ slider_view_mobile }}
    });
  }

  document.addEventListener('DOMContentLoaded', initShapes);
  
  if (Shopify.designMode) {
     document.addEventListener('shopify:section:unload', initShapes);
     document.addEventListener('shopify:section:load', initShapes);
  }
</script>

