<link
  rel="stylesheet"
  href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css"
/>


{%- liquid 
  assign padding_horizontal = section.settings.padding_horizontal
  assign padding_horizontal_mobile = section.settings.padding_horizontal_mobile
  assign padding_top = section.settings.padding_top
  assign padding_bottom = section.settings.padding_bottom
  assign border_color = section.settings.border_color
  assign border_thickness = section.settings.border_thickness
  assign margin_top = section.settings.margin_top
  assign margin_bottom = section.settings.margin_bottom
  assign background_color = section.settings.background_color
  assign background_gradient = section.settings.background_gradient
  assign full_width = section.settings.full_width
  assign content_width = section.settings.content_width
  assign lazy = section.settings.lazy

  assign heading = section.settings.heading
  assign heading_size = section.settings.heading_size
  assign heading_size_mobile = section.settings.heading_size_mobile
  assign heading_color = section.settings.heading_color
  assign heading_custom = section.settings.heading_custom
  assign heading_font = section.settings.heading_font
  assign heading_height = section.settings.heading_height
  assign heading_align = section.settings.heading_align
  assign heading_align_mobile = section.settings.heading_align_mobile

  assign body_gap = section.settings.body_gap
  assign body_gap_mobile = section.settings.body_gap_mobile
  assign body_mt = section.settings.body_mt

  assign use_slider = section.settings.use_slider
  assign slider_gap_mobile = section.settings.slider_gap_mobile
  assign slider_view_mobile = section.settings.slider_view_mobile

  assign text_color = section.settings.text_color
  assign text_custom = section.settings.text_custom
  assign text_font = section.settings.text_font
  assign text_height = section.settings.text_height
-%}

{%- style -%}

  {{  heading_font | font_face: font_display: 'swap' }}
  {{  text_font | font_face: font_display: 'swap' }}
  
  .section-{{ section.id }} {
    border-top: solid {{ border_color }} {{ border_thickness }}px;
    border-bottom: solid {{ border_color }} {{ border_thickness }}px;
    margin-top: {{ margin_top | times: 0.75 | round: 0 }}px;
    margin-bottom: {{ margin_bottom | times: 0.75 | round: 0 }}px;
  }
  
  .section-{{ section.id }}-settings {
    margin: 0 auto;
    padding-top: {{ padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ padding_bottom | times: 0.75 | round: 0 }}px;
    padding-left: {{ padding_horizontal_mobile }}rem;
    padding-right: {{ padding_horizontal_mobile }}rem;
  }

  .shapes-heading-{{ section.id }} {
    width: 100%;
    text-align: {{ heading_align_mobile }};
    padding-left: {{ padding_horizontal_mobile }}rem;
    padding-right: {{ padding_horizontal_mobile }}rem;
  }

  .shapes-heading-{{ section.id }} * {
    margin: 0;
    font-size: {{ heading_size_mobile }}px;
    color: {{ heading_color }};
    line-height: {{ heading_height }}%;
    text-transform: unset;
  }

  .shapes-body-{{ section.id }} {
    margin-top: {{ body_mt | times: 0.75 | round: 0 }}px;
    gap: {{ body_gap_mobile }}px;
  }

  .shapes-slider-{{ section.id }} {
    margin-top: {{ body_mt | times: 0.75 | round: 0 }}px;
  }

  .shapes-slider-{{ section.id }} .swiper-wrapper {
    align-items: center;
  }

  .shapes-item-{{ section.id }} {
    position: relative;
    box-sizing: border-box !important;
    overflow: hidden;
  }

  .shapes-figure-{{ section.id }} {
    display: block !important;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
    overflow: hidden;
  }

  .shapes-figure-{{ section.id }} svg,
  .shapes-figure-{{ section.id }} img {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .shapes-content-{{ section.id }} {
    position: relative;
    z-index: 2;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 40px;
  }

  .shapes-content-{{ section.id }} {
    text-align: center;
    height: 100%;
    width: 100%;
  }

  .shapes-content-{{ section.id }} * {
    margin: 0;
    line-height: {{ text_height }}%;
    text-transform: unset;
  }

  .figure-third-{{ section.id }} {
    position: relative;
    stroke: rgb(30, 114, 220);
    stroke-width: 2px;
    z-index: 1;
  }

  .figure-third-{{ section.id }} foreignObject {
    display: initial;
  }

  .figure-third-{{ section.id }} foreignObject > div {
    width: 100%;
    height: 100%;
  }

  .figure-third-{{ section.id }} foreignObject > div img {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
    vertical-align: bottom;
  }

  .figure-third-{{ section.id }} > g {
    display: none;
  }

  .hide-content-{{ section.id }} {
    opacity: 0;
    visibility: hidden;
    z-index: -1;
  }

  .clip-path-{{ section.id }} {
    display: none !important;
  }
  
  @media(min-width: 1024px) {

    .section-{{ section.id }} {
      margin-top: {{ margin_top }}px;
      margin-bottom: {{ margin_bottom }}px;
    }
    
    .section-{{ section.id }}-settings {
      padding: 0 5rem;
      padding-top: {{ padding_top }}px;
      padding-bottom: {{ padding_bottom }}px;
      padding-left: {{ padding_horizontal }}rem !important;
      padding-right: {{ padding_horizontal }}rem !important;
    }

    .shapes-heading-{{ section.id }} {
      text-align: {{ heading_align }};
      padding: 0px;
    }

    .shapes-heading-{{ section.id }} * {
      font-size: {{ heading_size }}px;
    }

    .shapes-body-{{ section.id }} {
      margin-top: {{ body_mt }}px;
      display: grid !important;
      align-items: stretch;
      grid-auto-rows: min-content;
      grid-template-columns: repeat(12, minmax(0, 1fr));
      gap: {{ body_gap }}px;
    }

    .shapes-slider-{{ section.id }} {
      display: none !important;
    }
    
    .shapes-content-{{ section.id }} {
      padding: 48px !important;
    }

  }
  
{%- endstyle -%}

{% unless full_width %}
  <style>
    .section-{{ section.id }}-settings {
      max-width: {{ content_width }}rem;
    }
  </style>
{% endunless %}

{% if heading_custom %}
  <style>
    .shapes-heading-{{ section.id }} * {
      font-family: {{ heading_font.family }}, {{ heading_font.fallback_families }};
      font-weight: {{ heading_font.weight }};
      font-style: {{ heading_font.style }};
    }
  </style>
{% endif %}

{% if text_custom %}
  <style>
    .shapes-content-{{ section.id }} * {
      font-family: {{ text_font.family }}, {{ text_font.fallback_families }};
      font-weight: {{ text_font.weight }};
      font-style: {{ text_font.style }};
    }
  </style>
{% endif %}

{% if use_slider %}
  <style>
    .section-{{ section.id }}-settings {
      padding-left: 0px;
      padding-right: 0px;
    }
    
    .shapes-slider-{{ section.id }} {
      display: block;
    }

    .shapes-body-{{ section.id }} {
      display: none;
    }
  </style>
{% else %}
  <style>
    .shapes-slider-{{ section.id }} {
      display: none;
    }

    .shapes-body-{{ section.id }} {
      display: grid;
    }
  </style>
{% endif %}

{% for block in section.blocks %}
  <style>

    .shapes-content-{{ block.id }} * {
      font-size: {{ block.settings.text_size_mobile }}px;
    }

    @media(min-width: 1024px) {
      .shapes-item-{{ block.id }} {
        grid-row: span {{ block.settings.row_width }} / span {{ block.settings.row_width }};
        grid-column: span {{ block.settings.column_width }} / span {{ block.settings.column_width }};
      } 

      .shapes-content-{{ block.id }} * {
        font-size: {{ block.settings.text_size }}px;
      }
    }

    .shapes-item-{{ block.id }} img {
      opacity: {{ block.settings.image_opacity | times: 100 }}%;
    }

    .shapes-item-{{ block.id }} .shapes-content-{{ section.id }} * {
      color: {{ block.settings.text_color }};
    }
  </style>

  {% if block.settings.shape_type == "landscape" %}
    <style>
      .shapes-item-{{ block.id }} {
        aspect-ratio: 12 / 7;
        border-radius: {{ block.settings.radius }}px;
        border: {{ block.settings.border_thickness }}px solid {{ block.settings.border_color }};
      }

      @media(min-width: 1024px) {
        .shapes-item-{{ block.id }} {
          aspect-ratio: 1 / 0.3;
        }
      }
    </style>
  {% elsif block.settings.shape_type == "portrait" %}
    <style>
      .shapes-item-{{ block.id }} {
        aspect-ratio: 9.6/13;
        border-radius: {{ block.settings.radius }}px;
        border: {{ block.settings.border_thickness }}px solid {{ block.settings.border_color }};
      }
    </style>
  {% elsif block.settings.shape_type == "square" %}
    <style>
      .shapes-item-{{ block.id }} {
        aspect-ratio: 12/12;
        border-radius: {{ block.settings.radius }}px;
        border: {{ block.settings.border_thickness }}px solid {{ block.settings.border_color }};
      }
    </style>
  {% elsif block.settings.shape_type == "oval" %}
    <style>
      .shapes-item-{{ block.id }} {
        aspect-ratio: 9.6/13;
        border-radius: 50%/35%;
        border: {{ block.settings.border_thickness }}px solid {{ block.settings.border_color }};
      }
    </style>
  {% elsif block.settings.shape_type == "egg" %}
    <style>
      .shapes-item-{{ block.id }} {
        border-radius: 50%;
        aspect-ratio: 9.6 / 12;
        border: {{ block.settings.border_thickness }}px solid {{ block.settings.border_color }};
      }
    </style>
  {% elsif block.settings.shape_type == "circle" %}
    <style>
      .shapes-item-{{ block.id }} {
        aspect-ratio: 1/1;
      }
      
      .shapes-item-{{ block.id }} .shapes-content-{{ section.id }} {
        padding: 50px;
      }
      
      .shapes-item-{{ block.id }} .shapes-figure-{{ section.id }} {
        border-radius: 50%;
        border: {{ block.settings.border_thickness }}px solid {{ block.settings.border_color }};
      }
    </style>
  {% elsif block.settings.shape_type == "second" %}
    <style>
      .shapes-item-{{ block.id }} {
        aspect-ratio: 1/1.7;
      }

      @media(min-width: 1024px) {
        .shapes-item-{{ block.id }} {
          aspect-ratio: auto;
        }
      }
    </style>
  {% elsif block.settings.shape_type == "third" %}
    <style>
      .shapes-item-{{ block.id }} {
        aspect-ratio: 1/1.7;
      }

      @media(min-width: 1024px) {
        .shapes-item-{{ block.id }} {
          aspect-ratio: auto;
        }
      }
    </style>
  {% elsif block.settings.shape_type == "first" %}
    <style>
      .shapes-item-{{ block.id }} {
        aspect-ratio: 1/1;
      }

      .shapes-item-{{ block.id }} .shapes-content-{{ section.id }} {
        padding: 20px;
      }

      @media(min-width: 1024px) {
        .shapes-item-{{ block.id }} {
          aspect-ratio: auto;
        }
      }
    </style>
  {% endif %}
{% endfor %}

<div class="section-{{ section.id }} shapes-{{ section.id }}" style="background-color:{{ background_color }}; background-image: {{ background_gradient }};">
    <div class="section-{{ section.id }}-settings">
      {% if heading != blank %}
        <div class="shapes-heading-{{ section.id }}">
          {{ heading }}
        </div>
      {% endif %}
      <div class="shapes-slider-{{ section.id }} swiper">
        <div class="swiper-wrapper">
          {% for block in section.blocks %}
          <{% if block.settings.url != blank %}a href="{{ block.settings.url }}"{% else %}div{% endif %} class="shapes-item-{{ section.id }} shapes-item-{{ block.id }} swiper-slide">
