{%- liquid 
  assign padding_horizontal = section.settings.padding_horizontal
  assign padding_horizontal_mobile = section.settings.padding_horizontal_mobile
  assign padding_top = section.settings.padding_top
  assign padding_bottom = section.settings.padding_bottom
  assign border_color = section.settings.border_color
  assign border_thickness = section.settings.border_thickness
  assign margin_top = section.settings.margin_top
  assign margin_bottom = section.settings.margin_bottom
  assign margin_horizontal_mobile = section.settings.margin_horizontal_mobile
  assign margin_horizontal = section.settings.margin_horizontal
  assign background_color = section.settings.background_color
  assign background_gradient = section.settings.background_gradient
  assign width = section.settings.width
  assign content_width = section.settings.content_width
  assign lazy = section.settings.lazy
  assign section_radius = section.settings.section_radius
  assign background_style = section.settings.background_style

  assign tab_content_padding_vertical = section.settings.tab_content_padding_vertical
  assign tab_content_padding_vertical_mobile = section.settings.tab_content_padding_vertical_mobile
  assign tab_content_padding_horizontal = section.settings.tab_content_padding_horizontal
  assign tab_content_padding_horizontal_mobile = section.settings.tab_content_padding_horizontal_mobile

  assign content_gap = section.settings.content_gap
  assign content_gap_mobile = section.settings.content_gap_mobile

  assign content_align = section.settings.content_align
  assign content_align_mobile = section.settings.content_align_mobile

  assign content_horizontal_align = ""
  if content_align == "center"
    assign content_horizontal_align = "center"
  elsif content_align == "right"
    assign content_horizontal_align = "end"
  else
    assign content_horizontal_align = "start"
  endif

  assign content_horizontal_align_mobile = ""
  if content_align_mobile == "center"
    assign content_horizontal_align_mobile = "center"
  elsif content_align_mobile == "right"
    assign content_horizontal_align_mobile = "end"
  else
    assign content_horizontal_align_mobile = "start"
  endif

  assign features_mt = section.settings.features_mt
  assign features_mt_mobile = section.settings.features_mt_mobile
  assign features_gap = section.settings.features_gap
  assign features_gap_mobile = section.settings.features_gap_mobile

  assign feature_padding_vertical_mobile = section.settings.feature_padding_vertical_mobile
  assign feature_padding_vertical = section.settings.feature_padding_vertical
  assign feature_padding_horizontal_mobile = section.settings.feature_padding_horizontal_mobile
  assign feature_padding_horizontal = section.settings.feature_padding_horizontal
  assign feature_border_thickness = section.settings.feature_border_thickness
  assign feature_border_color = section.settings.feature_border_color
  assign feature_radius = section.settings.feature_radius
  assign feature_bg_color = section.settings.feature_bg_color

  assign feature_icon_size = section.settings.feature_icon_size
  assign feature_icon_size_mobile = section.settings.feature_icon_size_mobile
  assign feature_icon_color = section.settings.feature_icon_color

  assign feature_title_size = section.settings.feature_title_size
  assign feature_title_size_mobile = section.settings.feature_title_size_mobile
  assign feature_title_color = section.settings.feature_title_color
  assign feature_title_custom = section.settings.feature_title_custom
  assign feature_title_font = section.settings.feature_title_font
  assign feature_title_height = section.settings.feature_title_height

  assign use_checkmark = section.settings.use_checkmark
  
  assign feature_text_size = section.settings.feature_text_size
  assign feature_text_size_mobile = section.settings.feature_text_size_mobile
  assign feature_text_color = section.settings.feature_text_color
  assign feature_text_custom = section.settings.feature_text_custom
  assign feature_text_font = section.settings.feature_text_font
  assign feature_text_height = section.settings.feature_text_height

  assign tabs_mt = section.settings.tabs_mt
  assign tabs_mt_mobile = section.settings.tabs_mt_mobile
  assign tabs_gap = section.settings.tabs_gap
  assign tabs_gap_mobile = section.settings.tabs_gap_mobile

  assign tab_padding_vertical_mobile = section.settings.tab_padding_vertical_mobile
  assign tab_padding_vertical = section.settings.tab_padding_vertical
  assign tab_padding_horizontal_mobile = section.settings.tab_padding_horizontal_mobile
  assign tab_padding_horizontal = section.settings.tab_padding_horizontal
  assign tab_border_thickness = section.settings.tab_border_thickness
  assign tab_border_color = section.settings.tab_border_color
  assign autoclose = section.settings.autoclose

  assign tab_title_size = section.settings.tab_title_size
  assign tab_title_size_mobile = section.settings.tab_title_size_mobile
  assign tab_title_color = section.settings.tab_title_color
  assign tab_title_custom = section.settings.tab_title_custom
  assign tab_title_font = section.settings.tab_title_font
  assign tab_title_height = section.settings.tab_title_height
  
  assign tab_text_size = section.settings.tab_text_size
  assign tab_text_size_mobile = section.settings.tab_text_size_mobile
  assign tab_text_color = section.settings.tab_text_color
  assign tab_text_custom = section.settings.tab_text_custom
  assign tab_text_font = section.settings.tab_text_font
  assign tab_text_height = section.settings.tab_text_height
  assign tab_text_padding_top = section.settings.tab_text_padding_top

  assign open_icon_color = section.settings.open_icon_color
  assign open_icon_size = section.settings.open_icon_size
  assign open_icon_size_mobile = section.settings.open_icon_size_mobile

  assign video = section.settings.video
  assign video_url = section.settings.video_url
  assign image = section.settings.image
  assign media_type = section.settings.media_type
  assign use_controls = section.settings.use_controls
  assign media_position_mobile = section.settings.media_position_mobile
  assign media_position = section.settings.media_position
  assign media_ratio = section.settings.media_ratio
  assign media_ratio_mobile = section.settings.media_ratio_mobile
  assign media_radius = section.settings.media_radius
  assign media_border_thickness = section.settings.media_border_thickness
  assign media_border_color = section.settings.media_border_color
  
  assign play_icon_size = section.settings.play_icon_size
  assign play_icon_size_mobile = section.settings.play_icon_size_mobile
  assign custom_play_icon_size = section.settings.custom_play_icon_size
  assign custom_play_icon_size_mobile = section.settings.custom_play_icon_size_mobile
  assign play_border_thickness = section.settings.play_border_thickness
  assign play_border_color = section.settings.play_border_color
  assign play_border_hover_color = section.settings.play_border_hover_color
  assign play_padding = section.settings.play_padding
  assign play_padding_mobile = section.settings.play_padding_mobile
  assign play_radius =section.settings.play_radius
  assign play_bg_color = section.settings.play_bg_color
  assign play_color = section.settings.play_color
  assign play_bg_hover_color = section.settings.play_bg_hover_color
  assign play_hover_color = section.settings.play_hover_color  
  assign play_button_image = section.settings.play_button_image
  assign play_button_pause_image = section.settings.play_button_pause_image
  assign use_custom_buttons = section.settings.use_custom_buttons

  assign loading_attribute = 'lazy'
  unless lazy
    assign loading_attribute = 'eager'
  endunless
-%}

{%- style -%}
  {{  tab_title_font | font_face: font_display: 'swap' }}
  {{  tab_text_font | font_face: font_display: 'swap' }}
  {{  feature_title_font | font_face: font_display: 'swap' }}
  {{  feature_text_font | font_face: font_display: 'swap' }}

  .section-{{ section.id }} {
    border-top: solid {{ border_color }} {{ border_thickness }}px;
    border-bottom: solid {{ border_color }} {{ border_thickness }}px;
    margin-top: {{ margin_top | times: 0.75 | round: 0 }}px;
    margin-bottom: {{ margin_bottom | times: 0.75 | round: 0 }}px;
    margin-left: {{ margin_horizontal_mobile }}rem;
    margin-right: {{ margin_horizontal_mobile }}rem;
    border-radius: {{ section_radius | times: 0.6 | round: 0 }}px;
    overflow: hidden;
  }
  
  .section-{{ section.id }}-settings {
    margin: 0 auto;
    padding-top: {{ padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ padding_bottom | times: 0.75 | round: 0 }}px;
    padding-left: {{ padding_horizontal_mobile }}rem;
    padding-right: {{ padding_horizontal_mobile }}rem;
  }

  .tabs-body-{{ section.id }} {
    display: grid;
    grid-template-columns: 1fr;
    align-items: stretch;
    gap: {{ content_gap_mobile }}px;
  }

  .tabs-content-{{ section.id }} {
    padding: {{ tab_content_padding_vertical_mobile }}px {{ tab_content_padding_horizontal_mobile }}px;
  }

  .tabs-tabs-{{ section.id }} {
    display: flex;
    flex-direction: column;
    align-items: start;
    width: 100%;
    justify-content: {{ content_horizontal_align_mobile }};
    margin-top: {{ tabs_mt_mobile }}px;
    gap: {{ tabs_gap_mobile }}px;
  }

  .tabs-features-{{ section.id }} {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: {{ features_gap_mobile }}px;
    margin-top: {{ features_mt_mobile }}px;
  }

  .tabs-feature-{{ section.id }} {
    display: flex;
    align-items: center;
    padding: {{ feature_padding_vertical_mobile }}px {{ feature_padding_horizontal_mobile }}px;
    border: {{ feature_border_thickness }}px solid {{ feature_border_color }};
    border-radius: {{ feature_radius }}px;
    justify-content: {{ content_horizontal_align_mobile }};   
    background-color: {{ feature_bg_color }};
    gap: {{ features_gap_mobile }}px;
  }

  .tabs-tab-{{ section.id }} {
    width: 100%;
    position: relative;
    padding: {{ tab_padding_vertical_mobile }}px {{ tab_padding_horizontal_mobile }}px;
    border-bottom: {{ tab_border_thickness }}px solid {{ tab_border_color }};
  }

  .tabs-toggle-{{ section.id }} {
    display: flex;
    justify-content: space-between;
    align-items: center;
    cursor: pointer;
  }

  .tabs-feature-icon-{{ section.id }} {
    overflow: hidden;
    flex-shrink: 0;
  }

  .tabs-feature-icon-{{ section.id }} img,
  .tabs-feature-icon-{{ section.id }} svg {
    display: block;
    width: {{ feature_icon_size_mobile }}px;
    height: {{ feature_icon_size_mobile }}px;
    object-fit: cover;
  }
  
  .tabs-feature-icon-{{ section.id }} svg path {
    fill: {{ feature_icon_color }};
  }

  .tabs-feature-icon-{{ section.id }} svg circle {
    fill: {{ feature_icon_color }};
  }

  .tabs-feature-icon-{{ section.id }} .refresh-{{ section.id }} path,
  .tabs-feature-icon-{{ section.id }} .refresh-{{ section.id }} polyline{
    fill: none;
    stroke: {{ feature_icon_color }};
  }

  .tabs-title-wrapper-{{ section.id }} {
    display: flex;
    align-items: center;
    gap: 20px;
  }

  .tabs-title-wrapper-{{ section.id }} svg {
    display: flex;
    width: {{ tab_title_size_mobile }}px;
    height: {{ tab_title_size_mobile }}px;
    object-fit: cover;
  }

  .tabs-title-wrapper-{{ section.id }} svg path {
    stroke: {{ tab_title_color }};
  }
  
  .tabs-title-{{ section.id }} {
    margin: 0px;
    font-size: {{ tab_title_size_mobile }}px;
    color: {{ tab_title_color }};
    line-height: {{ tab_title_height }}%;
    text-transform: unset;
    font-weight: 700;
  }

  .tabs-feature-title-{{ section.id }} {
    margin: 0px;
    font-size: {{ feature_title_size_mobile }}px;
    color: {{ feature_title_color }};
    line-height: {{ feature_title_height }}%;
    text-transform: unset;
    font-weight: 700;
  }

  .tabs-open-icon-{{ section.id }} {
    display: flex;
    align-items: center;
    justify-content: center;
    flex: 0 0 {{ open_icon_size_mobile }}px;
    height: {{ open_icon_size_mobile }}px;
    transition: all 0.25s ease 0s;
  }

  .tabs-open-icon-{{ section.id }}.active {
    transform: rotate(45deg);
  }

  .tabs-open-icon-{{ section.id }} svg {
    width: 100%;
    height: 100%;
    transition: all 0.25s ease 0s;
  }

  .tabs-open-icon-{{ section.id }} svg path {
    stroke: {{ open_icon_color }};
  }

  .tabs-tabs-text-{{ section.id }} {
    transition: all 0.2s ease-in-out;
    overflow: hidden;
  }

  .tabs-tabs-text-{{ section.id }} * {
    margin: 0px;
    font-size: {{ tab_text_size_mobile }}px;
    color: {{ tab_text_color }};
    line-height: {{ tab_text_height }}%;
    text-transform: unset;
    text-wrap: wrap;
    word-break: break-word;
  }

  .tabs-feature-text-{{ section.id }} * {
    margin: 0px;
    font-size: {{ feature_text_size_mobile }}px;
    color: {{ feature_text_color }};
    line-height: {{ feature_text_height }}%;
    text-transform: unset;
    text-wrap: wrap;
    word-break: break-word;
  }

  .tabs-height-cal-{{ section.id }} {
    visibility: hidden;
    opacity: 0;
    pointer-events: none;
    position: absolute;
    top: 0;
    left: 0;
    width: calc(100% - {{ tab_padding_horizontal_mobile | times: 2 }}px);
    padding-top: {{ tab_text_padding_top }}px;
  }

  .tabs-height-cal-{{ section.id }} * {
    margin: 0px;
    font-size: {{ tab_text_size_mobile }}px;
    color: {{ tab_text_color }};
    line-height: {{ tab_text_height }}%;
    text-transform: unset;
    text-wrap: wrap;
    word-break: break-word;
  }

  .tabs-media-{{ section.id }} {
    position: relative;
    overflow: hidden;
    border-radius: {{ media_radius }}px;
    border: {{ media_border_thickness }}px solid {{ media_border_color }};
    width: 100%;
  }

  .tabs-media-{{ section.id }} video,
  .tabs-media-{{ section.id }} iframe,
  .tabs-media-{{ section.id }} img,
  .tabs-media-{{ section.id }} svg {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
    border: 0px;
  }

  .tabs-media-{{ section.id }} svg {
    background-color: #AFAFAF;
  }

 .tabs-controls-{{ section.id }} {
    display: flex;
    position: absolute;
    top: 16px;
    left: 16px;
  }
  
  .tabs-play-{{ section.id }} {
    margin: 0;
    cursor: pointer;
    z-index: 2;
    {% if use_custom_buttons %}
      border-radius: 0;
      background-color: transparent;
      padding: 0;
      border: 0;
    {% else %}
      border-radius: {{ play_radius }}px;
      background-color: {{ play_bg_color }};
      padding: {{ play_padding_mobile }}px;
      border: {{ play_border_thickness }}px solid {{ play_border_color }};
    {% endif %}
    transition: all 0.25s ease 0s;
  }

  .tabs-play-{{ section.id }} svg:first-child path {
    fill: {{ play_color }};
  }

  .tabs-play-{{ section.id }} svg:last-child path {
    fill: {{ play_color }};
  }
  
  .tabs-play-{{ section.id }} svg,
  .tabs-play-{{ section.id }} img {
    display: block;
    object-fit: cover;
    {% if use_custom_buttons %}
      width: {{ custom_play_icon_size_mobile }}px;
      height: {{ custom_play_icon_size_mobile }}px;
    {% else %}
      width: {{ play_icon_size_mobile }}px;
      height: {{ play_icon_size_mobile }}px;
    {% endif %}
  }

  .tabs-play-{{ section.id }} svg,
  .tabs-play-{{ section.id }} img {
    background-color: transparent !important;
  }
  
  .tabs-play-{{ section.id }} svg:nth-child(1),
  .tabs-play-{{ section.id }} img:nth-child(1) {
    display: none;
  }

  .tabs-play-{{ section.id }}.active svg:nth-child(1),
  .tabs-play-{{ section.id }}.active img:nth-child(1) {
    display: block;
  }

  .tabs-play-{{ section.id }}.active svg:nth-child(2),
  .tabs-play-{{ section.id }}.active img:nth-child(2) {
    display: none;
  }

  @media(min-width: 1024px) {

    .section-{{ section.id }} {
      margin-top: {{ margin_top }}px;
      margin-bottom: {{ margin_bottom }}px;
      margin-left: {{ margin_horizontal }}rem;
      margin-right: {{ margin_horizontal }}rem;
      border-radius: {{ section_radius }}px;
    }
    
    .section-{{ section.id }}-settings {
      padding: 0 5rem;
      padding-top: {{ padding_top }}px;
      padding-bottom: {{ padding_bottom }}px;
      padding-left: {{ padding_horizontal }}rem;
      padding-right: {{ padding_horizontal }}rem;
    }

    .tabs-body-{{ section.id }} {
      grid-template-columns: 1fr 1fr;
      gap: {{ content_gap }}px;
    }

    .tabs-content-{{ section.id }} {
      padding: {{ tab_content_padding_vertical }}px {{ tab_content_padding_horizontal }}px;
    }

    .tabs-tabs-{{ section.id }} {
      margin-top: {{ tabs_mt }}px;
      gap: {{ tabs_gap }}px;
      justify-content: {{ content_horizontal_align }};
    }

    .tabs-features-{{ section.id }} {
      grid-template-columns: repeat(3, 1fr);
      gap: {{ features_gap }}px;
      margin-top: {{ features_mt }}px;
      justify-content: {{ content_horizontal_align }};   
    }

    .tabs-feature-{{ section.id }} {
      justify-content: {{ content_horizontal_align }};         
      padding: {{ feature_padding_vertical }}px {{ feature_padding_horizontal }}px;
    }

    .tabs-tab-{{ section.id }} {
      padding: {{ tab_padding_vertical }}px {{ tab_padding_horizontal }}px;
    }

    .tabs-feature-icon-{{ section.id }} img,
    .tabs-feature-icon-{{ section.id }} svg {
      width: {{ feature_icon_size }}px;
      height: {{ feature_icon_size }}px;
    }

    .tabs-title-wrapper-{{ section.id }} svg {
      width: {{ tab_title_size }}px;
      height: {{ tab_title_size }}px;
    }

    .tabs-title-{{ section.id }} {
      font-size: {{ tab_title_size }}px;
    }

    .tabs-feature-title-{{ section.id }} {
      font-size: {{ feature_title_size }}px;
    }

    .tabs-open-icon-{{ section.id }} {
      flex: 0 0 {{ open_icon_size }}px;
      height: {{ open_icon_size }}px;
    }

    .tabs-tabs-text-{{ section.id }} * {
      font-size: {{ tab_text_size }}px;
    }

    .tabs-feature-text-{{ section.id }} * {
      font-size: {{ feature_text_size }}px;
    }

    .tabs-height-cal-{{ section.id }} {
      width: calc(100% - {{ tab_padding_horizontal | times: 2 }}px);
    }

    .tabs-height-cal-{{ section.id }} * {
      font-size: {{ tab_text_size }}px;
    }

    .tabs-play-{{ section.id }} {
      {% if use_custom_buttons %}
        padding: 0;
      {% else %}
        padding: {{ play_padding }}px;
      {% endif %}
    }
    
    {% unless use_custom_buttons %}
    .tabs-play-{{ section.id }}:hover {      
      border: {{ play_border_thickness }}px solid {{ play_border_hover_color }};
      background-color: {{ play_bg_hover_color }};
      transition: all 0.25s ease 0s;
    }
    {% endunless %}
    
    .tabs-play-{{ section.id }}:hover svg:first-child path {
      fill: {{ play_hover_color }};
    }

    .tabs-play-{{ section.id }}:hover svg:last-child path {
      fill: {{ play_hover_color }};
    }

    .tabs-play-{{ section.id }} svg,
    .tabs-play-{{ section.id }} img {
      {% if use_custom_buttons %}
        width: {{ custom_play_icon_size }}px;
        height: {{ custom_play_icon_size }}px;
      {% else %}
        width: {{ play_icon_size }}px;
        height: {{ play_icon_size }}px;
      {% endif %}
    }
  }
  
{%- endstyle -%}

{% if width == 'custom' %}
  <style>
    .section-{{ section.id }}-settings {
      max-width: {{ content_width }}px;
    }
  </style>
{% elsif width == 'page' %}
  <style>
    .section-{{ section.id }}-settings {
      max-width: var(--page-width);
    }
  </style>
{% endif %}

{% if margin_horizontal_mobile > 0 %}
  <style>
    .section-{{ section.id }} {
      border-left: solid {{ border_color }} {{ border_thickness }}px;
      border-right: solid {{ border_color }} {{ border_thickness }}px;
    }
    
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: 0px;
        border-right: 0px;
      }
    }
  </style>
{% endif %}

{% if margin_horizontal > 0 %}
  <style>
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: solid {{ border_color }} {{ border_thickness }}px;
        border-right: solid {{ border_color }} {{ border_thickness }}px;
      }
    }
  </style>
{% endif %}

{% if tab_title_custom %}
  <style>
    .tabs-title-{{ section.id }} {
      font-family: {{ tab_title_font.family }}, {{ tab_title_font.fallback_families }};
      font-weight: {{ tab_title_font.weight }};
      font-style: {{ tab_title_font.style }};
    }
  </style>
{% endif %}

{% if tab_text_custom %}
  <style>
    .tabs-tabs-text-{{ section.id }} *,
    .tabs-height-cal-{{ section.id }} *  {
      font-family: {{ tab_text_font.family }}, {{ tab_text_font.fallback_families }};
      font-weight: {{ tab_text_font.weight }};
      font-style: {{ tab_text_font.style }};
    }
  </style>
{% endif %}

{% if feature_title_custom %}
  <style>
    .tabs-feature-title-{{ section.id }} {
      font-family: {{ feature_title_font.family }}, {{ feature_title_font.fallback_families }};
      font-weight: {{ feature_title_font.weight }};
      font-style: {{ feature_title_font.style }};
    }
  </style>
{% endif %}

{% if feature_text_custom %}
  <style>
    .tabs-feature-text-{{ section.id }} * {
      font-family: {{ feature_text_font.family }}, {{ feature_text_font.fallback_families }};
      font-weight: {{ feature_text_font.weight }};
      font-style: {{ feature_text_font.style }};
    }
  </style>
{% endif %}


{% if media_position_mobile == "top" %}
  <style>
    .tabs-media-{{ section.id }} {
        order: 1;
    }

    .tabs-content-{{ section.id }} {
        order: 2;
    }
  </style>
{% else %}
  <style>
    .tabs-media-{{ section.id }} {
        order: 2;
    }

    .tabs-content-{{ section.id }} {
        order: 1;
    }
  </style>
{% endif %}

{% if media_position == "right" %}
  <style>
    @media(min-width: 1024px) {

      .tabs-media-{{ section.id }} {
          order: 2;
      }

      .tabs-content-{{ section.id }} {
          order: 1;
      }
    }
  </style>
{% else %}
  <style>
    @media(min-width: 1024px) {

      .tabs-media-{{ section.id }} {
          order: 1;
      }

      .tabs-content-{{ section.id }} {
          order: 2;
      }
    }
  </style>
{% endif %}

{% if media_ratio_mobile == "portrait" %}
  <style>
    .tabs-media-{{ section.id }} {
     aspect-ratio: 9.6/12; 
    }
  </style>
{% elsif media_ratio_mobile == "landscape" %}
  <style>
    .tabs-media-{{ section.id }}{
     aspect-ratio: 12/9; 
    }
  </style>
{% elsif media_ratio_mobile == "square" %}
  <style>
    .tabs-media-{{ section.id }} {
     aspect-ratio: 12/12; 
    }
  </style>
{% else %}
  <style>
    .tabs-media-{{ section.id }} {
     aspect-ratio: auto; 
    }
  </style>
{% endif %}

{% if media_ratio == "portrait" %}
  <style>
    @media(min-width: 1024px) {
     .tabs-media-{{ section.id }} {
       aspect-ratio: 9.6/12; 
      } 
    }
