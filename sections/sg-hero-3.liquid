{%- liquid 
  assign padding_horizontal = section.settings.padding_horizontal
  assign padding_horizontal_mobile = section.settings.padding_horizontal_mobile
  assign padding_top = section.settings.padding_top
  assign padding_bottom = section.settings.padding_bottom
  assign border_color = section.settings.border_color
  assign border_thickness = section.settings.border_thickness
  assign margin_top = section.settings.margin_top
  assign margin_bottom = section.settings.margin_bottom
  assign background_color = section.settings.background_color
  assign background_gradient = section.settings.background_gradient
  assign full_width = section.settings.full_width
  assign content_width = section.settings.content_width
  assign lazy = section.settings.lazy

  assign bg_image = section.settings.bg_image
  assign bg_video = section.settings.bg_video
  assign bg_type = section.settings.bg_type
  assign bg_custom = section.settings.bg_custom
  assign bg_height = section.settings.bg_height
  assign bg_height_mobile = section.settings.bg_height_mobile
  assign bg_overlay = section.settings.bg_overlay
  assign bg_overlay_color = section.settings.bg_overlay_color
  assign use_bg_overlay_image = section.settings.use_bg_overlay_image

  assign first_image = section.settings.first_image
  assign first_image_custom = section.settings.first_image_custom
  assign first_image_height = section.settings.first_image_height
  assign first_image_height_mobile = section.settings.first_image_height_mobile
  assign first_image_radius = section.settings.first_image_radius
  assign first_image_border_thickness = section.settings.first_image_border_thickness
  assign first_image_border_color = section.settings.first_image_border_color
  assign first_image_hide = section.settings.first_image_hide

  assign second_image = section.settings.second_image
  assign second_image_custom = section.settings.second_image_custom
  assign second_image_height = section.settings.second_image_height
  assign second_image_height_mobile = section.settings.second_image_height_mobile
  assign second_image_radius = section.settings.second_image_radius
  assign second_image_border_thickness = section.settings.second_image_border_thickness
  assign second_image_border_color = section.settings.second_image_border_color
  assign second_image_hide = section.settings.second_image_hide

  assign layout_width = section.settings.layout_width
  assign layout_gap = section.settings.layout_gap
  assign layout_gap_mobile = section.settings.layout_gap_mobile
  assign layout_type = section.settings.layout_type
  assign layout_image = section.settings.layout_image
  assign layout_content = section.settings.layout_content

  assign content_padding_vertical = section.settings.content_padding_vertical
  assign content_padding_vertical_mobile = section.settings.content_padding_vertical_mobile
  assign content_padding_horizontal = section.settings.content_padding_horizontal
  assign content_padding_horizontal_mobile = section.settings.content_padding_horizontal_mobile
  assign content_radius = section.settings.content_radius
  assign content_border_thickness = section.settings.content_border_thickness
  assign content_border_color = section.settings.content_border_color
  assign content_bg_color = section.settings.content_bg_color
  assign content_bg_opacity = section.settings.content_bg_opacity

  assign heading = section.settings.heading
  assign heading_size = section.settings.heading_size
  assign heading_size_mobile = section.settings.heading_size_mobile
  assign heading_color = section.settings.heading_color
  assign heading_custom = section.settings.heading_custom
  assign heading_font = section.settings.heading_font
  assign heading_height = section.settings.heading_height
  assign heading_align = section.settings.heading_align

  assign text = section.settings.text
  assign text_size = section.settings.text_size
  assign text_size_mobile = section.settings.text_size_mobile
  assign text_color = section.settings.text_color
  assign text_custom = section.settings.text_custom
  assign text_font = section.settings.text_font
  assign text_height = section.settings.text_height
  assign text_align = section.settings.text_align
  assign text_mt = section.settings.text_mt

  assign button = section.settings.button
  assign button_url = section.settings.button_url
  assign button_size = section.settings.button_size
  assign button_size_mobile = section.settings.button_size_mobile
  assign button_color = section.settings.button_color
  assign button_hover_color = section.settings.button_hover_color
  assign button_custom = section.settings.button_custom
  assign button_font = section.settings.button_font
  assign button_height = section.settings.button_height
  assign button_align = section.settings.button_align
  assign button_mt = section.settings.button_mt
  assign button_padding_vertical = section.settings.button_padding_vertical
  assign button_padding_horizontal = section.settings.button_padding_horizontal
  assign button_radius = section.settings.button_radius
  assign button_border_thickness = section.settings.button_border_thickness
  assign button_border_color = section.settings.button_border_color
  assign button_border_hover_color = section.settings.button_border_hover_color
  assign button_bg_color = section.settings.button_bg_color
  assign button_bg_hover_color = section.settings.button_bg_hover_color
  assign button_use_arrow = section.settings.button_use_arrow

  assign rating = section.settings.rating
  assign rating_size = section.settings.rating_size
  assign rating_size_mobile = section.settings.rating_size_mobile
  assign rating_color = section.settings.rating_color
  assign rating_custom = section.settings.rating_custom
  assign rating_font = section.settings.rating_font
  assign rating_height = section.settings.rating_height
  assign rating_align = section.settings.rating_align
  assign rating_mt = section.settings.rating_mt
  assign rating_count = section.settings.rating_count
  assign rating_stars_color = section.settings.rating_stars_color
  assign rating_stars_size = section.settings.rating_stars_size
  
-%}

{%- style -%}

  {{  heading_font | font_face: font_display: 'swap' }}
  {{  text_font | font_face: font_display: 'swap' }}
  {{  button_font | font_face: font_display: 'swap' }}
  {{  rating_font | font_face: font_display: 'swap' }}
  
  .section-{{ section.id }} {
    border-top: solid {{ border_color }} {{ border_thickness }}px;
    border-bottom: solid {{ border_color }} {{ border_thickness }}px;
    margin-top: {{ margin_top | times: 0.75 | round: 0 }}px;
    margin-bottom: {{ margin_bottom | times: 0.75 | round: 0 }}px;
  }
  
  .section-{{ section.id }}-settings {
    margin: 0 auto;
    padding-top: {{ padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ padding_bottom | times: 0.75 | round: 0 }}px;
    padding-left: {{ padding_horizontal_mobile }}rem;
    padding-right: {{ padding_horizontal_mobile }}rem;
  }

  .hero-{{ section.id }} {
    position: relative;
  }

  .hero-body-{{ section.id }} {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    z-index: 2;
  }

  .hero-media-{{ section.id }} {
    position: relative;
  }

  .hero-media-{{ section.id }}:after {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    z-index: 1;
    background-color: {{ bg_overlay_color }};
    opacity: {{ bg_overlay }};
  }

  .hero-media-{{ section.id }} img,
  .hero-media-{{ section.id }} svg,
  .hero-media-{{ section.id }} video,
  .hero-media-{{ section.id }} iframe {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .hero-media-{{ section.id }} svg {
    background-color: #DDDDDD;
  }

  .hero-layout-{{ section.id }} {
    width: 100%;
    max-width: {{ layout_width }}rem;
    display: grid;
    gap: {{ layout_gap_mobile }}px;
  }

  .hero-first-image-{{ section.id }} {
    border-radius: {{ first_image_radius }}px;
    border: {{ first_image_border_thickness }}px solid {{ first_image_border_color }};
  }

  .hero-first-image-{{ section.id }} img,
  .hero-first-image-{{ section.id }} svg {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
    border-radius: {{ first_image_radius }}px;
  }

  .hero-first-image-{{ section.id }} svg {
    background-color: white;
  }

  .hero-second-image-{{ section.id }} {
    border-radius: {{ second_image_radius }}px;
    border: {{ second_image_border_thickness }}px solid {{ second_image_border_color }};
  }
  
  .hero-second-image-{{ section.id }} img,
  .hero-second-image-{{ section.id }} svg {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
    border-radius: {{ second_image_radius }}px;
  }

  .hero-second-image-{{ section.id }} svg {
    background-color: white;
  }

  .hero-first-image-{{ section.id }}.hero-first-image-desktop-{{ section.id  }},
  .hero-second-image-{{ section.id }}.hero-second-image-desktop-{{ section.id  }} {
    display: none;
  }

  .hero-media-images-{{ section.id }} {
    width: 100%;
    display: grid;
    align-items: center;
    gap: {{ layout_gap_mobile }}px;
  }

  .hero-content-{{ section.id }} {
    display: flex;
    flex-direction: column;
    justify-content: center;
    padding: {{ content_padding_vertical_mobile }}px {{ content_padding_horizontal_mobile }}px;
    border-radius: {{ content_radius }}px;
    border: {{ content_border_thickness }}px solid {{ content_border_color }};
    background-color: {{ content_bg_color | hex_to_rgba: content_bg_opacity }};
  }

  .hero-heading-{{ section.id }} {
    margin: 0;
    font-size: {{ heading_size_mobile }}px;
    color: {{ heading_color }};
    line-height: {{ heading_height }}%;
    text-transform: unset;
    text-align: {{ heading_align }};
  }

  .hero-text-{{ section.id }} {
    margin: 0;
    margin-top: {{ text_mt | times: 0.75 | round: 0 }}px;
    font-size: {{ text_size_mobile }}px;
    color: {{ text_color }};
    line-height: {{ text_height }}%;
    text-transform: unset;
    text-align: {{ text_align }};
  }

  .hero-btn-{{ section.id }} {
    width: 100%;
    max-width: 300px;
    margin: 0px;
    margin-top: {{ button_mt | times: 0.75 | round: 0 }}px;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: {{ button_size_mobile }}px;
    color: {{ button_color }};
    line-height: {{ button_height }}%;
    text-transform: unset;
    text-decoration: none;
    padding: {{ button_padding_vertical | times: 0.75 | round: 0 }}px {{ button_padding_horizontal | times: 0.75 | round: 0 }}px;
    border-radius: {{ button_radius }}px;
    border: {{ button_border_thickness }}px solid {{ button_border_color }};
    background-color: {{ button_bg_color }};
    transition: all 0.25s ease 0s;
  }

  .hero-btn-{{ section.id }}:hover {
    transition: all 0.25s ease 0s;
    color: {{ button_hover_color }};
    border: {{ button_border_thickness }}px solid {{ button_border_hover_color }};
    background-color: {{ button_bg_hover_color }};
  }

  .hero-rating-{{ section.id }} {
    margin-top: {{ rating_mt | times: 0.75 | round: 0 }}px;
    display: flex;
    align-items: center;
    justify-content: {{ rating_align }};
    gap: 10px;
  }

  .hero-stars-{{ section.id }} {
    display: flex;
    align-items: center;
    gap: 5px;
  }

  .hero-stars-{{ section.id }} svg {
    display: block;
    width: {{ rating_stars_size }}px;
    height: {{ rating_stars_size }}px;
  }

  .hero-stars-{{ section.id }} svg path {
    fill: {{ rating_stars_color }};
  }

  .hero-rating-text-{{ section.id }} {
    margin: 0px;
    font-size: {{ rating_size_mobile }}px;
    color: {{ rating_color }};
    line-height: {{ rating_height }}%;
    text-transform: unset;
  }
  
  @media(min-width: 1024px) {

    .section-{{ section.id }} {
      margin-top: {{ margin_top }}px;
      margin-bottom: {{ margin_bottom }}px;
    }
    
    .section-{{ section.id }}-settings {
      padding: 0 5rem;
      padding-top: {{ padding_top }}px;
      padding-bottom: {{ padding_bottom }}px;
      padding-left: {{ padding_horizontal }}rem;
      padding-right: {{ padding_horizontal }}rem;
    }

    .hero-layout-{{ section.id }} {
      gap: {{ layout_gap }}px;
    }

     .hero-heading-{{ section.id }} {
       font-size: {{ heading_size }}px;
     }

    .hero-text-{{ section.id }} {
      margin-top: {{ text_mt }}px;
      font-size: {{ text_size }}px;
    }

    .hero-btn-{{ section.id }} {
      margin-top: {{ button_mt }}px;
      font-size: {{ button_size }}px;
      padding: {{ button_padding_vertical }}px {{ button_padding_horizontal }}px;
    }

    .hero-rating-{{ section.id }} {
      margin-top: {{ rating_mt }}px;
    }

    .hero-rating-text-{{ section.id }} {
      font-size: {{ rating_size }}px;
    }

    .hero-media-images-{{ section.id }} {
      display: none;
    }

    .hero-first-image-{{ section.id }}.hero-first-image-desktop-{{ section.id  }},
    .hero-second-image-{{ section.id }}.hero-second-image-desktop-{{ section.id  }} {
      display: block;
    }
  }

  @media(min-width: 1200px) {
    .hero-content-{{ section.id }} {
      padding: {{ content_padding_vertical }}px {{ content_padding_horizontal }}px;
    }
  }
  
{%- endstyle -%}

{% unless full_width %}
  <style>
    .section-{{ section.id }}-settings {
      max-width: {{ content_width }}rem;
    }
  </style>
{% endunless %}

{% if bg_custom %}
  <style>
    .hero-media-{{ section.id }},
    .hero-media-{{ section.id }} img,
    .hero-media-{{ section.id }} svg,
    .hero-media-{{ section.id }} video {
      height: {{ bg_height_mobile }}px;
    }
    
    @media(min-width: 1024px) {
      .hero-media-{{ section.id }},
      .hero-media-{{ section.id }} img,
      .hero-media-{{ section.id }} svg,
      .hero-media-{{ section.id }} video {
        height: {{ bg_height }}px;
      }
    }
  </style>
{% endif %}

{% if first_image_custom %}
  <style>
    .hero-first-image-{{ section.id }},
    .hero-first-image-{{ section.id }} img {
      height: 100%;
      max-height: {{ first_image_height_mobile }}px;
    }

    @media(min-width: 1024px) {
      .hero-first-image-{{ section.id }},
      .hero-first-image-{{ section.id }} img {
        height: 100%;
        max-height: {{ first_image_height }}px;
      }
    }
  </style>
{% endif %}

{% if second_image_custom %}
  <style>
    .hero-second-image-{{ section.id }},
    .hero-second-image-{{ section.id }} img {
      height: 100%;
      max-height: {{ second_image_height_mobile }}px;
    }

    @media(min-width: 1024px) {
      .hero-second-image-{{ section.id }},
      .hero-second-image-{{ section.id }} img {
        height: 100%;
        max-height: {{ second_image_height }}px;
      }
    }
  </style>
{% endif %}

{% if first_image_hide and second_image_hide %}
  <style>
    @media(min-width: 1024px) {
      .hero-layout-{{ section.id }} {
        grid-template-columns: repeat(1, 1fr);
      } 
    }
  </style>
{% elsif first_image_hide == false and second_image_hide == false %}
  <style>
    @media(min-width: 1024px) {
      .hero-layout-{{ section.id }} {
        grid-template-columns: repeat(3, 1fr);
      } 
    }
  </style>
{% else %}
  <style>
    @media(min-width: 1024px) {
      .hero-layout-{{ section.id }} {
        grid-template-columns: repeat(2, 1fr);
      } 
    }
  </style>
{% endif %}

{% if layout_type == "content_media_media" %}
  <style>
    @media(min-width: 1024px) {
      .hero-content-{{ section.id }} {
        order: 1;
      }
      .hero-first-image-{{ section.id }} {
        order: 2;
      }
      .hero-second-image-{{ section.id }} {
        order: 3;
      } 
    }
  </style>
{% elsif layout_type == "media_content_media" %}
  <style>
    @media(min-width: 1024px) {
      .hero-content-{{ section.id }} {
        order: 2;
      }
      .hero-first-image-{{ section.id }} {
        order: 1;
      }
      .hero-second-image-{{ section.id }} {
        order: 3;
      } 
    }
  </style>
{% endif %}

{% if heading_custom %}
  <style>
    .hero-heading-{{ section.id }} {
      font-family: {{ heading_font.family }}, {{ heading_font.fallback_families }};
      font-weight: {{ heading_font.weight }};
      font-style: {{ heading_font.style }};
    }
  </style>
{% endif %}

{% if text_custom %}
  <style>
    .hero-text-{{ section.id }} {
      font-family: {{ text_font.family }}, {{ text_font.fallback_families }};
      font-weight: {{ text_font.weight }};
      font-style: {{ text_font.style }};
    }
  </style>
{% endif %}

{% if button_use_arrow %}
  <style>
    .hero-btn-{{ section.id }} span {
      display: inline-block;
      position: relative;
      transition: all .25s ease-out;
      will-change: transform;
    }

    .hero-btn-{{ section.id }} span svg {
      position: absolute;
      width: 12px;
      right: 0rem;
      opacity: 0!important;
      top: 50%;
      -webkit-transform: translateY(-50%);
      transform: translateY(-50%);
      -webkit-transition: all .25s ease-out;
      transition: all .25s ease-out;
      will-change: right,opacity;
    }

    .hero-btn-{{ section.id }} span svg path {
      fill: {{ button_color }};
      transition: all .25s ease-out;
    }

    .hero-btn-{{ section.id }}:hover span {
      transform: translate3d(-1rem,0,0);
      transition: all .25s ease-out;
    }

    .hero-btn-{{ section.id }}:hover span svg {
      opacity: 1!important;
      right: -1.75rem;
    }

    .hero-btn-{{ section.id }}:hover span svg path {
      fill: {{ button_hover_color }};
      transition: all .25s ease-out;
    }
  </style>
{% endif %}

{% if button_align == "center" %}
  <style>
    .hero-btn-{{ section.id }} {
      margin-left: auto;
      margin-right: auto;
    }
  </style>
{% elsif button_align == "right" %}
  <style>
    .hero-btn-{{ section.id }} {
      margin-left: auto;
    }
  </style>
{% endif %}

{% if rating_custom %}
  <style>
    .hero-rating-text-{{ section.id }} {
      font-family: {{ rating_font.family }}, {{ rating_font.fallback_families }};
      font-weight: {{ rating_font.weight }};
      font-style: {{ rating_font.style }};
    }
  </style>
{% endif %}

{% if layout_image == "row" %}
  <style>
    .hero-media-images-{{ section.id }} {
      grid-template-columns: repeat(2, 1fr);
    }
  </style>
{% else %}
   <style>
    .hero-media-images-{{ section.id }} {
      grid-template-columns: repeat(1, 1fr);
    }
  </style>
{% endif %}

{% if layout_content == "last" %}
  <style>
    .hero-content-{{ section.id }} {
      margin-top: {{ layout_gap_mobile }}px;
    }

    @media(min-width: 1024px){
      .hero-content-{{ section.id }} {
        margin-top: 0px;
      }
    }
  </style>
{% else %}
  <style>
    .hero-media-images-{{ section.id }} {
      margin-top: {{ layout_gap_mobile }}px;
    }
  </style>
{% endif %}

{% if layout_content == "middle" and layout_image == "column" %}
  <style>
    .hero-layout-{{ section.id }} {
      display: none;
    }
    @media(min-width: 1024px) {
      .hero-layout-{{ section.id }} {
        display: grid;
      }
    }
  </style>
{% endif %}

{% if use_bg_overlay_image %}
  <style>
    .hero-media-{{ section.id }}:before {
      content: '';
      position: absolute;
      left: 0;
      top: 0;
      right: 0;
      bottom: 0;
      z-index: 1;
      background-image: url("data:image/svg+xml,%3Csvg width='1440' height='790' viewBox='0 0 1440 790' fill='none' xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink'%3E%3Cpath d='M0 0H1440V790H0V0Z' fill='url(%23pattern0)' fill-opacity='0.5' style='mix-blend-mode:overlay'/%3E%3Cdefs%3E%3Cpattern id='pattern0' patternContentUnits='objectBoundingBox' width='0.0763889' height='0.139241'%3E%3Cuse xlink:href='%23image0_1_240' transform='scale(0.000347222 0.000632911)'/%3E%3C/pattern%3E%3Cimage id='image0_1_240' width='220' height='220' xlink:href='data:image/png;base64,'/%3E%3C/defs%3E%3C/svg%3E%0A");
      background-repeat: repeat;
      background-position: top left;
      background-blend-mode: overlay;
      opacity: .5;
    }
  </style>
{% endif %}

<div class="section-{{ section.id }} hero-{{ section.id }}" style="background-color:{{ background_color }}; background-image: {{ background_gradient }};">
    <div class="section-{{ section.id }}-settings hero-body-{{ section.id }}">
      {% if layout_content == "last" or layout_content == "middle" %}
        <div class="hero-media-images-{{ section.id }}">
          {% unless first_image_hide %}
            <div class="hero-first-image-{{ section.id }}">
              {% if first_image != blank %}
                <img src="{{ first_image | image_url }}" alt="{{ first_image.alt }}">
              {% else %}
                {{ 'image' | placeholder_svg_tag }}
              {% endif %}
            </div>
          {% endunless %}
          {% if layout_content == "middle" and layout_image == "column" %}
            <div class="hero-content-{{ section.id }}">
              {% if heading != blank %}
                <h2 class="hero-heading-{{ section.id }}">{{ heading }}</h2>
              {% endif %}
    
              {% if text != blank %}
                <p class="hero-text-{{ section.id }}">{{ text }}</p>
              {% endif %}
    
              {% if button != blank %}
                <a href="{{ button_url }}" class="hero-btn-{{ section.id }}">
                  {% if button_use_arrow %}
                  <span>
                    {{ button }}
                    <svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <path d="M6.728 10.28L5.64 9.208L7.368 7.544L8.552 6.408H1V4.872H8.552C8.136 4.488 7.75201 4.104 7.35201 3.72L5.64 2.072L6.728 1L11.528 5.64L6.728 10.28Z" fill="white"/>
                    </svg>
                  </span>
                  {% else %}
                     {{ button }}
                  {% endif %}
                </a>
              {% endif %}
              {% if rating != blank or rating_count != 0 %}
                <div class="hero-rating-{{ section.id }}">
                  {% if rating_count != 0 %}
                    <div class="hero-stars-{{ section.id }}">
                      {% for item in (1..rating_count) %}
                        <svg xmlns="http://www.w3.org/2000/svg" width="20" height="19" viewBox="0 0 20 19" fill="none">
                          <path d="M10 0L12.9389 5.95492L19.5106 6.90983L14.7553 11.5451L15.8779 18.0902L10 15L4.12215 18.0902L5.24472 11.5451L0.489435 6.90983L7.06107 5.95492L10 0Z" fill="#1F448C"></path>
                        </svg>
                      {% endfor %}
                    </div>
                  {% endif %}
                  <p class="hero-rating-text-{{ section.id }}">{{ rating }}</p>
                </div>
              {% endif %}
            </div>
          {% endif %}
          {% unless second_image_hide %}
            <div class="hero-second-image-{{ section.id }}">
              {% if second_image != blank %}
                <img src="{{ second_image | image_url }}" alt="{{ second_image.alt }}">
              {% else %}
                {{ 'image' | placeholder_svg_tag }}
              {% endif %}
            </div>
          {% endunless %}
        </div>
      {% endif %}
      <div class="hero-layout-{{ section.id }}">
          {% unless first_image_hide %}
            <div class="hero-first-image-{{ section.id }} hero-first-image-desktop-{{ section.id }}">
              {% if first_image != blank %}
                <img src="{{ first_image | image_url }}" alt="{{ first_image.alt }}">
              {% else %}
                {{ 'image' | placeholder_svg_tag }}
              {% endif %}
            </div>
          {% endunless %}
          {% unless second_image_hide %}
            <div class="hero-second-image-{{ section.id }} hero-second-image-desktop-{{ section.id }}">
              {% if second_image != blank %}
                <img src="{{ second_image | image_url }}" alt="{{ second_image.alt }}">
              {% else %}
                {{ 'image' | placeholder_svg_tag }}
              {% endif %}
            </div>
          {% endunless %}
        <div class="hero-content-{{ section.id }}">
          {% if heading != blank %}
            <h2 class="hero-heading-{{ section.id }}">{{ heading }}</h2>
          {% endif %}

          {% if text != blank %}
            <p class="hero-text-{{ section.id }}">{{ text }}</p>
          {% endif %}

          {% if button != blank %}
            <a href="{{ button_url }}" class="hero-btn-{{ section.id }}">
              {% if button_use_arrow %}
              <span>
                {{ button }}
                <svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M6.728 10.28L5.64 9.208L7.368 7.544L8.552 6.408H1V4.872H8.552C8.136 4.488 7.75201 4.104 7.35201 3.72L5.64 2.072L6.728 1L11.528 5.64L6.728 10.28Z" fill="white"/>
                </svg>
              </span>
              {% else %}
                 {{ button }}
              {% endif %}
            </a>
          {% endif %}
          {% if rating != blank or rating_count != 0 %}
            <div class="hero-rating-{{ section.id }}">
              {% if rating_count != 0 %}
                <div class="hero-stars-{{ section.id }}">
                  {% for item in (1..rating_count) %}
                    <svg xmlns="http://www.w3.org/2000/svg" width="20" height="19" viewBox="0 0 20 19" fill="none">
                      <path d="M10 0L12.9389 5.95492L19.5106 6.90983L14.7553 11.5451L15.8779 18.0902L10 15L4.12215 18.0902L5.24472 11.5451L0.489435 6.90983L7.06107 5.95492L10 0Z" fill="#1F448C"></path>
                    </svg>
                  {% endfor %}
                </div>
              {% endif %}
              <p class="hero-rating-text-{{ section.id }}">{{ rating }}</p>
            </div>
          {% endif %}
        </div>
      </div>
      {% if layout_content == "first" %}
        <div class="hero-media-images-{{ section.id }}">
          {% unless first_image_hide %}
            <div class="hero-first-image-{{ section.id }}">
              {% if first_image != blank %}
                <img src="{{ first_image | image_url }}" alt="{{ first_image.alt }}">
              {% else %}
                {{ 'image' | placeholder_svg_tag }}
              {% endif %}
            </div>
          {% endunless %}
          {% unless second_image_hide %}
            <div class="hero-second-image-{{ section.id }}">
              {% if second_image != blank %}
                <img src="{{ second_image | image_url }}" alt="{{ second_image.alt }}">
              {% else %}
                {{ 'image' | placeholder_svg_tag }}
              {% endif %}
            </div>
          {% endunless %}
        </div>
      {% endif %}
    </div>
    <div class="hero-media-{{ section.id }}">
      {% if bg_type == "image" %}
        
        {% if bg_image != blank %}
          <img src="{{ bg_image | image_url }}" alt="{{ bg_image.alt }}" {% if lazy %}loading="lazy"{% endif %}>
        {% else %}
          {{ 'image' | placeholder_svg_tag }}
        {% endif %}
        
      {% else %}
        
        {% if bg_video != blank %}
          <video data-src="{{ bg_video.sources[1].url }}" muted autoplay playsinline preload="none" loop class="lazy"></video>
          
        {% endif %}
        
      {% endif %}
    </div>
</div>

<script src="https://cdn.jsdelivr.net/npm/vanilla-lazyload@19.1.3/dist/lazyload.min.js"></script>

<script>
  function initHero3() {
    
    var lazyLoadInstance = new LazyLoad();

    window.addEventListener(
      "LazyLoad::Initialized",
      function (event) {
        window.lazyLoadInstance = event.detail.instance;
      },
      false
    );
  }

  document.addEventListener('DOMContentLoaded', initHero3);
  
  if (Shopify.designMode) {
     document.addEventListener('shopify:section:unload', initHero3);
     document.addEventListener('shopify:section:load', initHero3);
  }
</script>        

