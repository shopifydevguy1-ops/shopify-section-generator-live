                        </g>
                        <defs>
                          <clipPath id="clip0_408_5968">
                            <rect width="14" height="14" fill="white" transform="translate(0 0.5)"/>
                          </clipPath>
                        </defs>
                      </svg>
                    {% endif %}
                    
                  {% endif %}
                </div>
              {% endif %}              
              {% if block.settings.first_text != blank %}
                <p class="comparison-text-{{ section.id }}">{{ block.settings.first_text }}</p>  
              {% endif %}                                      
            </div>
            <div class="comparison-right-value-{{ section.id }}">
              {% if block.settings.second_icon != 'none' %}
                <div class="comparison-icon-{{ section.id }}">
