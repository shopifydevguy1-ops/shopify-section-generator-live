          <path id="Vector_2" d="M17.5781 28.7793H22.4314L27.5274 39.8978L32.6014 28.7793H37.4106L29.8438 44.2217H25.1228L17.5781 28.7793Z" fill="white"/>
          </g>
          </g>
        </svg>
      </div>
      <div class="comparison-image-{{ section.id }}">
        {% if first_image != blank %}
          <img src="{{ first_image | image_url }}" alt="{{  first_image.alt }}" {% if lazy %}loading="lazy"{% endif %}>
        {% else %}
          {{ 'image' | placeholder_svg_tag }}
        {% endif %}  
      </div>
      <div class="comparison-table-{{ section.id }}">
        {% for block in section.blocks %}
          <div class="comparison-value-{{ section.id }}">
