    "padding_top": {
      "type": "text",
      "default": "36",
      "label": "t:sections.all.padding.padding_top",
      "description": "t:sections.all.padding.padding_top"
    },
    "padding_bottom": {
      "type": "text",
      "default": "36",
      "label": "t:sections.all.padding.padding_bottom",
      "description": "t:sections.all.padding.padding_bottom"
    }
  }
