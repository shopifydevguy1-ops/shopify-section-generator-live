{
  "id": "custom-shapes",
  "name": "Shapes",
  "description": "Section: Shapes",
  "tags": [
    "shapes"
  ],
  "type": "custom",
  "liquid_code": "<link\n  rel=\"stylesheet\"\n  href=\"https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css\"\n/>\n\n\n{%- liquid \n  assign padding_horizontal = section.settings.padding_horizontal\n  assign padding_horizontal_mobile = section.settings.padding_horizontal_mobile\n  assign padding_top = section.settings.padding_top\n  assign padding_bottom = section.settings.padding_bottom\n  assign border_color = section.settings.border_color\n  assign border_thickness = section.settings.border_thickness\n  assign margin_top = section.settings.margin_top\n  assign margin_bottom = section.settings.margin_bottom\n  assign background_color = section.settings.background_color\n  assign background_gradient = section.settings.background_gradient\n  assign full_width = section.settings.full_width\n  assign content_width = section.settings.content_width\n  assign lazy = section.settings.lazy\n\n  assign heading = section.settings.heading\n  assign heading_size = section.settings.heading_size\n  assign heading_size_mobile = section.settings.heading_size_mobile\n  assign heading_color = section.settings.heading_color\n  assign heading_custom = section.settings.heading_custom\n  assign heading_font = section.settings.heading_font\n  assign heading_height = section.settings.heading_height\n  assign heading_align = section.settings.heading_align\n  assign heading_align_mobile = section.settings.heading_align_mobile\n\n  assign body_gap = section.settings.body_gap\n  assign body_gap_mobile = section.settings.body_gap_mobile\n  assign body_mt = section.settings.body_mt\n\n  assign use_slider = section.settings.use_slider\n  assign slider_gap_mobile = section.settings.slider_gap_mobile\n  assign slider_view_mobile = section.settings.slider_view_mobile\n\n  assign text_color = section.settings.text_color\n  assign text_custom = section.settings.text_custom\n  assign text_font = section.settings.text_font\n  assign text_height = section.settings.text_height\n-%}\n\n{%- style -%}\n\n  {{  heading_font | font_face: font_display: 'swap' }}\n  {{  text_font | font_face: font_display: 'swap' }}\n  \n  .section-{{ section.id }} {\n    border-top: solid {{ border_color }} {{ border_thickness }}px;\n    border-bottom: solid {{ border_color }} {{ border_thickness }}px;\n    margin-top: {{ margin_top | times: 0.75 | round: 0 }}px;\n    margin-bottom: {{ margin_bottom | times: 0.75 | round: 0 }}px;\n  }\n  \n  .section-{{ section.id }}-settings {\n    margin: 0 auto;\n    padding-top: {{ padding_top | times: 0.75 | round: 0 }}px;\n    padding-bottom: {{ padding_bottom | times: 0.75 | round: 0 }}px;\n    padding-left: {{ padding_horizontal_mobile }}rem;\n    padding-right: {{ padding_horizontal_mobile }}rem;\n  }\n\n  .shapes-heading-{{ section.id }} {\n    width: 100%;\n    text-align: {{ heading_align_mobile }};\n    padding-left: {{ padding_horizontal_mobile }}rem;\n    padding-right: {{ padding_horizontal_mobile }}rem;\n  }\n\n  .shapes-heading-{{ section.id }} * {\n    margin: 0;\n    font-size: {{ heading_size_mobile }}px;\n    color: {{ heading_color }};\n    line-height: {{ heading_height }}%;\n    text-transform: unset;\n  }\n\n  .shapes-body-{{ section.id }} {\n    margin-top: {{ body_mt | times: 0.75 | round: 0 }}px;\n    gap: {{ body_gap_mobile }}px;\n  }\n\n  .shapes-slider-{{ section.id }} {\n    margin-top: {{ body_mt | times: 0.75 | round: 0 }}px;\n  }\n\n  .shapes-slider-{{ section.id }} .swiper-wrapper {\n    align-items: center;\n  }\n\n  .shapes-item-{{ section.id }} {\n    position: relative;\n    box-sizing: border-box !important;\n    overflow: hidden;\n  }\n\n  .shapes-figure-{{ section.id }} {\n    display: block !important;\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n    z-index: 1;\n    overflow: hidden;\n  }\n\n  .shapes-figure-{{ section.id }} svg,\n  .shapes-figure-{{ section.id }} img {\n    display: block;\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n  }\n\n  .shapes-content-{{ section.id }} {\n    position: relative;\n    z-index: 2;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    padding: 40px;\n  }\n\n  .shapes-content-{{ section.id }} {\n    text-align: center;\n    height: 100%;\n    width: 100%;\n  }\n\n  .shapes-content-{{ section.id }} * {\n    margin: 0;\n    line-height: {{ text_height }}%;\n    text-transform: unset;\n  }\n\n  .figure-third-{{ section.id }} {\n    position: relative;\n    stroke: rgb(30, 114, 220);\n    stroke-width: 2px;\n    z-index: 1;\n  }\n\n  .figure-third-{{ section.id }} foreignObject {\n    display: initial;\n  }\n\n  .figure-third-{{ section.id }} foreignObject > div {\n    width: 100%;\n    height: 100%;\n  }\n\n  .figure-third-{{ section.id }} foreignObject > div img {\n    display: block;\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n    vertical-align: bottom;\n  }\n\n  .figure-third-{{ section.id }} > g {\n    display: none;\n  }\n\n  .hide-content-{{ section.id }} {\n    opacity: 0;\n    visibility: hidden;\n    z-index: -1;\n  }\n\n  .clip-path-{{ section.id }} {\n    display: none !important;\n  }\n  \n  @media(min-width: 1024px) {\n\n    .section-{{ section.id }} {\n      margin-top: {{ margin_top }}px;\n      margin-bottom: {{ margin_bottom }}px;\n    }\n    \n    .section-{{ section.id }}-settings {\n      padding: 0 5rem;\n      padding-top: {{ padding_top }}px;\n      padding-bottom: {{ padding_bottom }}px;\n      padding-left: {{ padding_horizontal }}rem !important;\n      padding-right: {{ padding_horizontal }}rem !important;\n    }\n\n    .shapes-heading-{{ section.id }} {\n      text-align: {{ heading_align }};\n      padding: 0px;\n    }\n\n    .shapes-heading-{{ section.id }} * {\n      font-size: {{ heading_size }}px;\n    }\n\n    .shapes-body-{{ section.id }} {\n      margin-top: {{ body_mt }}px;\n      display: grid !important;\n      align-items: stretch;\n      grid-auto-rows: min-content;\n      grid-template-columns: repeat(12, minmax(0, 1fr));\n      gap: {{ body_gap }}px;\n    }\n\n    .shapes-slider-{{ section.id }} {\n      display: none !important;\n    }\n    \n    .shapes-content-{{ section.id }} {\n      padding: 48px !important;\n    }\n\n  }\n  \n{%- endstyle -%}\n\n{% unless full_width %}\n  <style>\n    .section-{{ section.id }}-settings {\n      max-width: {{ content_width }}rem;\n    }\n  </style>\n{% endunless %}\n\n{% if heading_custom %}\n  <style>\n    .shapes-heading-{{ section.id }} * {\n      font-family: {{ heading_font.family }}, {{ heading_font.fallback_families }};\n      font-weight: {{ heading_font.weight }};\n      font-style: {{ heading_font.style }};\n    }\n  </style>\n{% endif %}\n\n{% if text_custom %}\n  <style>\n    .shapes-content-{{ section.id }} * {\n      font-family: {{ text_font.family }}, {{ text_font.fallback_families }};\n      font-weight: {{ text_font.weight }};\n      font-style: {{ text_font.style }};\n    }\n  </style>\n{% endif %}\n\n{% if use_slider %}\n  <style>\n    .section-{{ section.id }}-settings {\n      padding-left: 0px;\n      padding-right: 0px;\n    }\n    \n    .shapes-slider-{{ section.id }} {\n      display: block;\n    }\n\n    .shapes-body-{{ section.id }} {\n      display: none;\n    }\n  </style>\n{% else %}\n  <style>\n    .shapes-slider-{{ section.id }} {\n      display: none;\n    }\n\n    .shapes-body-{{ section.id }} {\n      display: grid;\n    }\n  </style>\n{% endif %}\n\n{% for block in section.blocks %}\n  <style>\n\n    .shapes-content-{{ block.id }} * {\n      font-size: {{ block.settings.text_size_mobile }}px;\n    }\n\n    @media(min-width: 1024px) {\n      .shapes-item-{{ block.id }} {\n        grid-row: span {{ block.settings.row_width }} / span {{ block.settings.row_width }};\n        grid-column: span {{ block.settings.column_width }} / span {{ block.settings.column_width }};\n      } \n\n      .shapes-content-{{ block.id }} * {\n        font-size: {{ block.settings.text_size }}px;\n      }\n    }\n\n    .shapes-item-{{ block.id }} img {\n      opacity: {{ block.settings.image_opacity | times: 100 }}%;\n    }\n\n    .shapes-item-{{ block.id }} .shapes-content-{{ section.id }} * {\n      color: {{ block.settings.text_color }};\n    }\n  </style>\n\n  {% if block.settings.shape_type == \"landscape\" %}\n    <style>\n      .shapes-item-{{ block.id }} {\n        aspect-ratio: 12 / 7;\n        border-radius: {{ block.settings.radius }}px;\n        border: {{ block.settings.border_thickness }}px solid {{ block.settings.border_color }};\n      }\n\n      @media(min-width: 1024px) {\n        .shapes-item-{{ block.id }} {\n          aspect-ratio: 1 / 0.3;\n        }\n      }\n    </style>\n  {% elsif block.settings.shape_type == \"portrait\" %}\n    <style>\n      .shapes-item-{{ block.id }} {\n        aspect-ratio: 9.6/13;\n        border-radius: {{ block.settings.radius }}px;\n        border: {{ block.settings.border_thickness }}px solid {{ block.settings.border_color }};\n      }\n    </style>\n  {% elsif block.settings.shape_type == \"square\" %}\n    <style>\n      .shapes-item-{{ block.id }} {\n        aspect-ratio: 12/12;\n        border-radius: {{ block.settings.radius }}px;\n        border: {{ block.settings.border_thickness }}px solid {{ block.settings.border_color }};\n      }\n    </style>\n  {% elsif block.settings.shape_type == \"oval\" %}\n    <style>\n      .shapes-item-{{ block.id }} {\n        aspect-ratio: 9.6/13;\n        border-radius: 50%/35%;\n        border: {{ block.settings.border_thickness }}px solid {{ block.settings.border_color }};\n      }\n    </style>\n  {% elsif block.settings.shape_type == \"egg\" %}\n    <style>\n      .shapes-item-{{ block.id }} {\n        border-radius: 50%;\n        aspect-ratio: 9.6 / 12;\n        border: {{ block.settings.border_thickness }}px solid {{ block.settings.border_color }};\n      }\n    </style>\n  {% elsif block.settings.shape_type == \"circle\" %}\n    <style>\n      .shapes-item-{{ block.id }} {\n        aspect-ratio: 1/1;\n      }\n      \n      .shapes-item-{{ block.id }} .shapes-content-{{ section.id }} {\n        padding: 50px;\n      }\n      \n      .shapes-item-{{ block.id }} .shapes-figure-{{ section.id }} {\n        border-radius: 50%;\n        border: {{ block.settings.border_thickness }}px solid {{ block.settings.border_color }};\n      }\n    </style>\n  {% elsif block.settings.shape_type == \"second\" %}\n    <style>\n      .shapes-item-{{ block.id }} {\n        aspect-ratio: 1/1.7;\n      }\n\n      @media(min-width: 1024px) {\n        .shapes-item-{{ block.id }} {\n          aspect-ratio: auto;\n        }\n      }\n    </style>\n  {% elsif block.settings.shape_type == \"third\" %}\n    <style>\n      .shapes-item-{{ block.id }} {\n        aspect-ratio: 1/1.7;\n      }\n\n      @media(min-width: 1024px) {\n        .shapes-item-{{ block.id }} {\n          aspect-ratio: auto;\n        }\n      }\n    </style>\n  {% elsif block.settings.shape_type == \"first\" %}\n    <style>\n      .shapes-item-{{ block.id }} {\n        aspect-ratio: 1/1;\n      }\n\n      .shapes-item-{{ block.id }} .shapes-content-{{ section.id }} {\n        padding: 20px;\n      }\n\n      @media(min-width: 1024px) {\n        .shapes-item-{{ block.id }} {\n          aspect-ratio: auto;\n        }\n      }\n    </style>\n  {% endif %}\n{% endfor %}\n\n<div class=\"section-{{ section.id }} shapes-{{ section.id }}\" style=\"background-color:{{ background_color }}; background-image: {{ background_gradient }};\">\n    <div class=\"section-{{ section.id }}-settings\">\n      {% if heading != blank %}\n        <div class=\"shapes-heading-{{ section.id }}\">\n          {{ heading }}\n        </div>\n      {% endif %}\n      <div class=\"shapes-slider-{{ section.id }} swiper\">\n        <div class=\"swiper-wrapper\">\n          {% for block in section.blocks %}\n          <{% if block.settings.url != blank %}a href=\"{{ block.settings.url }}\"{% else %}div{% endif %